	SELECTCASE ARG:1
			;通常攻撃
			;弱攻撃
			CASE 10
			;中攻撃
			CASE 11
			;強攻撃
			CASE 12
			;溜め攻撃
			CASE 13
				;溜め
				IF CFLAG:31 == 2
				;発射
				ELSE
				ENDIF
			;ばら撒き弾
			CASE 14
			;貫通弾
			CASE 15
			ENDSELECT
RETURN

