@BATTLE_MIKATA_KOUDOU_HATUDOU_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	IF ARG:1 < 10
		CALL BATTLE_MIKATA_KOUDOU_SONOTA_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	ELSEIF ARG:1 < 16
		CALL BATTLE_MIKATA_KOUDOU_DANMAKU_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	ELSEIF ARG:1 < 25
		CALL BATTLE_MIKATA_KOUDOU_SYOUMOUBUKI_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	ELSEIF ARG:1 < 35
		CALL BATTLE_MIKATA_KOUDOU_NO_SYOUMOUBUKI_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	ELSEIF ARG:1 < 54
		CALL BATTLE_MIKATA_KOUDOU_KOUSOKU_NOW_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	ELSEIF ARG:1 < 87
		CALL BATTLE_MIKATA_KOUDOU_SAIMIN_26_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	ENDIF
RETURN

