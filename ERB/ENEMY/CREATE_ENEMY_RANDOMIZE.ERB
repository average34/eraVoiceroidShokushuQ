			ENDSELECT
		CASE 1
			SELECTCASE 接頭語強度:LCOUNT
				CASE 0
					DA:(FLAG:1):5 = MAX(DA:(FLAG:1):5 * 70 / 100, DA:(FLAG:1):5 - 30)
				CASE 1
