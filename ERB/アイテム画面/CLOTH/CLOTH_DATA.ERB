;=================================================
;服装データ
;「魔法少女」
;表示はされないけど一応セリフ関係全部用意
;=================================================
@CLOTH_DATA_102(処理ID, キャラID)
#DIM 処理ID
#DIM キャラID
;-MEMO-
;服装データの説明用
;関数名の"服装ID"は0～49で空いてる番号に置き換えます

	RESULT = 0
	RESULTS = %""%
	SELECTCASE 処理ID
		;---服の名前---
		CASE 0
			RESULTS = 魔法少女のドレス
			
		;---服の説明---
		CASE 1
			RESULTS = 少女達の夢の力で素質のある少女はパワーアップ
			
		;---選択可不可---
		CASE 2
			RESULT = -1
			
		;---服選択時の地の文---
		CASE 5
			PRINTFORMW キラキラとひらひらとふりふりがぎゅっと詰まっている
			PRINTFORMW なぜだか見ていると希望が沸いてくる
			IF TALENT:(キャラID):100 || TALENT:(キャラID):150
				PRINTFORMW 
			ELSE
				PRINTFORMW 
			ENDIF
			PRINTFORML 
			
		;---ステータスの服装表示---
		CASE 10	;CP 通常時
			RESULTS = 魔法少女の服
			
		CASE 11	;CP 損傷時
			RESULTS = ボロボロの魔法少女の服
			
		CASE 12	;CP 半裸時
			RESULTS = 布切れと化した魔法少女の服

		;---遭遇時の服装表現---
		CASE 20	;CP 通常時
			RESULTS = 魔法少女の姿をした
			
		CASE 21	;CP 損傷時
			RESULTS = ボロボロの魔法少女の姿をした
			
		CASE 22	;CP 半裸時
			RESULTS = 変身が今にも解けそうな魔法少女の
			
		;----被ダメージ補正-----
		CASE 1000
		;HPダメに対する補正(%)
			RESULT = -10
		CASE 1001
		;MPダメに対する補正(%)
			RESULT = -10
		CASE 1002
		;EPダメに対する補正(%)
			RESULT = 10
		CASE 1003
		;CPダメに対する補正(%)
			RESULT = -10
		;-----ステータス補正-----
		CASE 1100
		;戦闘力補正値(%)
			;小柄か妖精なら効果大
			IF TALENT:(キャラID):100 || TALENT:(キャラID):150
				RESULT = 30
			ELSE
				RESULT = 0
			ENDIF
		CASE 1103
		;速度補正値
			;小柄か妖精なら効果大
			IF TALENT:(キャラID):100 || TALENT:(キャラID):150
				RESULT = 30
			ELSE
				RESULT = 5
			ENDIF
			
	ENDSELECT
	
RETURN RESULT
