; ■ フラグ情報表示パッチ
; このパッチでは以下の変数を使用しています
; FLAG  550 フラグ情報表示 システムメッセージを表示する（0：表示しない　1：表示する）
; CFLAG 750 フラグ情報表示 システムメッセージを表示する（0：表示しない　1：表示する）

; ■ フラグ情報一覧を表示
; 口上を書く→仕様書を見る→ソースコードを見る　のループがよく発生するので防止に
@SHOW_PRINT_MESSAGE_INFO (ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM  TEMP_REDRAW

	; 初期値であれば何もせずに戻る
	SIF ARG:0 == 0
		RETURN
	
	CURRENTREDRAW
	TEMP_REDRAW = RESULT
	REDRAW 0
	SETCOLOR 0x555555
	DRAWLINEFORM =
	PRINTFORM PRINT_MESSAGEフラグ情報表示　対象：
	; キャラ口上の表示
	IF TARGET != -1
		SETCOLOR CFLAG:700
		PRINTFORM %CALLNAME:TARGET%　
		SETCOLOR 0x555555
		
		PRINTFORM 性格：
		
		IF CFLAG:12 <= -40
			PRINTFORM 超弱気
		ELSEIF CFLAG:12 <= -20
			PRINTFORM 弱気
		ELSEIF CFLAG:12 <= 19
			PRINTFORM 普通
		ELSEIF CFLAG:12 <= 39
			PRINTFORM 強気
		ELSE
			PRINTFORM 超強気
		ENDIF
		
		PRINTFORM ({CFLAG:12}), 
		
		SELECTCASE CFLAG:100
		CASE 1
			PRINTFORM おせっかい
		CASE 2
			PRINTFORM おてんば
		CASE 3
			PRINTFORM がんこもの
		CASE 4
			PRINTFORM がんばりや
		CASE 5
			PRINTFORM きれもの
		CASE 6
			PRINTFORM くろうにん
		CASE 7
			PRINTFORM しあわせもの
		CASE 8
			PRINTFORM セクシーギャル
		CASE 9
			PRINTFORM ぬけめがない
		CASE 10
			PRINTFORM ねっけつかん
		CASE 11
			PRINTFORM のんきもの
		CASE 12
			PRINTFORM ひっこみじあん
		CASE 13
			PRINTFORM やさしいひと
		CASE 14
			PRINTFORM わがまま
		CASEELSE
			PRINTFORM ふつう
		ENDSELECT
		
		PRINTFORML ({CFLAG:100})
		
		
	; システムメッセージのみの表示
	ELSE
		PRINTFORML システムメッセージ
	ENDIF
	PRINTFORML ARG:0={ARG:0}　ARG:1={ARG:1}　ARGS:0=%ARGS:0%　ARGS:1=%ARGS:1%　ARG:2={ARG:2}　ARG:3={ARG:3}　ARG:4={ARG:4}　ARG:5={ARG:5}　ARG:6={ARG:6}
	DRAWLINEFORM -
	
	; 状況ID分岐
	SELECTCASE ARG:0
	CASE 1
		PRINTFORML ARG:0={ARG:0, 5}　味方の攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_ADD_SKILLNAME(ARG:1)%%SKILLNAME(ARG:1, TARGET)%)
		; 行動内容によって引数が変化するので分岐
		SELECTCASE ARG:1
		; 防御
		CASE 1
			PRINTFORML ARG:2={ARG:2, 5}　0：通常の防御　1：装備品の残弾リロード
			IF ARG:2
				PRINTFORML ARG:3={ARG:3, 5}　残弾リロードしたアイテムのアイテムID (ID.{ARG:3}=%ITEMNAME:(ARG:3)%)
			ELSE
				PRINTFORML ARG:3={ARG:3, 5}　残弾リロードしたアイテムのアイテムID (通常の防御時は0)
			ENDIF
			PRINTFORML 補足       　残弾リロードが発生する条件は以下の通り
			PRINTFORML            　装備した消耗武器の残弾が0、別途アイテムで弾を所持、道具戦闘LV2以上、非拘束時に防御
			PRINTFORML            　装備品1・装備品2の残弾が共に0で、道具戦闘LV3以上の場合は同時にリロードを行う
		; アイテム使用
		CASE 2
			PRINTFORML ARG:2={ARG:2, 5}　アイテムID (ID.{ARG:2}=%ITEMNAME:(ARG:2)%)
			PRINTFORML ARG:3={ARG:3, 5}　使用対象キャラのID (ID.{ARG:3}=%CALLNAME:(ARG:3)%)
		; 逃走
		CASE 5
			PRINTFORML ARG:2={ARG:2, 5}　逃走成功フラグ　0：失敗　1：成功
		; 強攻撃
		CASE 12
			PRINTFORML 補足       　コマンドID.19 霊撃からも強攻撃口上が呼び出されます　判別にはCFLAG:50を参照
		; 溜め攻撃
		CASE 13
			PRINTFORML ARG:2={ARG:2, 5}　0：溜め　1：撃ち
		; 拘束からの離脱
		CASE 42
			PRINTFORML ARG:2={ARG:2, 5}　丸呑みフラグ　0：拘束　1：丸呑み
			PRINTFORML ARG:3={ARG:3, 5}　状況　0：脱出開始　1：絶頂による失敗　2：脱出成功　3：少しだけ脱出　4：脱出失敗
			PRINTFORML ARG:4={ARG:4, 5}　拘束している敵のID (ID.{ARG:4}=%SAVESTR:(ARG:4)%)
			PRINTFORML 補足       　戦闘行動による自力脱出
		; 力溜め
		CASE 43
			PRINTFORML ARG:2={ARG:2, 5}　状況　0：通常　1：絶頂で脱力
		; 諦め
		CASE 44
			PRINTFORML ARG:2={ARG:2, 5}　状況　0：諦めた瞬間　1：諦め済み
			PRINTFORML ARG:3={ARG:3, 5}　敵の種類 (%ENEMY_NAME(ARG:3)%)
		; 装備アイテムを使う (61は呼び出されないハズだけれど一応)
		CASE 60, 61
			PRINTFORML ARG:2={ARG:2, 5}　アイテムID (ID.{ARG:2}=%ITEMNAME:(ARG:2)%)
			PRINTFORML ARG:3={ARG:3, 5}　状況　1：アイテムを使用　2：アイテム効果発動済
		; 仲間を襲う
		CASE 73
			PRINTFORML ARG:2={ARG:2, 5}　襲われるキャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
			PRINTFORML ARG:3={ARG:3, 5}　状況　0：通常時　2：ふたなり時
		; 抱きつかれた
		CASE 95
			PRINTFORML ARG:2={ARG:2, 5}　抱きついたキャラのID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
			PRINTFORML ARG:3={ARG:3, 5}　状況　0：抱きつかれたまま　1：抜け出した　2：解放された
		; 抱きついた
		CASE 96
			PRINTFORML ARG:2={ARG:2, 5}　抱きつかれたキャラのID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
			PRINTFORML ARG:3={ARG:3, 5}　状況　0：抱きついたまま　1：抱きついたまま(ふたなり)
			PRINTFORML                  　2：抱きついた　3：抱きついた(ふたなり)
		ENDSELECT
	CASE 2
		PRINTFORML ARG:0={ARG:0, 5}　敵の通常攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		SELECTCASE ARG:1
		CASE 126, 137, 138, 139, 140, 145
			PRINTFORML ※「テキスト表現の残酷さ」設定によって内容が変化
		ENDSELECT
	CASE 3
		PRINTFORML ARG:0={ARG:0, 5}　敵の衣服攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		PRINTFORML ARG:3={ARG:3, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
		SELECTCASE ARG:1
		CASE 237, 238
			PRINTFORML ※「テキスト表現の残酷さ」設定によって内容が変化
		ENDSELECT
	CASE 4
		PRINTFORML ARG:0={ARG:0, 5}　敵の性攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		PRINTFORML ARG:3={ARG:3, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
	CASE 5
		PRINTFORML ARG:0={ARG:0, 5}　敵の拘束攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		PRINTFORML ARG:3={ARG:3, 5}　0：拘束　1：丸呑み
	CASE 6
		PRINTFORML ARG:0={ARG:0, 5}　敵のその他寄生攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		PRINTFORML ARG:3={ARG:3, 5}　0：通常の寄生攻撃　1～：妊娠攻撃(射精量)
		PRINTFORML ARG:4={ARG:4, 5}　メイン性属性 (0：A　1：B　2：C　3：V)
		PRINTFORML 補足       　特殊寄生・特殊妊娠の場合、産まれる敵は寄生主とは別の種族になる
		PRINTFORML            　※クイーンによる寄生→リビングバインを出産　等
	CASE 7
		PRINTFORML ARG:0={ARG:0, 5}　敵のその他攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
	CASE 8
		PRINTFORML ARG:0={ARG:0, 5}　敵の触手責め攻撃 (発動時)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		PRINTFORML ARG:3={ARG:3, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
	CASE 11
		PRINTFORML ARG:0={ARG:0, 5}　味方の攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_ADD_SKILLNAME(ARG:1)%%SKILLNAME(ARG:1, TARGET)%)
		PRINTFORML ARG:2={ARG:2, 5}　与ダメージ
	CASE 12
		PRINTFORML ARG:0={ARG:0, 5}　敵の通常攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　HPダメージ
		PRINTFORML ARG:3={ARG:3, 5}　CPダメージ
		PRINTFORML ARG:4={ARG:4, 5}　命中メッセージによる特殊処理 (TFLAG:98)
		SELECTCASE ARG:1
		CASE 126, 137, 138, 139, 140, 145
			PRINTFORML ※「テキスト表現の残酷さ」設定によって内容が変化
		ENDSELECT
	CASE 13
		PRINTFORML ARG:0={ARG:0, 5}　敵の衣服攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
		PRINTFORML ARG:3={ARG:3, 5}　CPダメージ
		PRINTFORML ARG:4={ARG:4, 5}　EPダメージ
		PRINTFORML ARG:5={ARG:5, 5}　命中メッセージによる特殊処理 (TFLAG:98)
		SELECTCASE ARG:1
		CASE 237, 238
			PRINTFORML ※「テキスト表現の残酷さ」設定によって内容が変化
		ENDSELECT
	CASE 14
		PRINTFORML ARG:0={ARG:0, 5}　敵の性攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
		PRINTFORML ARG:3={ARG:3, 5}　EPダメージ
		PRINTFORML ARG:4={ARG:4, 5}　命中メッセージによる特殊処理 (TFLAG:98)
		SELECTCASE ARG:1
		CASE 685, 692
			PRINTFORML ※「テキスト表現の残酷さ」設定によって内容が変化
		ENDSELECT
	CASE 15
		PRINTFORML ARG:0={ARG:0, 5}　敵の拘束攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　0：拘束　1：丸呑み
		PRINTFORML ARG:3={ARG:3, 5}　命中メッセージによる特殊処理 (TFLAG:98)
	CASE 16
		PRINTFORML ARG:0={ARG:0, 5}　敵の寄生攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　0：通常の寄生攻撃　1～：妊娠攻撃(射精量)
		PRINTFORML ARG:3={ARG:3, 5}　MPダメージ
		PRINTFORML ARG:4={ARG:4, 5}　命中メッセージによる特殊処理 (TFLAG:98)
		PRINTFORML 補足       　特殊寄生・特殊妊娠の場合、産まれる敵は寄生主とは別の種族になる
		PRINTFORML            　※クイーンによる寄生→リビングバインを出産　等
	CASE 17
		PRINTFORML ARG:0={ARG:0, 5}　敵のその他攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		SELECTCASE ARG:1
		CASE 24
			PRINTFORML ARG:3={ARG:3, 5}　状況　0：通常　1：PTの誰かが拘束中
		ENDSELECT
	CASE 18
		PRINTFORML ARG:0={ARG:0, 5}　敵の触手責め攻撃 (命中)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
		PRINTFORML ARG:3={ARG:3, 5}　MPダメージ
		PRINTFORML ARG:4={ARG:4, 5}　命中メッセージによる特殊処理 (TFLAG:98)
	CASE 21
		PRINTFORML ARG:0={ARG:0, 5}　味方の攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_ADD_SKILLNAME(ARG:1)%%SKILLNAME(ARG:1, TARGET)%)
	CASE 22
		PRINTFORML ARG:0={ARG:0, 5}　敵の通常攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
	CASE 23
		PRINTFORML ARG:0={ARG:0, 5}　敵の衣服攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
	CASE 24
		PRINTFORML ARG:0={ARG:0, 5}　敵の性攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
	CASE 25
		PRINTFORML ARG:0={ARG:0, 5}　敵の拘束攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
	CASE 26
		PRINTFORML ARG:0={ARG:0, 5}　敵の寄生攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
	CASE 27
		PRINTFORML ARG:0={ARG:0, 5}　敵のその他攻撃 (回避)
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		SELECTCASE ARG:1
		CASE 10 TO 17
			PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		CASE 950 TO 953
			PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		CASE 1300 TO 1306
			PRINTFORML ARG:2={ARG:2, 5}　対象キャラID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		ENDSELECT
	CASE 31
		PRINTFORML ARG:0={ARG:0, 5}　敵を撃破
		PRINTFORML ARG:1={ARG:1, 5}　敵ID (ID.{ARG:1}=%SAVESTR:(ARG:1)%)
	CASE 32
		PRINTFORML ARG:0={ARG:0, 5}　HPが0になった
	CASE 33
		PRINTFORML ARG:0={ARG:0, 5}　MPが0になった
	CASE 34
		PRINTFORML ARG:0={ARG:0, 5}　EPが0になった (絶頂の瞬間より前に呼び出される 絶頂の瞬間はARG:0=120)
		PRINTFORML ARG:1={ARG:1, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
		PRINTFORML ARG:2={ARG:2, 5}　状況分岐に使用予定 (現状では分岐が無いので常に0)
	CASE 35
		PRINTFORML ARG:0={ARG:0, 5}　CPが減少した
		PRINTFORML ARG:1={ARG:1, 5}　破れ強度　0：全裸になった　1：半裸になった　2：破れ始めた
	CASE 36
		PRINTFORML ARG:0={ARG:0, 5}　アイテムを使った・使って貰った
		PRINTFORML ARG:1={ARG:1, 5}　アイテムID (ID.{ARG:1}=%ITEMNAME:(ARG:1)%)
		IF ARG:2 != 0
			PRINTFORML ARG:2={ARG:2, 5}　アイテムを使用したキャラのID (ID.{ARG:2}=%CALLNAME:(ARG:2)%)
		ELSE
			PRINTFORML ARG:2={ARG:2, 5}　アイテムを使用したキャラのID (ID==0の場合、アイテムを自分で使用)
		ENDIF
	CASE 37
		PRINTFORML ARG:0={ARG:0, 5}　丸呑みされていて行動が届かない (呑まれている側)
		PRINTFORML ARG:1={ARG:1, 5}　敵ID (ID.{ARG:1}=%SAVESTR:(ARG:1)%)
	CASE 38
		PRINTFORML ARG:0={ARG:0, 5}　丸呑みされていて行動が届かない (行動した側)
		PRINTFORML ARG:1={ARG:1, 5}　敵ID (ID.{ARG:1}=%SAVESTR:(ARG:1)%)
	CASE 39
		PRINTFORML ARG:0={ARG:0, 5}　通常攻撃を受けてひるんだ
	CASE 40
		PRINTFORML ARG:0={ARG:0, 5}　寄生された
		PRINTFORML ARG:1={ARG:1, 5}　敵ID (ID.{ARG:1}=%SAVESTR:(ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　命中メッセージによる特殊処理 (TFLAG:98)
	CASE 41
		PRINTFORML ARG:0={ARG:0, 5}　攻撃されて起きた
		PRINTFORML ARG:1={ARG:1, 5}　コマンドID (ID.{ARG:1}=%SHOW_SYSTEM_MESSAGE_INFO_GET_ENEMY_ATTACK_COMMAND (ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　攻撃の種類　0：味方の攻撃　1：通常　2：衣服　3：性　4：拘束　5：寄生　6：その他
		PRINTFORML                        　9：霊撃　10：フェアリーイーター丸呑み
		PRINTFORML ARG:3={ARG:3, 5}　部位　0：A　1：B　2：C　3：V　5：肌　9：M
	CASE 42
		PRINTFORML ARG:0={ARG:0, 5}　敵から解放された
		PRINTFORML ARG:1={ARG:1, 5}　敵ID (ID.{ARG:1}=%SAVESTR:(ARG:1)%)
		PRINTFORML ARG:2={ARG:2, 5}　解放の状況　1:触手による自然解放　2:味方の攻撃による解放　3:霊撃　4:自力脱出
