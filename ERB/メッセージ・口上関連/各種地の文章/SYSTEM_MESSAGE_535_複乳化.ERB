;複乳化
@SYSTEM_MESSAGE_535(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

;素質チェック用
IF ARG:1 == 0
	PRINTFORML %ARGS:0%は胸の近くの肌に違和感があるようだ……
ELSEIF ARG:1 == 1
	PRINTFORML %ARGS:0%の胸の近くの肌が膨らんで、新たな乳頭ができている……
ELSEIF ARG:1 == 2
	PRINTFORML %ARGS:0%の胸の近くの肌が更に膨らみ、天辺の乳頭が発達してきている……
ELSEIF ARG:1 == 3
	PRINTFORML %ARGS:0%の胸の近くの肌が更に膨らみ、新たな乳房が形成されてしまった……　
ELSEIF ARG:1 == 4
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%は【複乳】になってしまった
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%は【複乳】になってしまった
ENDIF

ELSEIF ARG:1 == 5
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%は【複乳 Lv2】になってしまった
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%は【複乳 Lv2】になってしまった
ENDIF

ELSEIF ARG:1 == 6
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%は【複乳 Lv3】になってしまった
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%は【複乳 Lv3】になってしまった
ENDIF

ELSEIF ARG:1 == 7
;敵の複乳化攻撃による反応
ELSEIF ARG:1 == 11
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%の胸に何かを仕込まれたようだ……
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%の胸に何かを仕込まれたようだ……
ENDIF

ENDIF
