@SYSTEM_MESSAGE_460(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

IF ARG:1 == 0
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:1%は%ARGS:0%に二度と自分たちを襲わないように言い聞かせた！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:1%は%ARGS:0%に二度と自分たちを襲わないように言い聞かせた！
ENDIF

ELSEIF ARG:1 == -1
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:1%の肉棒が獲物を逃さないと言うかのように脈動し始めた！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:1%の肉棒が獲物を逃さないと言うかのように脈動し始めた！
ENDIF

IF FLAG:AUTOMODE
    PRINTFORML どうやら逃げられないようだ……
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  どうやら逃げられないようだ……
ENDIF

ELSEIF ARG:1 == 1
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:1%は動けない%ARGS:0%の膣内へ擬似陰茎を挿入した！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:1%は動けない%ARGS:0%の膣内へ擬似陰茎を挿入した！
ENDIF

ELSEIF ARG:1 == 2
	IF FLAG:521 == 0
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:1%は%ARGS:0%の豊満な胸でパイズリし始めた！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:1%は%ARGS:0%の豊満な胸でパイズリし始めた！
ENDIF

	ELSEIF FLAG:521 == 1
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:1%は擬似陰茎を%ARGS:0%の胸に擦りつけている！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:1%は擬似陰茎を%ARGS:0%の胸に擦りつけている！
ENDIF

	ENDIF
ELSEIF ARG:1 == 3
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%は%ARGS:1%の擬似陰茎を尻穴で飲み込まされた！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%は%ARGS:1%の擬似陰茎を尻穴で飲み込まされた！
