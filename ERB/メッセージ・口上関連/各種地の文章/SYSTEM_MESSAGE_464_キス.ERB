;キス　軽い冗談から徐々に濃厚に
@SYSTEM_MESSAGE_464(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

IF ARG:1 == 0
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%は%ARGS:1%と軽い冗談を交わした。
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%は%ARGS:1%と軽い冗談を交わした。
	ENDIF

ELSEIF ARG:1 == 2
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:1%は%ARGS:0%の頬に軽いキスをした！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:1%は%ARGS:0%の頬に軽いキスをした！
	ENDIF

ELSEIF ARG:1 == 4
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%は%ARGS:1%と唇を重ね合わせた！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%は%ARGS:1%と唇を重ね合わせた！
	ENDIF

ELSEIF ARG:1 == 6
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%は%ARGS:1%と何度か唇を重ね合わせた！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%は%ARGS:1%と何度か唇を重ね合わせた！
	ENDIF

ELSEIF ARG:1 == 8
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%は衝動のままに%ARGS:1%を抱きしめ、舌を絡めるようにキスをした！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%は衝動のままに%ARGS:1%を抱きしめ、舌を絡めるようにキスをした！
	ENDIF

ELSEIF ARG:1 == 10
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%と%ARGS:1%は抱き合いながら、しばらくの間キスをし続けた！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%と%ARGS:1%は抱き合いながら、しばらくの間キスをし続けた！
	ENDIF

ELSEIF ARG:1 == 12
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%と%ARGS:1%は胸を揉みあいながらキスの快楽に溺れている！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%と%ARGS:1%は胸を揉みあいながらキスの快楽に溺れている！
	ENDIF

ELSEIF ARG:1 == 14
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%と%ARGS:1%はお互いの口内を貪りながら股間を擦り付け始めた！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%と%ARGS:1%はお互いの口内を貪りながら股間を擦り付け始めた！
	ENDIF

ENDIF
