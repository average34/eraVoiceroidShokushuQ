
@MOB_NAME(ARG:0)
;ARG:0	モブの種類
#FUNCTIONS

SELECTCASE ARG:0
	CASE (FLAG:2 + 1)
		RETURNF "メイド妖精"
	CASE (FLAG:2 + 2)
		RETURNF "地上の兎"
	CASE (FLAG:2 + 3)
		RETURNF "月兎"
	CASE (FLAG:2 + 4)
		RETURNF "河童"
	CASE (FLAG:2 + 5)
		RETURNF "烏天狗"
	CASE (FLAG:2 + 6)
		RETURNF "白狼天狗"
	CASE (FLAG:2 + 7)
		RETURNF "ゾンビフェアリー"
	CASE (FLAG:2 + 8)
		RETURNF "妖狐"
	CASE (FLAG:2 + 9)
		RETURNF "化け猫"
	CASE (FLAG:2 + 10)
		RETURNF "化け狸"
	CASE (FLAG:2 + 11)
		RETURNF "妖獣"
	CASE (FLAG:2 + 12)
		RETURNF "村娘"
	CASEELSE
		RETURNF "存在しない種族"
ENDSELECT


