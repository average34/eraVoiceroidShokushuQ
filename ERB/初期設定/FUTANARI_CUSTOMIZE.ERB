;ふたなり細分化
@FUTANARI_CUSTOMIZE

PRINTFORML 
PRINTFORML 全キャラのふたなり素質はどうしますか？
PRINTFORML [1] 全部自分の想像に任せる
PRINTFORML [2] 全員同じで細かく決める
PRINTFORML [3] どんな陰茎でも問題ない（素質ランダム付与）
INPUT
IF RESULT == 1
	FLAG:590 = 1
	RETURN
ELSEIF RESULT == 2
	LOCAL:1 = 0

	PRINTL 
	PRINTFORML 竿の長さを設定してください
	PRINTFORML [0] 短い
	PRINTFORML [1] ふつう
	PRINTFORML [2] 長め
	PRINTFORML [3] 長い
	PRINTFORML [4] 超長い
	$INPUT_LOOP_00
	INPUT
	IF RESULT >= 0 && RESULT <= 4
		FLAG:570 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_00
	ENDIF
	
	PRINTL 竿の太さを設定してください
	PRINTL [0] そのまま
	PRINTL [1] 極太
	$INPUT_LOOP_1
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:571 = RESULT
	ELSE
		GOTO INPUT_LOOP_1
	ENDIF

	PRINTFORML 精力を設定してください
	PRINTFORML [0] 薄弱
	PRINTFORML [1] ふつう
	PRINTFORML [2] 絶倫
	PRINTFORML [3] 超絶倫
	$INPUT_LOOP_2
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:572 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_2
	ENDIF

	PRINTL 皮を設定してください
	PRINTFORML [0] ふつう
	PRINTFORML [1] 剥けかけ
	PRINTFORML [2] 仮性包茎
	PRINTFORML [3] 真性包茎
	$INPUT_LOOP_3
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:573 = RESULT
	ELSE
		GOTO INPUT_LOOP_3
	ENDIF

	PRINTL 垢を設定してください
	PRINTFORML [0] 清潔
	PRINTFORML [1] 恥垢体質
	$INPUT_LOOP_4
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:574 = RESULT
	ELSE
		GOTO INPUT_LOOP_4
	ENDIF

	PRINTL 臭いを設定してください
	PRINTFORML [0] ふつう
	PRINTFORML [1] 悪臭
	$INPUT_LOOP_5
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:575 = RESULT
	ELSE
		GOTO INPUT_LOOP_5
	ENDIF

	;先天性後天性
	PRINTL 先天性を設定してください
	PRINTFORML [0] 設定しない
	PRINTFORML [1] 全員生まれつき
	PRINTFORML [2] 全員後天的
	$INPUT_LOOP_6
	INPUT
	IF RESULT >= 0 && RESULT <= 2
		FLAG:576 = RESULT
	ELSE
		GOTO INPUT_LOOP_6
	ENDIF

	PRINTL 色を設定してください
	PRINTFORML [0] ふつう
	PRINTFORML [1] 黒光
	$INPUT_LOOP_7
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:577 = RESULT
	ELSE
		GOTO INPUT_LOOP_7
	ENDIF

	;次はTALENT:180からなので飛ばし

	PRINTL 玉の有無を設定してください
	PRINTFORML [0] 玉なし
	PRINTFORML [1] 玉あり
	PRINTFORML [2] 巨玉
	PRINTFORML [3] 爆玉
	PRINTFORML [4] 超巨玉
	$INPUT_LOOP_10
	INPUT
	IF RESULT >= 0 && RESULT <= 4
		FLAG:580 = RESULT
		IF RESULT > 0 && RESULT <= 4
			PRINTL 玉毛を設定してください
			PRINTFORML [0] パイパン
			PRINTFORML [1] 産毛
			PRINTFORML [2] 剛毛
			$INPUT_LOOP_11
			INPUT
			IF RESULT >= 0 && RESULT <= 2
				FLAG:581 = RESULT
			ELSE
				GOTO INPUT_LOOP_11
			ENDIF
		ENDIF
	ELSE
		GOTO INPUT_LOOP_10
	ENDIF
	

	

	;次はTALENT:185からなので飛ばし

	PRINTL 精液の量を設定してください
	PRINTFORML [0] 少量
	PRINTFORML [1] ふつう
	PRINTFORML [2] 大量
	PRINTFORML [3] 超大量
	$INPUT_LOOP_15
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:585 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_15
	ENDIF

	PRINTL 精液の濃度を設定してください
	PRINTFORML [0] 薄弱
	PRINTFORML [1] ふつう
	PRINTFORML [2] 特濃
	PRINTFORML [3] 超特濃
	$INPUT_LOOP_16
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:586 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_16
	ENDIF

	PRINTL 精液の熱さを設定してください
	PRINTFORML [0] ふつう
	PRINTFORML [1] 熱い
	$INPUT_LOOP_17
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:587 = RESULT
	ELSE
		GOTO INPUT_LOOP_17
	ENDIF

	PRINTL 精液の黄ばみを設定してください
	PRINTFORML [0] 白
	PRINTFORML [1] 黄ばみあり
	$INPUT_LOOP_18
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:588 = RESULT
	ELSE
		GOTO INPUT_LOOP_18
	ENDIF

	;ランダム発生をOFFにする
	FLAG:590 = 0

	;別のところからも飛ばしたいので分ける
	CALL FUTANARI_CUSTOMIZE_GET

ELSEIF RESULT == 3
	CALL FUTANARI_RANDOM_GET
	FLAG:590 = 2
ENDIF

;全キャラ同時入手用
@FUTANARI_CUSTOMIZE_GET

FOR LOCAL:0, 1, CHARANUM
	FOR LOCAL:1,0,20
		TALENT:(LOCAL:0):(170 + LOCAL:1) = FLAG:(570 + LOCAL:1)
		CFLAG:(LOCAL:0):515 = 1
	NEXT
NEXT

;各キャラ個別入手用
@FUTANARI_CUSTOMIZE_GET_2(ARG:0)
SIF CFLAG:(ARG:0):515 == 1
	RETURN
FOR LOCAL:1, 1, 20
	TALENT:(ARG:0):(170 + LOCAL:1) = FLAG:(570 + LOCAL:1)
NEXT
CFLAG:(ARG:0):515 = 1

;全キャラ同時ランダム素質入手用
@FUTANARI_RANDOM_GET(ARG:0)

FOR LOCAL:0, 1, CHARANUM
	;数値範囲がバラバラなので1個ずつ
	TALENT:(LOCAL:0):170 = RAND:5 - 1
	TALENT:(LOCAL:0):171 = RAND:2
	TALENT:(LOCAL:0):172 = RAND:4 - 1
	TALENT:(LOCAL:0):173 = RAND:4
	TALENT:(LOCAL:0):174 = RAND:2
	TALENT:(LOCAL:0):175 = RAND:2
	;先天的か後天的かだけふたなり素質が必要
	IF TALENT:(LOCAL:0):121
		TALENT:(LOCAL:0):176 = RAND:2
	ENDIF
	TALENT:(LOCAL:0):177 = RAND:2
	IF ARG:0 == 3
		TALENT:(LOCAL:0):180 = RAND:5
		IF TALENT:(LOCAL:0):180
			TALENT:(LOCAL:0):181 = RAND:3
		ENDIF
	ENDIF
	TALENT:(LOCAL:0):185 = RAND:4 - 1
	TALENT:(LOCAL:0):186 = RAND:4 - 1
	TALENT:(LOCAL:0):187 = RAND:2
	TALENT:(LOCAL:0):188 = RAND:2

	CFLAG:(LOCAL:0):515 = 1
NEXT

;各キャラ個別ランダム素質入手用
@FUTANARI_RANDOM_GET_2(ARG:0)
SIF CFLAG:(ARG:0):515 == 1
	RETURN

;数値範囲がバラバラなので1個ずつ
TALENT:(ARG:0):170 = RAND:5 - 1
TALENT:(ARG:0):171 = RAND:2
TALENT:(ARG:0):172 = RAND:4 - 1
TALENT:(ARG:0):173 = RAND:4
TALENT:(ARG:0):174 = RAND:2
TALENT:(ARG:0):175 = RAND:2
;先天的か後天的かだけふたなり素質が必要
IF GET_PENIS(ARG:0) > 1
	TALENT:(ARG:0):176 = RAND:2
ENDIF
TALENT:(ARG:0):177 = RAND:2
IF ARG:0 == 3
	TALENT:(ARG:0):180 = RAND:5
	IF TALENT:(ARG:0):180
		TALENT:(ARG:0):181 = RAND:3
	ENDIF
ENDIF
TALENT:(ARG:0):185 = RAND:4 - 1
TALENT:(ARG:0):186 = RAND:4 - 1
TALENT:(ARG:0):187 = RAND:2
TALENT:(ARG:0):188 = RAND:2

CFLAG:(ARG:0):515 = 1

