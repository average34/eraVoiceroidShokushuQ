;ARG:1	能力ID
;＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝＝
@CHECK_STATUS_TAB_SUB1(ARG:0, ARG:1)
#DIM LCOUNT,3	;ループ用
#DIM 現在値
#DIM 必要値
	
	
	
	
	;-----レベル表示-----
	PRINTFORM %ABLNAME:(ARG:1),8,LEFT% Lv{ABL:(ARG:0):(ARG:1)} EXP:
	
	
	
	
	;-----バー表示-----
	;現在の量
	現在値 = EXP:(ARG:0):(ARG:1)
	
	;次レベルに必要な量
	;※レベルMAXだと-1を返す
	CALL GET_NEED_EXP(ARG:1, ABL:(ARG:0):(ARG:1) + 1)
	必要値 = RESULT
	
	IF 必要値 <= 0
		CALL PRINT_COLORBAR( 1, 1, 15, BARCOLORSET("白"), RESULT:1)
	ELSE
		CALL PRINT_COLORBAR( 現在値, 必要値, 15, BARCOLORSET("白"), RESULT:1)
	ENDIF
	
	;-----数値表示-----
	;---現在値---
	;表示桁調整
	LOCAL = 0
	IF 現在値 > 999999
		FOR LCOUNT:1, 0, 5
			IF 現在値 > 999999
				現在値 /= 1000
				LOCAL += 1
			ENDIF
		NEXT
	ENDIF
	LOCALS:1 = {現在値}
	SELECTCASE LOCAL
		CASE 0
			
		CASE 1
			LOCALS:1 = %LOCALS:1%k
		CASE 2
			LOCALS:1 = %LOCALS:1%M
		CASE 3
			LOCALS:1 = %LOCALS:1%G
		CASE 4
			LOCALS:1 = %LOCALS:1%T
		CASE 5
			LOCALS:1 = %LOCALS:1%P
		CASE 6
			LOCALS:1 = %LOCALS:1%E
	ENDSELECT
	
	;---目標値---
	IF 必要値 <= 0
		LOCALS:2 = -----
	ELSE
		LOCALS:2 = {必要値}
	ENDIF
	
	;---表示---
	PRINTFORM (%LOCALS:1,7,RIGHT%/%LOCALS:2,5,RIGHT%)
	
	
	
	
