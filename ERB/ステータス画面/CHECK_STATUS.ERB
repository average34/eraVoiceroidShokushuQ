CALL CHECK_STATUS(ARG)

;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡
;PT全体及び指定キャラの詳細能力確認
;ARG:0	0:PT全体表示
;		1～:キャラID指定(遭遇会話時の閲覧用)
;ARG:1	1:キャラ情報の編集の許可
;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡
@CHECK_STATUS(ARG = 0, ARG:1 = 0)
#DIM LCOUNT,3	;ループ用

#DIM PTID,3
;PTメンバID格納用
;ループ処理を容易にするために使用

#DIM MODE
;表示対象キャラ

#DIM TABN
;タブ位置
;0全般
;1素質
#DIM TABMAX
#DIM 入力
#DIM LINECOUNTBAK

#DIM LINEDEL
;行削除判定
#DIM LINEDELET
;削除する追加行数


#DIM NAMESTRSUM, 3

;タブの数を変える時はココ
IF FLAG:9
	TABMAX = 4
ELSE
	TABMAX = 3
ENDIF



IF ARG > 0
	PTID:0 = ARG
	PTID:1 = 0
	PTID:2 = 0
ELSE
	PTID:0 = 1
	PTID:1 = FLAG:10
	PTID:2 = FLAG:11
	ARG:1 = 1
ENDIF

MODE = 0
TABN = 0
LINEDEL = 0
LINEDELET = 0


;≡≡≡≡≡ループここから≡≡≡≡≡
DO

;＝＝＝描画処理ここから＝＝＝
REDRAW 2

;-----前表示消去-----
;基本的に47行消去、表示行数が多い場合それ以上
IF LINEDEL > 0
	IF LINEDEL == 1
		CLEARLINE 47 + LINEDELET
	ENDIF
	LINEDEL = 0
	LINEDELET = 0
ENDIF
DRAWLINEFORM =


	;-----キャラタブ（３行）---
	;ボタン割り当て1-9
	
	;前準備
	VARSET NAMESTRSUM
	FOR LCOUNT, 0, 3
		IF PTID:(LCOUNT) > 0
			;タブのラベル部分に表示する内容の長さを事前に算出（名前+括弧+ボタン）
			STRLENS CALLNAME:(PTID:(LCOUNT))
			NAMESTRSUM:(LCOUNT) = RESULT + 7
			;タブの表示には全角文字をつかうので文字数を偶数にしておく
			SIF NAMESTRSUM:(LCOUNT) % 2
				NAMESTRSUM:(LCOUNT)++
		ENDIF
	NEXT
	
	;タブ上部
	PRINTFORM 　
	PRINTFORM ┌
	FOR LCOUNT, 0, 3
		IF PTID:(LCOUNT) > 0
			PRINTFORM %"─" * (NAMESTRSUM:(LCOUNT) / 2)%
			IF LCOUNT < MODE
				PRINTFORM ┌
			ELSE
				PRINTFORM ┐
			ENDIF
		ENDIF
	NEXT
	PRINTL
	
	
	;タブ中部
	PRINTFORM 　
	PRINTFORM │
	FOR LCOUNT, 0, 3
		IF PTID:(LCOUNT) > 0
			SETCOLOR LCOUNT == MODE ? 0xFFFF70 # 0x00FF00
			PRINTBUTTON @"%@"[{LCOUNT+1}]【%CALLNAME:(PTID:(LCOUNT))%】", NAMESTRSUM:(LCOUNT), LEFT%", LCOUNT + 1
			RESETCOLOR
			PRINTFORM │
		ENDIF
	NEXT
	PRINTL
	
	
	;タブ下部
	PRINTFORM ─
	PRINTFORM \@MODE == 0?┘#┴\@
	FOR LCOUNT, 0, 3
		IF PTID:(LCOUNT) > 0
			PRINTFORM %@"\@LCOUNT == MODE?　#─\@" * (NAMESTRSUM:(LCOUNT) / 2)%
			IF LCOUNT == MODE
				PRINTFORM └
			ELSEIF LCOUNT + 1 == MODE
				PRINTFORM ┘
			ELSE
				PRINTFORM ┴
			ENDIF
		ELSE
			PRINT ─
		ENDIF
	NEXT
	LOCAL = 0
	FOR LCOUNT, 0, 3
		LOCAL += NAMESTRSUM:(LCOUNT)
	NEXT
	PRINTFORM %"─" * ((90 - LOCAL) / 2 - 1)%
	PRINTL



	;-----選択キャラステータス------
	
	;---メイン---
	CALL SHOW_SIMPLE_STATUS(PTID:(MODE))
	
	
	
	
	;---装備---
	DRAWLINEFORM ･
	CALL SHOW_EQUIP_LIST(PTID:(MODE))
	
	
	
	
	
	;---タブ（３行）---
	;ボタン割り当て10-19
	
	;タブ上部
	PRINTFORM 　
	PRINTFORM ┌
	FOR LCOUNT, 0, TABMAX
		PRINTFORM ──────
		IF LCOUNT < TABN
			PRINTFORM ┌
		ELSE
			PRINTFORM ┐
		ENDIF
	NEXT
	PRINTL
	
	
	;タブ中部
	PRINTFORM 　
	PRINTFORM │
	FOR LCOUNT, 0, TABMAX
		SETCOLOR LCOUNT == TABN ? 0xFFFF70 # 0x00FF00
		SELECTCASE LCOUNT
		CASE 0
			PRINTBUTTON "[10]全　般  ",10
		CASE 1
			PRINTBUTTON "[11]素　質  ",11
		CASE 2
			PRINTBUTTON "[12]刻　印  ",12
		CASEELSE
			PRINTBUTTON @"[{10+LCOUNT}]未定義  ",10 + LCOUNT
		ENDSELECT
		RESETCOLOR
		PRINTFORM │
	NEXT
	PRINTL
	
	
	;タブ下部
	PRINTFORM ─
	PRINTFORM \@TABN == 0?┘#┴\@
	FOR LCOUNT, 0, TABMAX
		PRINTFORM \@LCOUNT == TABN?　　　　　　#──────\@
		IF LCOUNT == TABN
			PRINTFORM └
		ELSEIF LCOUNT + 1 == TABN
			PRINTFORM ┘
		ELSE
			PRINTFORM ┴
		ENDIF
		
	NEXT
	
	FOR LCOUNT, 0, 6 - TABMAX
		PRINTFORM ───────
	NEXT
	PRINTL ─────
	
	
	;＝＝＝＝＝タブの内容表示ここから＝＝＝＝＝
	LINECOUNTBAK = LINECOUNT
	IF TABN == 0
		;全般
		;CALL CHECK_STATUS_TAB1(PTID:(MODE))
		CALL CHECK_STATUS_TAB3(PTID:(MODE))
	
	ELSEIF TABN == 1
	
		;素質
		CALL SHOW_TALENT_LIST(PTID:(MODE))
		
		
		
	ELSEIF TABN == 2
		;刻印
		CALL SHOW_MARK_LIST(PTID:(MODE))
	ENDIF
	
	;---余白自動調整---
	FOR LCOUNT, 0, 30 - (LINECOUNT - LINECOUNTBAK)
		PRINTL
	NEXT
	;---表示行数が多い場合記憶---
	IF 30 - (LINECOUNT - LINECOUNTBAK) < 0
		LINEDELET -= 30 - (LINECOUNT - LINECOUNTBAK)
	ENDIF
	
	
	;＝＝＝＝＝タブの内容表示ここまで＝＝＝＝＝
	
	;---その他のコマンド---
	PRINTFORM [ 0]戻る　
	IF ARG:1 == 1
		PRINTFORM [20]愛称を変更する　
		;変幻自在持ち
		IF TALENT:(PTID:(MODE)):139
			PRINTFORM [21]変幻自在で能力を変更する　
		ELSEIF TALENT:(PTID:(MODE)):166 && (DAY == 1 && TIME:0 == 0)
			PRINTFORM [22]モブの能力を変更する（最初のみ）　
		ENDIF
	ENDIF
	PRINTL
	DRAWLINE
	
	;---案内---
	REUSELASTLINE %CALLNAME:(PTID:(MODE))%さんのステータスです
	
	

REDRAW 1
;＝＝＝入力処理ここから＝＝＝
INPUT
入力 = RESULT




IF 入力 == 0
	BREAK
ELSEIF 入力 >= 1 && 入力 <= 3 && PTID:(入力 - 1) > 0
	MODE = 入力 - 1
	LINEDEL = 1
ELSEIF 入力 >= 10 && 入力 <= (9 + TABMAX)
	TABN = 入力 - 10
	LINEDEL = 1
ELSEIF 入力 == 20 && ARG:1 == 1
	;愛称変更
	CALL CHECK_STATUS_CHANGE_CALLNAME(PTID:(MODE))
	
ELSEIF 入力 == 21 && ARG:1 == 1 && TALENT:(PTID:(MODE)):139
	;能力変更
	CALL CHANGE_PERSONAL_STATUS(PTID:(MODE))
	
ELSEIF 入力 == 22 && ARG:1 == 1 && TALENT:(PTID:(MODE)):166 && (DAY == 1 && TIME:0 == 0)
	;能力変更
	CALL CHANGE_PERSONAL_STATUS_2(PTID:(MODE))
	
ELSE
	LINEDEL = 1
ENDIF





