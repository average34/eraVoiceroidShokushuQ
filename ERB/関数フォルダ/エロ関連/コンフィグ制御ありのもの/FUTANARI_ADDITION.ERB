;ふたなりの状態異常発生パッチの追加まとめ
;ARG:0	対象キャラID
;LOCAL
;	:0	EPダメージ
;	:1	MPダメージ
;	:2	HPダメージ
;	:3	CPダメージ
;	:4	TEMP1
;	:5	TEMP2
;	:6	処女フラグ
;	:7	最大追加回数
;	:8	合計MPダメージ
;	:9	LOOP

;それぞれのイベントが起こる条件
@FUTANARI_PERCENT(LOCAL:0)
#FUNCTION
SELECTCASE LOCAL:0
	CASE 0
		;NPCがふたなり化する確率
		LOCAL:1 = 1
	CASE 1
		;NPCが襲ってくる好感度
		LOCAL:1 = 200
	CASE 2
		;NPCが中田氏しようとする確率が上がる最低好感度
		LOCAL:1 = 400
	CASE 3
		;NPCが中田氏しようとする最大確率
		;100でも最低好感度から段階的に上がっていくので確実ではありません
		LOCAL:1 = 90
	CASE 4
		;同意モードの最低好感度(0の場合は同意モード無し)
		LOCAL:1 = 800
	CASE 5
		;処女喪失ダメージ倍率
		LOCAL:1 = 2
	CASE 6
		;処女喪失基本ダメージ
		LOCAL:1 = 5000
	CASE 7
		;妊娠する確率(さらに1/4は流産)
		LOCAL:1 = 5
	CASE 8
		;追加攻撃フラグ(1かつ発情していると発生)
		LOCAL:1 = 0
	CASE 9
		;ふたなりが伝染する確率
		LOCAL:1 = 5
	CASE 10
		;アイテム譲渡時に外田氏する最低確率
		LOCAL:1 = 10
	CASEELSE
		LOCAL:1 = 0
ENDSELECT
RETURNF LOCAL:1



;NPCをランダムでふたなり化
;既にふたなりの場合は何もしない
@FUTANARI_ATTACH(ARG:0)
IF RAND:100 < FUTANARI_PERCENT(0) && GET_PENIS(ARG:0) == 0
	CFLAG:(ARG:0):42 = 1 + RAND:4 + RAND:3 + RAND:2
	TRYCCALLFORM KOJO_{NO:(ARG:0)}_{CFLAG:(ARG:0):701}_FUTANARI(0,0,1,ARG:0,0)
	CATCH
	ENDCATCH
ENDIF
RETURN 0


;睡眠時に見張りのふたなり及び好感度チェック
;条件を満たしていれば……
@FUTANARI_H(ARG:0)
;素質エネミーがない、かつふたなりでないか襲う気にならなかった
IF TALENT:(ARG:0):157 == 0
	SIF GET_PENIS(ARG:0) == 0 || CFLAG:(ARG:0):5 < FUTANARI_PERCENT(1)
		RETURN 0
ENDIF

;エネミーは襲いたくなると精力増大
SIF TALENT:(ARG:0):157 == 1
	CFLAG:(ARG:0):42 += 10

;眠ったままのレイプを追加
IF TALENT:1:0 == 0 && RAND:5 == 0
	CALL FUTANARI_H_SLEEPRAPE(ARG:0)
	WAIT
	CALL ADD_ABL_EXP(13, 1,3 + RAND:5)
ELSEIF CFLAG:(ARG:0):5 >= FUTANARI_PERCENT(4) && FUTANARI_PERCENT(4) != 0
	CALL FUTANARI_H_SEX(ARG:0)
	WAIT
	CALL ADD_ABL_EXP(13, 1,3 + RAND:5)
ELSE
	CALL FUTANARI_H_RAPE(ARG:0)
	WAIT
	CALL ADD_ABL_EXP(13, 1,3 + RAND:5)
ENDIF




RETURN RESULT


