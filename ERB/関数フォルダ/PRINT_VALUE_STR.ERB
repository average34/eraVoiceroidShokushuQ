;眠気、汚染度、状態異常等の表示
;デバッグモードでの淫乱系素質や感度の表示

;各種数値を文字列として表示

;=================================================
;現在の眠気状態表示
;=================================================
@PRINT_SLEEPINESS

LOCAL:1 = FLAG:7

IF LOCAL:1 < 3
	SETCOLOR 0x60EEEE
	PRINTFORM [元気]
ELSEIF LOCAL:1 < 7
	SETCOLOR 0x80FF20
	PRINTFORM [睡眠可]
ELSEIF LOCAL:1 < 11
	SETCOLOR 0xEEEE10
	PRINTFORM [要睡眠]
ELSEIF LOCAL:1 < 15
	SETCOLOR 0xEE1010
	PRINTFORM [朦朧]
ELSE
	SETCOLOR 0xEE1010
	PRINTFORM [気絶]
ENDIF
RESETCOLOR


;=================================================
;現在の汚染度表示
;=================================================
@PRINT_POLLUTION
;バグ対策
SIF FLAG:(50 + FLAG:5) > 100
	FLAG:(50 + FLAG:5) = 100

IF FLAG:(50 + FLAG:5) < 10
	SETCOLOR 0x1060EE
	PRINTFORM [清浄]
ELSEIF FLAG:(50 + FLAG:5) < 40
	RESETCOLOR
	PRINTFORM [通常]
ELSEIF FLAG:(50 + FLAG:5) < 70
	SETCOLOR 0xEEEE10
	PRINTFORM [軽度汚染]
ELSEIF FLAG:(50 + FLAG:5) < 100
	SETCOLOR 0xEE10EE
	PRINTFORM [中度汚染]
ELSE
	SETCOLOR 0xEE1010
	PRINTFORM [重度汚染]
ENDIF
RESETCOLOR

;=================================================
;CPレベルを表示
;ARG:0	対象キャラ
;=================================================
@PRINT_CLOTH_LEVEL(ARG:0)
#DIM PERCENT	;値率

	PERCENT = BASE:(ARG:0):CP * 100 / MAXBASE:(ARG:0):CP
	IF BASE:(ARG:0):CP <= 0
		SETCOLOR 0xEE1010
		PRINTFORM [大破]
	ELSEIF BASE:(ARG:0):CP - (BASE:(ARG:0):5 * 4) <= 0
		SETCOLOR 0xEE1010
		PRINTFORM [濡透]
	ELSE
		IF PERCENT >= 100
			SETCOLOR 0x10EEEE
			PRINTFORM [新品]
		ELSEIF PERCENT > 66
			RESETCOLOR
			PRINTFORM [通常]
		ELSEIF PERCENT > 33
			SETCOLOR 0xEEEE10
			PRINTFORM [損傷]
		ELSE
			SETCOLOR 0xEE10EE
			PRINTFORM [中破]
		ENDIF
	ENDIF
	RESETCOLOR

;=================================================
;現在の状態異常を表示
;ARG:0	対象キャラ
;RESULT:0 使用文字数（全角は一文字2）
;=================================================
@PRINT_CONDITION(ARG:0)
#DIM LCOUNT,3  ;ループ用

;LOCAL:0 使用文字数カウント用
LOCAL = 0
FOR LCOUNT,0, 50
	RESULTS = %""%
	TRYCALLFORM PRINT_CONDITION_{LCOUNT}(ARG)
	IF RESULTS != ""
		IF RESULT > 0
			SETCOLOR RESULT
			PRINTFORM %RESULTS% 
		ELSE
			PRINTFORMD %RESULTS% 
		ENDIF
		STRLENS (RESULTS)
		LOCAL += RESULT+1
	ENDIF
NEXT

RESETCOLOR
RETURN LOCAL
