;=================================================
;現在の素質を表示(1文字)
;ARG:0	対象キャラ
;RESULT:0 使用文字数（全角は一文字2）
;=================================================
@PRINT_TALENT_S(ARG:0)
#DIM LCOUNT,3  ;ループ用

;LOCAL:0 使用文字数カウント用
LOCAL = 0
FOR LCOUNT,0, 50
	RESULTS = %""%
	TRYCALLFORM PRINT_TALENT_{LCOUNT}S(ARG)
	IF RESULTS != ""
		IF RESULT > 0
			SETCOLOR RESULT
			PRINTFORM %RESULTS%
		ELSE
			PRINTFORMD %RESULTS%
		ENDIF
		STRLENS (RESULTS)
		LOCAL += RESULT
	ENDIF
NEXT

RESETCOLOR
RETURN LOCAL

