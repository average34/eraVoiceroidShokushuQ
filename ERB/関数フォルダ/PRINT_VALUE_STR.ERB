;=======================
;寄生・妊娠・排卵・抱卵・乳卵
;2014/09/08　排卵日や危険日を口上側で判別しやすいようにCFLAG:159に記憶　排卵日=1　危険日=2　その他=0
;=======================
@PRINT_CONDITION_4(ARG:0)
;LOCAL:1	月経周期何日目か
LOCAL:1 = (DAY % 30) - CFLAG:(ARG:0):150
SIF LOCAL:1 < 0
	LOCAL:1 += 30

IF CFLAG:(ARG:0):22 && CFLAG:(ARG:0):36 == 0
	IF CFLAG:(ARG:0):23  >= 8
		RESULTS = 寄生
	ELSEIF CFLAG:(ARG:0):23 >= 4
		RESULTS = 寄生
		RETURN 0xEEEE10
	ELSEIF CFLAG:(ARG:0):23 >= 1
		RESULTS = 寄生
		RETURN 0xEE1010
	ENDIF
	;お腹に寄生体いる時はCFLAG:(ARG:0):159を念のためリセット
	CFLAG:(ARG:0):159 = 0
ELSEIF CFLAG:(ARG:0):36 >= 3
	IF CFLAG:(ARG:0):23 >= 5
		RESULTS = 妊娠
	ELSEIF CFLAG:(ARG:0):23 >= 3
		RESULTS = 妊娠
		RETURN 0xEEEE10
	ELSEIF CFLAG:(ARG:0):23 >= 1
		RESULTS = 妊娠
		RETURN 0xEE1010
	ENDIF
	;お腹に赤ちゃんいる時はCFLAG:(ARG:0):159を念のためリセット
	CFLAG:(ARG:0):159 = 0
ELSEIF LOCAL:1 >= 0
	IF FLAG:535 == 1 && TALENT:(ARG:0):137 == 1 && TALENT:(ARG:0):122 == 0
		IF LOCAL:1 >= 11 && LOCAL:1 <= 14
			RESULTS = 危険日
			RETURN 0xEE0066
			CFLAG:(ARG:0):159 = 2
		ELSEIF LOCAL:1 >= 15 && LOCAL:1 <= 17
			RESULTS = 排卵
			RETURN 0xEE60EE
			CFLAG:(ARG:0):159 = 1
		ELSEIF LOCAL:1 == 0
			RESULTS = 月経
			CFLAG:(ARG:0):159 = 3
		;「中に誰もいませんよ」で危険日でも排卵日でもないので安全日として記憶
		ELSE
			CFLAG:(ARG:0):159 = 0
		ENDIF
	ELSEIF FLAG:535 == 1 && TALENT:(ARG:0):137 == 2 && TALENT:(ARG:0):122 == 0
		IF LOCAL:1 >= 10 && LOCAL:1 <= 14
			RESULTS = 危険日
			RETURN 0xEE0066
			CFLAG:(ARG:0):159 = 2
		ELSEIF LOCAL:1 >= 15 && LOCAL:1 <= 18
			RESULTS = 排卵
			RETURN 0xEE60EE
			CFLAG:(ARG:0):159 = 1
		ELSEIF LOCAL:1 == 0
			RESULTS = 月経
			CFLAG:(ARG:0):159 = 3
		;「中に誰もいませんよ」で危険日でも排卵日でもないので安全日として記憶
		ELSE
			CFLAG:(ARG:0):159 = 0
		ENDIF
	ELSEIF FLAG:535 == 1 && TALENT:(ARG:0):137 == 3 && TALENT:(ARG:0):122 == 0
		RESULTS = 排卵
		RETURN 0xEE0066
		CFLAG:(ARG:0):159 = 1
	ELSEIF ((FLAG:535 == 1 && TALENT:(ARG:0):137 == 0) || FLAG:535 == 0) && TALENT:(ARG:0):122 == 0
		IF LOCAL:1 >= 12 && LOCAL:1 <= 14
			RESULTS = 危険日
			RETURN 0xEE0066
			CFLAG:(ARG:0):159 = 2
		ELSEIF LOCAL:1 >= 15 && LOCAL:1 <= 17
			RESULTS = 排卵
			RETURN 0xEE60EE
			CFLAG:(ARG:0):159 = 1
		ELSEIF LOCAL:1 == 0
			RESULTS = 月経
			CFLAG:(ARG:0):159 = 3
		;「中に誰もいませんよ」で危険日でも排卵日でもないので安全日として記憶
		ELSE
			CFLAG:(ARG:0):159 = 0
		ENDIF
	ENDIF
ENDIF
