
; ------------------------------------------------------------------------------
; ■ 120 絶頂の瞬間 - 前回情報参照 118 自慰
; ------------------------------------------------------------------------------

; □ 0 通常絶頂
; ◆ 汎用口上
@KOJO_16_0_SYSTEM_MESSAGE_120_PREV_118_X_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 基本的に部位別等の下位関数から呼び出される為、優先口上指定は行わない
	; 汎用口上と、それ以外両方で優先指定されていると片方が消えてしまう為
	
	; 追加地の文関数(前)を登録
	CALL KOJO_16_0_REGIST_DESCRIPTION_BEFORE("KOJO_16_0_SYSTEM_MESSAGE_120_DESCRIPTION_BEFORE")
	
	; 睡眠・諦め
	SIF CFLAG:25 || TFLAG:16 == 2
		RETURN 0
	
	; 戦意喪失中
	IF CFLAG:29
		SELECTCASE KOJO_16_0_KOJO_MODE
		; 淫乱, 発情
		CASE "淫乱", "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ぁ…♥　ぁ…♥　す…きぃ…♥　自慰…、すき…ですの…♥」\n"
		CASEELSE
		ENDSELECT
		RETURN 1
	ENDIF
	
	; 催眠中
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; 催眠深度によって変化
			SELECTCASE CFLAG:43
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"（あっ！？　ああぁっ！？　どうなっていますの…！？）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（くっ…　カラダ操られて…　無理矢理イカされてしまう…　なんて…っ）\n"
			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"「あはは♪　あたしは見られながらイクのが大好きな、淫乱女ですわっ♪」\n（！？　口っ！？　口が勝手にっ！？）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（見られてますわ…　あたしがイクとこ…　みんなに……）\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「%CALLNAME:TARGET%はぁ…っ…　オナニー…がッ、だいしゅきな変態雌狐になッ…れてぇ…　しあわ、せで…すの…」\n"
				CALL KOJO_16_0_PARAGRAPH_MESSAGE
				CALL KOJO_16_0_ADD_MESSAGE, @"\d上ずった声で絶頂を告げながら、%CALLNAME:TARGET%は快感を与える手を止めようとしない\d\n"
			ENDSELECT
			RETURN 1
		ENDSELECT
	ENDIF
	
	; 寸止め・誘発中
	IF CFLAG:820
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"（あたしのカラダ…っ！？　どうなっちゃってるますの…っ！？\n　この疼きを鎮めるつもり…でしたのにっ♥♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（あたし…っ　ガマンできなくてっ…　こんな場所でいたして…イっていますわ…っ♥♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ひッ♥　あッ♥　あッ♥♥　イクイクイクぅぅッ♥　こわれますのっ♥　もどれなくなっちゃうぅっ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「どうしてっ♥　どうしてこんなのがっ、こんなに気持ちいいんですの…っ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あぁぁぁっ♥　こんなイキかたっ♥　恥ずかしいですわっ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「おなにーとまらないっ♥　おなにーとまらないっ♥　おなにーとまらないんですのっッ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「だめっ♥　みないでっ♥　みないでくださいましっ♥♥\n　あああぁぁ♥　きもちいいっ♥　みられるのきもちいいっ♥♥　みてはだめぇぇ♥♥」\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			CALL KOJO_16_0_ADD_MESSAGE, @"\d想像以上の快感に襲われた%CALLNAME:TARGET%は、涙や涎で蕩けきったイキ顔を晒している…\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d興奮が止まらなくなった%CALLNAME:TARGET%は、絶頂したにも関わらず行為を続けている！\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d肉欲を抑えられなくなった%CALLNAME:TARGET%は、嬉しそうに絶頂を繰り返している…\d\n"
			RETURN 1
		ENDSELECT
	ENDIF
	
	SELECTCASE KOJO_16_0_KOJO_MODE
	CASE "淫乱"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あは…ぁ♥　きもち、よかっ…たですわ♥」\n"
		
		; 部位別 追加口上
		SELECTCASE ARG:1
		CASE 2
			; ふたなり時
			IF GET_PENIS(TARGET)
				CALL KOJO_16_0_ADD_MESSAGE, @"「ちんぽっ♥　おちんぽっ♥　せーし射精るっ♥　射精ちゃうぅっ♥♥」\n"
			ENDIF
		ENDSELECT
		
	CASE "発情"
		; 媚薬発情時
		IF CFLAG:28 != 0
			CALL KOJO_16_0_ADD_MESSAGE, @"（きもちいいの…　はやく、おさまってくださいまし…っ！）\n"
			CALL KOJO_16_0_FIX_MESSAGE
			
			CALL KOJO_16_0_ADD_MESSAGE, @" 「…ッ！？　―――ッ♥　―――ッッ♥♥」\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			CALL KOJO_16_0_ADD_MESSAGE, @"\d強制的に発情させられた%CALLNAME:TARGET%は、全身が蕩けるような快感にだらしない顔で絶頂し続けている！\d\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			CALL KOJO_16_0_ADD_MESSAGE, @"（あっ…あああぁ…っ！？　からだ…が…　おかしく、されて…　いつもの何倍も、感じて…っ！？）\n"
			
		; 累積発情時
		ELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「こんなこと続けてたら…　あたしおかしくなっちゃいますわ…」\n"
		ENDIF
		
	CASEELSE
		CALL KOJO_16_0_ADD_MESSAGE, @"「うぁ…ぁっ…　はぁ……　ぁ……\n　こんな時に…何をしているんですの、あたし……」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「ぁぁ…　ぁ…　自分でいじって…　イっちゃい…ましたわ…」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「……ッ！　…んんっ！　…んふぅっ！！\n　はぁ……♥　はぁ……♥　これで少しは…　ぞわぞわする身体…　マシに……！」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「ぅんんっ…！　ううっ…！ 不本意…ですけれど…っ\n　魔物に…無理矢理イかされるより、何倍もマシ…ですわ…！」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「気持よくなるための…っ　おなにーじゃ…ない…からっ♥\n　気持ちいいの…　はやく…おさまってくださいまし…♥」\n"
	ENDSELECT
RETURN 1


