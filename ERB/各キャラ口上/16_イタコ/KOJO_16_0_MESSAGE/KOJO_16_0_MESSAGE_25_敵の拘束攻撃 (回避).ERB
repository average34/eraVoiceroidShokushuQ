
; ------------------------------------------------------------------------------
; ■ 25 敵の拘束攻撃 (回避)
; ------------------------------------------------------------------------------

; □ 汎用口上
@KOJO_16_0_SYSTEM_MESSAGE_25(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 催眠中
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_16_0_KOJO_MODE
	CASE "淫乱"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あたしを捕まえたいなら、もっとしっかり狙うことですわ♪」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「ふふっ♪あたしはこっちですわ～♪」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あたしを捕まえたいならもっとがんばってくださいまし♥」\n"
	CASE "発情"
		CALL KOJO_16_0_ADD_MESSAGE, @"「よ、避けれましたわ……」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あ、危なかったですわ……」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あんなのに捕まったらあたし……」\n"
		CALL KOJO_16_0_PARAGRAPH_MESSAGE
		CALL KOJO_16_0_ADD_MESSAGE, @"\d%ARGS:1%の攻撃を回避できた事に、%CALLNAME:TARGET%は火照る身体を抱きしめながら安堵の声を漏らした…\d\n"
	CASE "強気"
		CALL KOJO_16_0_ADD_MESSAGE, @"「そんな大振りであたしを捕まえるなんて甘いですわ♪」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あたしを捕まえたいならもっと工夫することですわ！」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「そんな単調な動きでは、あたしは捉えられませんわよ」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「遅いですわ！」\n"
	CASEELSE
		CALL KOJO_16_0_ADD_MESSAGE, @"「残念でしたわね、避けるのは実は得意なんですの」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あぶなかったですわぁ……！」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「ふぅ、ちゃんと避けられましたわ……」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あんなのに捕まったら、どうなってしまうんでしょう……」\n"
	ENDSELECT
	CALL KOJO_16_0_FIX_MESSAGE
	
	; 服を破って拘束を回避時追加口上
	IF BASE:3 != KOJO_16_0_PREV_BASE:3
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「にゃあ♥　そんなに焦らずに楽しみましょうね♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「少しずつ脱がすの…♥　なんだかえっちですわね……♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ちゅわっ！服が……！」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「服が残ってなければ…今頃は…」\n"
		CASE "強気"
			CALL KOJO_16_0_ADD_MESSAGE, @"「もうっ！　そんなに袖を引っ張らないでくださいまし！」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「破れた服の代金分、痛い目にあわせてさしあげますわ！」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「この服、結構気に入ってましたのに…」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「服がなければ危なかったですわ……」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「破けてしまいましたわ…　でも、捕まるよりマシですわね…」\n"
		ENDSELECT
	ENDIF
RETURN 1


