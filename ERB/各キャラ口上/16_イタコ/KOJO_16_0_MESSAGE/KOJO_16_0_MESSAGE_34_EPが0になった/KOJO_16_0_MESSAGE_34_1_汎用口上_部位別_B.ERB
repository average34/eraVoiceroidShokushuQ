			ENDSELECT

		CASE "発情"
			; B感覚 + 素質[淫乱] + 素質[淫乳] で分岐
			SELECTCASE (ABL:7 + TALENT:74 + TALENT:76)
			CASE 0
				CALL KOJO_16_0_ADD_MESSAGE, @"「うそ……っ！？　おっぱいが、ジンジンって……！？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「くうぅぅ……っ……　胸が……　胸がぞわぞわしてますわ……っ！」\n"
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"「ダメだからっ！！　おっぱい……っ！！　おっぱいでイッちゃうからあぁぁぁっ！！」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「あついんですの……っ！！　おっぱいが、あつく…て…っ！！」\n"
			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"「ダメっ♥　もうダメですのっ♥　おっぱい熱いっ♥♥　熱いのおおぉぉっッ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「ごめんなさいっ♥　胸で気持ちよくなってごめんなさいっ♥　だめっ♥　だめっ♥　イカせ、イカせないでくださいまひぃぃっ♥♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「くるっ♥　きちゃうぅ……っ♥　おっぱいでっ♥　おっぱいでイっちゃいますのおおおぉぉぉぉっ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「イクっ♥　イクっ♥　イクううぅぅぅっ♥♥　許してくださいぃぃっ♥♥　えっちなおっぱい許してくださいましぃぃっっ♥♥♥」\n"
			ENDSELECT
			
		CASEELSE
			; B感覚 + 素質[淫乱] + 素質[淫乳] で分岐
			SELECTCASE (ABL:7 + TALENT:74 + TALENT:76)
			CASE 0
				CALL KOJO_16_0_ADD_MESSAGE, @"「どうしてですの……！？　胸で、こんなに……！？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「く…ぅ…っ！？　胸が、ゾクゾクしてますわ……っ！？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「う、ウソですわよね……！？　胸で、こんなに感じるなんて――！？」\n"
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"「いやですのに……っ！　胸で気持ちよくなる……なんて……っ！！」\n"
