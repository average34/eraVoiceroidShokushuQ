
; ------------------------------------------------------------------------------
; ■ 34 EPが0になった - 前回情報参照 461 ふたなり治療(治療する側)
; ------------------------------------------------------------------------------

; □ 0 V (治療する側)
@KOJO_16_0_SYSTEM_MESSAGE_34_PREV_461_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 優先口上指定
	CALL KOJO_16_0_SET_IMPORTANT_FLAG()
	
	; 追加地の文関数(前)を登録
	CALL KOJO_16_0_REGIST_DESCRIPTION_BEFORE("KOJO_16_0_SYSTEM_MESSAGE_34_DESCRIPTION_BEFORE")
	
	; 催眠中
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_16_0_KOJO_MODE
	CASE "淫乱"
		CALL KOJO_16_0_ADD_MESSAGE, @"\d%CALLNAME:TARGET%は腰を押しつけ一際深くペニスをくわえ込むと、絶頂寸前の子宮口へ密着させた\d\n"
		CALL KOJO_16_0_PARAGRAPH_MESSAGE
		CALL KOJO_16_0_ADD_MESSAGE, @"「ふふっ♥　あたしの一番奥で搾り取ってあげますわ♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「そろそろ…　あたしのおまんこ、本気モードで治療してあげますわね♥♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"（子作りセックスはじめたら、治療なんてもうどうでもよくなっちゃいますわね……っ♥♥）\n"
		
		; 妊娠していない or 妊娠中(判明前)なら追加口上
		IF !CFLAG:36 || CFLAG:36 == 2
			CALL KOJO_16_0_ADD_MESSAGE, @"「このまま赤ちゃんの部屋に出したらっ♥　二人の子供が出来てしまうかもしれませんわよ♥♥」\n"
		ENDIF
		
	CASE "発情"
		CALL KOJO_16_0_ADD_MESSAGE, @"\d%CALLNAME:TARGET%は無意識のうちに子宮口をペニスを密着させ、艶めかしく腰を動かしている\d\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"\d%CALLNAME:TARGET%は無意識のうちに子宮口をペニスを密着させ、流し込まれる精液を待ち構えている\d\n"
		CALL KOJO_16_0_PARAGRAPH_MESSAGE
		CALL KOJO_16_0_ADD_MESSAGE, @"「イ、イクっ♥　イッちゃう♥　イッちゃうっ♥♥　イッちゃうっ♥♥♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"（抜かないとっ！　抜かないとダメですのに…っ！　あたしの子宮、精液ほしがって吸いついてますわ…っ！）\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"（抜かないとぉ……っ♥　抜かないと……ただの子作りになっちゃいますわっ♥♥\n　ダメ……ですのにぃっ……♥　気持ち、いいんですのっ♥　きもちよすぎるんですのぉっッッ♥♥♥）\n"
		
		; 妊娠していない or 妊娠中(判明前)なら追加口上
		IF !CFLAG:36 || CFLAG:36 == 2
			CALL KOJO_16_0_ADD_MESSAGE, @"（こっ……このままじゃ妊娠しちゃう♥♥　でも……っ、とまらない、とまらないんですのっっ♥♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「抜いてっ♥　抜いてくださいましっっ♥♥　赤ちゃん出来ちゃいますわ♥♥　貴方ので妊娠しちゃいますのおぉっっ♥♥」\n"
		ENDIF

	CASEELSE
		CALL KOJO_16_0_ADD_MESSAGE, @"「なか…ぁ…　ジンジンしびれ…てますわっ♥　もう、だ……め……っ♥♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「ち、治療ですのにっ♥　あたしの方が先に、果てちゃいますわ…っ♥」\n"
	ENDSELECT
RETURN 1


