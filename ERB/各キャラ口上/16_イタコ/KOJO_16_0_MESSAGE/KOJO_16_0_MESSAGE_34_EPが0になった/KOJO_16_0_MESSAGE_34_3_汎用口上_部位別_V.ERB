			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「クるうぅっ♥　また大事なとこに気持ちイイのきちゃうぅぅぅっ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「ふーっ♥　ふーっ♥　おまっ…んこ…♥　赤ちゃんのっ……お部屋ぁッッ♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	CASE 2 TO 3
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「あそこが……あついですわ……っ♥　寸止めで、おかし、く……♥♥」\n"
		ENDSELECT
		
	CASE 4 TO 5
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「もうガマンできませんのっ♥♥　イキたいっ♥♥　トロトロのおまんこでイキたいんですのっ♥♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（イかせてっ♥　イかせてくださいいましっ♥♥　おまん……こ……でっ……イか、せてええぇぇっッッ♥♥♥）\n"
		ENDSELECT
		
	CASE 6 TO 8
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ぐっちゃぐちゃのおマンコっッッっ……でっ、無様イキっしてみせますわッ……ですから……ぁっ♥♥　もう、もう寸止めやめてくださいましいぃぃぃっ♥♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あはははッ♥♥　まだおまんこイかせてもらえませんわっッ♥♥♥　狂っちゃあぁぁぅぅッッ♥♥♥♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ぐちゃぐちゃオマンコおぉぉッ♥♥　イカせてえぇぇぇっ♥　イカへぇてくださぃまひいぃっ……♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「んひいいぃぃっ♥♥　イカせてぇっ♥♥　エロまんこ……っ、もっともっと虐めてぇえぇぇっッ♥♥」\n"
		ENDSELECT
	
	CASEELSE
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あﾞぁﾞあﾞあﾞっ♥♥　気持ちいいぃぃィぃっ♥♥\n　変態堕落マンコぉ……っ♥♥　もう、もうイイイイイッちゃイイイィィィそうですのぉおぉッ♥♥♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「イカせ、てえぇぇぇっ♥　イカへぇて、くださいまひぃぃっ……♥♥　\nもう、おまんこ壊しても……いいです……わぁッッっ♥♥」\n"
		ENDSELECT
	ENDSELECT
RETURN 1
