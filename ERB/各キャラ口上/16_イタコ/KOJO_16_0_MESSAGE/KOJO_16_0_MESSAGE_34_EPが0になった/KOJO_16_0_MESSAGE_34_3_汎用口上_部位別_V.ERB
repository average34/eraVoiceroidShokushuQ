			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"（イクッ♥　イクイクイクううぅっ♥♥　おまんこ顔無様に晒して、イっちゃうぅぅぅっっッ♥♥）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「そっ、そこイイっ♥♥　ぐりぐり好きいぃっ♥♥　赤ちゃんの部屋ぐりぐり好きなんですのぉぉぉっッ♥♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「あははっ♥♥　イクぅぅっ♥♥　ドスケベおまんこイっちゃううぅぅっ♥♥♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「ひいぃっ♥　ひぎいぃぃっッ♥　おちんっぽっ穴アﾞグメぇぇっッ♥　んおﾞあﾞぁぁぁぁぁっ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「おﾞ…ごっ♥♥　おﾞまﾞんご……っ♥♥　おﾞまﾞんごイグッ♥♥　おﾞまﾞんごイグううゥゥッッ♥♥♥」\n"
			ENDSELECT

		CASE "発情"
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_16_0_ADD_MESSAGE, @"「だめ……っ！？　お腹の奥に、気持ちいいのがキますわ……っ！？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「ああぁ……っ……　これ以上、おまたの穴……刺激されて……はぁぁ……」\n"
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"「くぅぅぅっ！！　あかちゃんの穴があついいぃっ！！　誰か、誰か鎮めてくださいましぃぃっ！！」\n"
			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"「イキ…たくないっ♥　おまんこでイキたくないんですのっ♥♥　頭が快楽でトロトロになっちゃいますからぁっ♥♥♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「おﾞっ♥　おﾞまﾞんごとけるうぅぅ……っ♥　もﾞうううぅだめですのぉぉっっっ……っッ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「イクっ♥　イクっ♥　イクううぅぅぅっ♥♥\n　ごめんなさいっ♥♥　えっちなおまんこでごめんなさいいぃっ♥♥♥」\n"
			ENDSELECT
			
		CASEELSE
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_16_0_ADD_MESSAGE, @"「ひぐ……っ！？　あそこ……がっ……っ！？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「くぅぅっ……！？　アソコが……じんじんしてますわ……っ！？」\n"
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"「だめっ……！！　また大事なとこで気持ちよくなっちゃいますのっ……！！」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「くうぅ……っ！　あつく…なってますわ…っ！　おまたが…あつく……っっ！！」\n"
			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"「ダ……メ……っ♥　おまたが……っ♥　どんどんエッチに……されて……っ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（あそこでイッちゃいますわ……♥　またはしたなくイッちゃううぅぅ……っッ♥♥）\n"
