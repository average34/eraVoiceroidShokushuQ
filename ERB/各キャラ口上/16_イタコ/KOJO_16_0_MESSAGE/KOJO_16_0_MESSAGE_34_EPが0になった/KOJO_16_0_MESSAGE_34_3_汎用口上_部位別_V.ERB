					CALL KOJO_16_0_ADD_MESSAGE, @"「イキますわっ♥　変態マゾまんこ、セックスでイっちゃいますのっ♥♥」\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"「子作り……♥　子作り……♥　子作りぃ……っッ♥」\n"
				ENDIF
			ENDSELECT
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	; 寸止め回数によって分岐
	; 1回目の寸止めは自慰誘発でも使っているので、絶頂寸止め以外の表現が必要
	SELECTCASE CFLAG:820
	CASE 0 TO 1
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_16_0_ADD_MESSAGE, @"「くぅっ♥　くうぅぅっ♥　おまんこ熱いですわっっ♥♥　気持ちいいですのおおおぉぉぉっっ♥♥」\n"
