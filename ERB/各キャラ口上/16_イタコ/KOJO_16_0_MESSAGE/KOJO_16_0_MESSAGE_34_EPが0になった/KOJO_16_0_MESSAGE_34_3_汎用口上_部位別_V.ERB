@KOJO_16_0_SYSTEM_MESSAGE_34_3(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 追加地の文関数(前)を登録
	CALL KOJO_16_0_REGIST_DESCRIPTION_BEFORE("KOJO_16_0_SYSTEM_MESSAGE_34_DESCRIPTION_BEFORE")
	
	; 睡眠 or 喪失時は喋らずに帰る
	SIF CFLAG:25 || CFLAG:29
		RETURN 0
		
	; 催眠中
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_16_0_ADD_MESSAGE, @"「イキ……ますわ……　おまたの穴に、気持ちいいのが……　きてますの……」\n"
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"「イキますわ……　おまんこイキますわ……　イキ……っ　」\n"
			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"「あっ……　あっ……　イキますわ……　%CALLNAME:TARGET%は…交尾穴で絶頂いたしますの……」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「愛液まき散らしながら、交尾穴で本気イキいたしますわ……」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「これから精液便所変態%CALLNAME:TARGET%は絶頂しますわ……　無様おまんこ、イキまくりますの……」\n"
				
				; NEXTコマンド 押し倒され なら追加口上
