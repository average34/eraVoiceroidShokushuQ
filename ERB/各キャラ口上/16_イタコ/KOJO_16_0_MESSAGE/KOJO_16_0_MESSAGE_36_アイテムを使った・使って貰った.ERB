; □ 54 着替えセット
@KOJO_16_0_SYSTEM_MESSAGE_36_54(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 前の服状態による口上
	; 裸だった
	IF KOJO_16_0_PREV_BASE:3 == 0
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「服を着ると少し安心できますわね」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「裸では何されるかわかりませんもの」\n"
		ENDSELECT
		
	; 新品だった
	ELSEIF KOJO_16_0_PREV_BASE:3 == MAXBASE:3
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "強気"
			CALL KOJO_16_0_ADD_MESSAGE, @"「こんな時こそ気分転換が大事ですわ」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「よし、新しい服を着て心機一転ですわ」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ちょっともったいなかったかもしれませんわね」\n"
		ENDSELECT
	
	; 破れていた
	ELSE
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「結構気してましたのよ」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ほんの少しの傷で大事に至ることだってありますわ」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「測ったようにピッタリですわ…？」\n"
		ENDSELECT
	ENDIF
	
	; 服装による追加口上
	SELECTCASE CFLAG:501
	; 00:いつもの服
	CASE 0
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「やっぱりこの服が一番動きやすいですわね♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「どうしてあたしの服が落ちてるのかしら…？　気味が悪いですわね…」\n"
		ENDSELECT
	; 01:普通の服
	CASE 1
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「たまにはこういう普通なのも悪くないですわね」\n"
		ENDSELECT
	; 02:体操着
	CASE 2
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「運動用ですから、動きやすさは抜群ですわね」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「さすがにこんな状況じゃなければ、着ようと思いませんわ……」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「動きやすいのは良いのですが、攻撃はあまり防げそうにありませんわね」\n"
		ENDSELECT
	; 03:メイド服
	CASE 3
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「おかえりなさいませ！ご主人様！……ちゅわ……恥ずかしいですわ！」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「こういうフリフリな服もたまには良いですわね」\n"
		ENDSELECT
	; 04:パジャマ
	CASE 4
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「お布団で寝たくなりますわね……でもここで寝るのは不安ですわ」\n"
		ENDSELECT
	; 05:巫女服
	CASE 5
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「あたしの普段着とは微妙に違いますわね」\n"
		ENDSELECT
	; 06:スクール水着
	CASE 6
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ちゅわ……これを着るのはちょっと躊躇しますわ……」\n"
		ENDSELECT
	; 07:マイクロビキニ
	CASE 7
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ほ、ほとんど丸見えですわ…♥　この状態で魔物に襲われたら…♥♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「な、何も着てないよりはマシ…ですわ…」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ちゅわ！布が少なすぎますわ！」\n"
		ENDSELECT
	; 08:ボディスーツ
	CASE 8
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「カラダにぴっちりしてて、裸よりエッチな気がしますわ…」\n"
		ENDSELECT
	; 09:セーラー服
	CASE 9
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ずんちゃんの学校の制服ですわ……なんであたしにピッタリなんですの？」\n"
		ENDSELECT
	; 10:半ズボン
	CASE 10
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「こんなにふとももを晒すなんて……閃きましたわ！」\n"
		ENDSELECT
	; 11:妊婦服
	CASE 11
		; 妊娠・寄生されている
		IF CFLAG:36 >= 3 || (CFLAG:22 && CFLAG:36 == 0)
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「マタニティドレス…今のあたしには必要ですわね…」\n"
			ENDSELECT
			
		; 通常状態
		ELSE
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「なんだかこれを着ていると、孕ませて欲しいみたいじゃないですの…」\n"
			ENDSELECT
		ENDIF
	; 12:ランジェリードレス
	CASE 12
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「こんなの着てたら♥　誘ってるって思われちゃいますにゃ♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「うぅ…　この服、透けててエッチですわ…っ…」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ここでこれを着ていれば注目の的ですわね…悪い意味で…」\n"
		ENDSELECT
	; 13:セーター
	CASE 13
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「暖かくて不思議な安心感がありますわ」\n"
		ENDSELECT
	; 14:チャイナドレス
	CASE 14
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「スリットが深すぎて激しく動いたら全部見えちゃいそうですわね」\n"
		ENDSELECT
	; 15:貞操帯
	CASE 15
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「たまには禁欲をしなさいというお告げでしょうか…」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「こ、こんなのを付ける日が来るとは思いませんでしたわ…\n　でも、仕方ありませんわね。安全第一で行きましょう…」\n"
		ENDSELECT
	; 16:ビキニアーマー
	CASE 16
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「こんな重い物の身に着けたら、お相手のやりたい放題に…♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「お、重い…ですわ！　その分、防御力に期待ですわね…」\n"
		ENDSELECT
	; 17:園児服
	CASE 17
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「にゃぁっ♥　エッチじゃないのにすごくエッチですわ♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「つまりこういう服の子を犯すのが大好きな殿方が居る、ということですわね♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ど、どうしても着なきゃいけませんの…？　きりちゃんでももう着ませんわよ！」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（これは身を守るため……私の趣味じゃありませんわ……）\n"
		ENDSELECT
	; 18:エプロン
	CASE 18
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「もしかして炊事中にこの迷宮に落ちてしまった方がいらっしゃるのでしょうか」\n"
		ENDSELECT
	; 19:ローブ
	CASE 19
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「イタコは魔法使いじゃありませんけど、使えたら良かったですわ」\n"
		ENDSELECT
	; 20:ウェディングドレス
	CASE 20
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「こんなのを着たら触手のお嫁さんになるみたいですわ♥　それも悪くありませんわね♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「きっとこのドレスがもっと真っ白になるまで…♥　あぁ…楽しみですにゃ♥♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「前にずんちゃんに着せようとしたらずんだドレスにされましたわね…」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「東北家は代々白無垢ですから、ドレスには憧れてましたわ♪」\n"
		ENDSELECT
	; 21:ふんどし
	CASE 21
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「修行の時には履きますけれども…」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ま、まさか…下着だけですの！？」\n"
		ENDSELECT
	; 22:触手服
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ちゅわぁ……これは服と呼んでいいんですの？」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ぢゅっ！？！？　この服、動きましたわ！…　あたしの手をぺろんって舐めましたわ！」\n"
		ENDSELECT
	; 23:スリングショット
	CASE 23
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あぁ♥　食い込んで♥　集中できませんわ♥♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「これはもう服じゃなくて紐ですわ！？」\n"
		ENDSELECT
	ENDSELECT
	
	; 過去に触手スーツを経験済みなら追加口上あり
	; ただし触手服選択時は除く
	IF GETBIT(CFLAG:900, 2) && CFLAG:501 != 22
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あら？　今回はあの動く服じゃありませんのね……」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「またあの生きてる服を着てみたいですわ…♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「この服は動かない服ですわね……急に動き出したりしたら……っ♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"（ふぅ…　この服は大丈夫なようですわ…）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（今回はまともな服であってほしいですわ…）\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「ふぅ…これは普通の服でしたわ…」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「もう変な服は嫌ですわ…」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「この服は…　大丈夫ですの…？」\n"
		ENDSELECT
	ENDIF
RETURN 1


