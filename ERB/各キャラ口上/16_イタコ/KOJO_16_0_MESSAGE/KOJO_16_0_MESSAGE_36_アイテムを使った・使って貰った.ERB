	SIF ARG:2 != 0
		RETURN 0
		
	; 口上呼び出し前にCP回復が発生するので回復後の数値を考えて
	TEMP = BASE:CP * 100 / MAXBASE:CP
	
	; 服:新品
	IF TEMP >= 100
	; 服:通常
	ELSEIF TEMP > 66
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「治療は早いうちにするのが大切ですわ！」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「今でできるのはこれくらいですわね…」\n"
		ENDSELECT
		
	; 服:損傷
	ELSEIF TEMP > 33
	; 服:中破
	ELSE
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「何も着ていないより、いくらかましですわ…」\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
		ENDSELECT
	ENDIF
RETURN 1

