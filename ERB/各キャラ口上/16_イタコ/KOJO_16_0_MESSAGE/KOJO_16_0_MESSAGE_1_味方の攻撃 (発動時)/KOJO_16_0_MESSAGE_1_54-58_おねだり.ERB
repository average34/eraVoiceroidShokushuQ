	; 先制攻撃成功時 追加口上
	IF KOJO_16_0_GET_PREV_ARG(0) == 60 && KOJO_16_0_GET_PREV_ARG(2) == 1
		SELECTCASE KOJO_16_0_KOJO_MODE
