	ENDIF
	CALL KOJO_16_0_PARAGRAPH_MESSAGE
	
	; りばーすはPRINT_MESSAGEで処理されるので、メッセージ表示タイミングも制御
	IF KOJO_16_0_IS_REVERSE()
