			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%無様に肉欲を貪るだけの雌と化した%FONT_COLOR_ESCAPE%"
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%交尾の事しか考えられなくなった%FONT_COLOR_ESCAPE%"
