			IF CFLAG:35
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%性欲処理の悦びに支配された%FONT_COLOR_ESCAPE%"
			ELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%催眠にかかった%FONT_COLOR_ESCAPE%"
