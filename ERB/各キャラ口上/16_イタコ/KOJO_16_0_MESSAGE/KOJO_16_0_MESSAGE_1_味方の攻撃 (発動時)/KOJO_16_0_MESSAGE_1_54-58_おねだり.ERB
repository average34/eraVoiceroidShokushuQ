			IF CFLAG:35
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%無理矢理絶頂に導かれた%FONT_COLOR_ESCAPE%"
			ELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%身体の自由を奪われた%FONT_COLOR_ESCAPE%"
			ENDIF
