			ENDSELECT
		ENDIF
	ENDIF
	
	; 淫乱系素質(A) [尻穴狂い]所持 または りばーす東方触手宮でA感覚LV3以上
	IF ARG:1 == 54 && (TALENT:75 || (KOJO_16_0_IS_REVERSE() && ABL:6 >= 3))
		CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%アナルセックス中毒の%FONT_COLOR_ESCAPE%"
