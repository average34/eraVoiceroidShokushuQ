				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%イキ顔を晒したまま%FONT_COLOR_ESCAPE%"
				CALL KOJO_16_0_ADD_MESSAGE, @"%FONT_COLOR_ESCAPE%本能に屈した%FONT_COLOR_ESCAPE%"
			ENDSELECT
			
		; 通常時
