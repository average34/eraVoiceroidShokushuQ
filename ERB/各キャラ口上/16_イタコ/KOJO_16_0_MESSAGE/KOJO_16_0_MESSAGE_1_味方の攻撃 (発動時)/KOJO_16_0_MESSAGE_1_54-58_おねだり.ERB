		ENDSELECT
	ENDIF
	
	; PTメンバーなしのソロプレイ追加口上
	IF GET_MEMBER_SUM() == 1
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"（あたしったら…触手におねだりしてるっ……誰も助けてくれませんのに……）\n"
