
; ------------------------------------------------------------------------------
; ■ 457 性欲発散(催眠中)
; ------------------------------------------------------------------------------

; □ 追加地の文
@KOJO_16_0_SYSTEM_MESSAGE_457_DES_DESCRIPTIVE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	SELECTCASE ARG:1
	; 催眠中
	CASE 0, 5, 10, 15
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠の力で身体の自由を奪われた%ARGS:1%に、%CALLNAME:TARGET%は囁いた\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠の力で身体の自由を奪われた%ARGS:1%に、%CALLNAME:TARGET%は命令した\d\n"
		; 発情・感情操作系
		CASE 1
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠の力で快楽を得る事しか考えられなくなった%ARGS:1%に、%CALLNAME:TARGET%は囁いた\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠の力で快楽を得る事しか考えられなくなった%ARGS:1%に、%CALLNAME:TARGET%は命令した\d\n"
		; 無意識・認識操作系
		CASE 2
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠の力で何でも言うことを聞くようになった%ARGS:1%に、%CALLNAME:TARGET%は囁いた\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠の力で何でも言うことを聞くようになった%ARGS:1%に、%CALLNAME:TARGET%は命令した\d\n"
		ENDSELECT
		
	; 同意
	CASE 1
		CALL KOJO_16_0_ADD_MESSAGE, @"\d今から何をされるか察し身体の力を抜いた%ARGS:1%に、%CALLNAME:TARGET%はゆっくりと近付いた\d\n"
	ENDSELECT
	CALL KOJO_16_0_PARAGRAPH_MESSAGE
RETURN 1


; □ 00 V
@KOJO_16_0_SYSTEM_MESSAGE_457_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 追加地の文読み込み
	CALL KOJO_16_0_SYSTEM_MESSAGE_457_DES_DESCRIPTIVE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; タコ姉が主人公
	IF TARGET == 1
		; 汎用口上
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱" 
			CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%のおまんこぉ♥　たぁくさん膣内射精してあげますわ♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あたしのっ……♥　あたしの発情した身体、鎮めてくださいまし……♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「お、おちんちんっ！　あたしのおちんちん、%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%のおまんこで気持よくしてくださいましっ！」\n"
		ENDSELECT
		
		; 遭遇キャラが[淫壺]所持時 追加口上
		IF TALENT:(ARG:3):78
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%のおまんこ、いつでも準備万端なんですのね♥\n　とってもえっちなニオイがしてるから、あたしのおちんぽガッチガチになっちゃいましたわ♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「あ……これ、たっくさん開発されてエッチに造り替えられたんですのね♥\n　精液狂いにされちゃった%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%のおちんぽ専用穴、ひくひくしてますわ♥」\n"
			CASEELSE
			ENDSELECT
		ENDIF
		
		; 催眠種類で分岐 各種追加口上
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%は今からあたしを使ってオナニーをしてくださいまし♥　使っていいのはわたし%TACHI()%のおちんぽだけですのよ？　頑張ってくださいまし♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「おちんぽ勃起を鎮めてくださる、心優しい方は居ないでしょうか♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%は下のお口で、あたし%TACHI()%の男の人のモノを咥え込んでくださいまし？」\n"
			ENDSELECT
			
		; 発情・感情操作系
		CASE 1
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「はい、%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%の大好きなおちんちんですわっ♥　犯したいですわよね♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「『私はセックスのことばかり考える変態淫乱おまんこ奴隷です』\n　って言えたら%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%の大好きなおちんちん、食べさせてあげますわよ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「今の%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%はとってもえっちな孕み奴隷♥　孕ませて貰う事しか考えられないんですのね♥♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「え、えっとっ…　%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%の大好きなセックスの時間ですわ……　特別にあたし%TACHI()%のおちんちん使ってもいい、ですわよ？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「今から%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%は、あたし%TACHI()%のおちんちんから精液を絞り取るんですのよね　ほら、我慢しなくて良いんですわよ？」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「とってもえっちな目でおちんちん見てますわね　ほら、好きに加えこんで良いんですのよ」\n"
			ENDSELECT
			
		; 無意識・認識操作系
		CASE 2
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%のおまんこは精液便所ですの\n　精液便所の前におちんぽがあれば、やることは判りますわよね♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%はトロトロに涎を垂らしてる下の口を使って、あたし%TACHI()%のおちんちんを気持ちよくしたい…　そう、ですわね？」\n"
			ENDSELECT
		ENDSELECT
		
	; タコ姉はパーティーメンバー
	ELSE
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱" 
			CALL KOJO_16_0_ADD_MESSAGE, @"「あたしも%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%の中に沢山射精して、お腹ぱんぱんに膨れ上がらせてあげたいですわ♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「あ、あの！　あたしのおちんちんも気持ちよくしてくださいまし…！　%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%達見てたら、ガマンできなくなっってしまいましたの…」\n"
		ENDSELECT
	ENDIF
	CALL KOJO_16_0_PARAGRAPH_MESSAGE
	
	; 地の文を表示
	CALL KOJO_16_0_ADD_MESSAGE, @"\M"
RETURN 1


; □ 01 V(同意)
; 発生条件が結構厳しめ
; 相手が(レズ中毒 LV1超 or [エネミー]持ち) & 友好度200以上 で発生
@KOJO_16_0_SYSTEM_MESSAGE_457_1(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 追加地の文読み込み
	CALL KOJO_16_0_SYSTEM_MESSAGE_457_DES_DESCRIPTIVE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; タコ姉が主人公の時しかこの口上は呼び出されない
	; パーティーメンバー状態での口上は不要
	SELECTCASE KOJO_16_0_KOJO_MODE
	CASE "淫乱"
		CALL KOJO_16_0_ADD_MESSAGE, @"「はやくおちんぽくださいって顔に書いてありましてよ、変態さん♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「これが欲しいんですのよね？　もう、%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%は淫乱なんですからっ♥\n　そんな顔されたら、私も張りきっちゃいますわよ♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「あは…　ガマンできないんですの？\n　%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%のおまんこ♥　ぐっちゃぐちゃに犯して欲しくてえっちな蜜が溢れてきちゃってますわね♥」\n"
	CASEELSE
		CALL KOJO_16_0_ADD_MESSAGE, @"「今から%KOJO_16_0_CALLNAME_FROM_ID(ARG:3)%は、あたし%TACHI()%に犯されちゃうというのに…すっごい嬉しそうですのね？」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「そんな顔で見ないでくださいまし…　余計に興奮してきちゃいますわ……」\n"
	ENDSELECT
	CALL KOJO_16_0_PARAGRAPH_MESSAGE
	
	; 地の文を表示
	CALL KOJO_16_0_ADD_MESSAGE, @"\M"
RETURN 1


