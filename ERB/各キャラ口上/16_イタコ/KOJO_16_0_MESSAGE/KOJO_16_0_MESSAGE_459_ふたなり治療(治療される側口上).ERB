
; ------------------------------------------------------------------------------
; ■ 459 ふたなり治療(治療される側口上)
; ------------------------------------------------------------------------------

; □ X 汎用口上
@KOJO_16_0_SYSTEM_MESSAGE_459(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 催眠時処理
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			CALL KOJO_16_0_ADD_MESSAGE, @"「あたしの精液でよければ、いくらでも……　お射精しいたしますわ……」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「どうぞ……　あたしの勃起ちんぽ……　お楽しみくださいまし……」\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠術で射精奴隷へ堕ちた%ARGS:1%は、ペニスを嬲る%ARGS:0%を虚ろな表情で眺めている……\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠術で射精奴隷へ堕ちた%ARGS:1%は、%ARGS:0%に精液を捧げるため快感を貪っている……\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠術を掛けられた%ARGS:1%は、無様に射精する姿を見て貰う事が何よりの幸せだと思い込んでいる！\d\n"
			RETURN 1
		ENDSELECT
	ENDIF
	
	; 戦意喪失中
	IF CFLAG:29
		; 射精中毒 Lv0
		IF ABL:12 == 0
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「さわっ…　ちゃぁ…　だぁ、めですわ……」\n"
			ENDSELECT

		; 射精中毒 Lv1
		ELSEIF ABL:12 == 1
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「そんな…に…　された…らぁ…　でちゃ…い…　ますわ……っ」\n"
			ENDSELECT

		; 射精中毒 Lv2
		ELSEIF ABL:12 == 2
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「だ…め…ぇ…　おちん…ちん…　だ…め……」\n"
			ENDSELECT

		; 射精中毒 Lv3以上
		ELSEIF ABL:12 >= 3 || TALENT:121
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「おちん…ちん…　きもちよく…　して…ぇ……」\n"
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	; 通常時
	; 射精中毒 Lv0
	IF ABL:12 == 0
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「まだあまり気持ちよくなれないので…　開発して欲しいですわっ♪」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「うっ…　あたしだって好きでこんなの付けてるわけじゃないんですのよ…」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「だめっ！　そこダメっ！　いじっちゃダメぇぇっ！」\n"
		ENDSELECT

	; 射精中毒 Lv1
	ELSEIF ABL:12 == 1
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「あたしのおちんぽ治してくださいましっ♥　こんなのついてたらみんなの事襲っちゃいますわ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「そんなえっちな治療されたら、おちんちんで気持ちよくなるの……　忘れられなくなるかもしれませんわ…♥♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ぁ…っ、おちんちん……　勝手に大きくなってますの…っ……」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「そこ…っ、敏感だから……　優しくさわって…くださいまし…」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"（%KOJO_16_0_CALLNAME_FROM_ID(1)%に触られて、おちんちん勃起…しちゃってますわ…っ）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「いや…ぁっ！　さわらないでくださいましっ！　そこ触られるとっ…！　何故か気持ちよくなっちいますのっ…！」\n"
		ENDSELECT

	; 射精中毒 Lv2
	ELSEIF ABL:12 == 2
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「治療なんて言ってますが、ホントはおちんぽが欲しいんですのよね♥\n　いいですわ♥　あたしのおちんぽお好きにどうぞ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「もっと♥　壊れるくらい激しくしてください…ましぃ♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「どうして…っ…！　おちんちん…　びくびくして…　とまりませんわ…っ！？」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「壊れちゃうっ…！　おちんちん壊れちゃうからぁああああっ！！」\n"
		ENDSELECT

	; 射精中毒 Lv3以上
	ELSEIF ABL:12 >= 3 || TALENT:121
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「うぁ…ぁっ！　きもち…ぃ…っ♪　おちんちんっ、いじめられるの…っ♪　だいすきっ…ですのっ♪」\n "
		ENDSELECT
	ENDIF
RETURN 1


; □ 0 V
@KOJO_16_0_SYSTEM_MESSAGE_459_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	; 催眠時処理
	IF KOJO_16_0_IS_HYPNO()
