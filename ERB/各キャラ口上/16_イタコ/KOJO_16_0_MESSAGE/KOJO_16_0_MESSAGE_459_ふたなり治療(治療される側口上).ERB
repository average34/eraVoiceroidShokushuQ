		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			CALL KOJO_16_0_ADD_MESSAGE, @"「はい……　%KOJO_16_0_CALLNAME_FROM_ID(1)%様のおまんこに……　ぐつぐつに煮えたぎった濃い精子、射精させていただきますわ……」\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_16_0_ADD_MESSAGE, @"\d催眠術で射精奴隷へ堕ちた%ARGS:1%は、%ARGS:0%の雌穴を満足させる為必死に腰を振っている！\d\n"
			RETURN 1
		ENDSELECT
	ENDIF
	
	; 射精中毒 Lv0
	IF ABL:12 == 0
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(1)%！？　自分で何やってるかわかってますの…っ！？　ひぎっ…！！」\n"
		ENDSELECT
		
	; 射精中毒 Lv1
	ELSEIF ABL:12 == 1
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「自分からくわえこむとか%KOJO_16_0_CALLNAME_FROM_ID(1)%って淫乱だったんですのね♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"（あたしっ、あたし%KOJO_16_0_CALLNAME_FROM_ID(1)%とセックスしちゃってますわ♥）\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「うくぅぅっ…　あたしのモノが、%KOJO_16_0_CALLNAME_FROM_ID(1)%のおまんこに…はいって…！」\n"
		ENDSELECT
		
	; 射精中毒 Lv2
	ELSEIF ABL:12 == 2
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「治療なんて嘘、ですわよね♥　%KOJO_16_0_CALLNAME_FROM_ID(1)%のおちんぽを見る目、発情した雌犬状態ですわよ？\n　ふふっ、じゃあ治療をお願いしますわ♥　お互い、いっぱい気持ちよくなりましょう♥♥」\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"（中に射精したいっ♥　%KOJO_16_0_CALLNAME_FROM_ID(1)%を、あたしの精液で妊娠させたいですわ……っ♥♥）\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(1)%のナカぁっ…ぬるぬるして気持ちいいですの…っ　おちんちん暴れちゃって…ぇっ」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「くぅぅっ！　おちんちんが言うこと聞きませんわっ！　%KOJO_16_0_CALLNAME_FROM_ID(1)%の中でおっきくなって…っ！！」\n"
		ENDSELECT
		
	; 射精中毒 Lv3以上
	ELSEIF ABL:12 >= 3 || TALENT:121
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「そんな事されたら…っ♥　ガマンできなくて、ナカに射精しちゃいますの…ぉっ♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「あたしのおちんちん、%KOJO_16_0_CALLNAME_FROM_ID(1)%に膣内射精したがってますわ…！」\n"
		ENDSELECT
	ENDIF
	
	; 治療する側が[淫壺]所持 or V感覚LV3以上なら追加口上
	IF TALENT:1:78 || ABL:1:9 >= 3
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「なるほど、%KOJO_16_0_CALLNAME_FROM_ID(1)%のおまんこぐっちゅぐちゅに改造されちゃったんですのね？\n　雄を発情させるニオイ、スッゴいですわ♥　あたしのおちんちんもバッキバキに勃起しちゃってますの♥♥」\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(1)%の中凄いっ♥　何ですのこれっ！？　溶けるぅっ♥♥　おちんちん溶けちゃうぅっ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ひゃあぅっ♥　おちん……ちんっ、搾り…とられ……っ♥♥」\n"
		ENDSELECT
	ENDIF
RETURN 1


