					CALL KOJO_16_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_16_0_ADD_MESSAGE, @"\d思考と身体を操られているにも関わらず、%CALLNAME:TARGET%の声は繰り返される寸止めに蕩けきっている…\d\n"
				ENDSELECT
				
			; 通常時
			ELSE
				SELECTCASE CFLAG:43
				CASE 1
					CALL KOJO_16_0_ADD_MESSAGE, @"（あれ？　どうしてあたし、オナニーしていますの……？　目の前に敵が……ますのに……？）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"（ひっ！？　あたしオナニーしてるっ！？　どうして！？）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"（手が勝手に大事なとこを…っ！？　いやぁっ！　とまって！　とまってくださいましっ！！）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"（身体が……操られてるいますわっ！？　ど、どうにかして抜け出さな…ひぐぅっ♥）\n"
				CASE 2
					CALL KOJO_16_0_ADD_MESSAGE, @"（オナニーすれば…このピンチを乗り切れる……はずですわ…？）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"（自分を壊すぐらいおまんこ掻き回せば、なんとかなるかもしれませんわ……）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"「隙を見せましたね！　あなたの弱点はお見通しですわ！　さあ、あたしの本気オナニーをとくとご覧あれ！」\n"
