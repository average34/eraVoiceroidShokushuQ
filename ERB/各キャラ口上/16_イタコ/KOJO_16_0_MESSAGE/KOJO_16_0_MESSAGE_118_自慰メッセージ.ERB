		CALL KOJO_16_0_ADD_MESSAGE, @"「……♥　……っ♥　……んぅっ♥　……あぅっ♥」\n"
		CALL KOJO_16_0_PARAGRAPH_MESSAGE
		
		CALL KOJO_16_0_ADD_MESSAGE, @"\dどうやら%CALLNAME:TARGET%は、夢の中でも自慰をしているらしい…\d\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"\d眠ったまま自慰をする%CALLNAME:TARGET%の手の動きが激しくなっていく…\d\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"\d眠ったまま自慰をする%CALLNAME:TARGET%は、甘い声をあげるだけで目を覚まさない…\d\n"
		RETURN 1
	ENDIF
	
	; 催眠中
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
