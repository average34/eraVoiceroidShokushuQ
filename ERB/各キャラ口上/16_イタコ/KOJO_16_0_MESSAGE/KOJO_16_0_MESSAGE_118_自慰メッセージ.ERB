					CALL KOJO_16_0_ADD_MESSAGE, @"\d肉便器として犯して貰う為、%CALLNAME:TARGET%は媚びた声をあげながら自慰を見せつけた\d\n"
				ENDSELECT
			ENDIF
			RETURN 1
		ENDSELECT
	ENDIF

	; 汎用口上
	; 寸止め or 自慰誘発中
	IF CFLAG:820
		; 寸止めカウンタの数値で分岐
		SELECTCASE CFLAG:820
		; 1回目の寸止めは誘発を含むので注意
		CASE 1
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「もうガマンできませんのっ♥　今すぐ気が狂うほど気持ちよくなりたいですわ……っ♥　んあぁ……ぁっ♥♥」\n"
			CASE "発情"
				CALL KOJO_16_0_ADD_MESSAGE, @"（はしたない事ですの……にっ！　頭の中が、オナニーで一杯になって……ますわぁっ！？）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（手が……っ、勝手に…ぐちゅぐちゅって…っ！　止めなきゃ♥　止めなきゃいけませんのにぃぃっ♥♥）\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"（どうして……　こんな時におな……にぃを…？　やだっ！？手が止まりませんわっ！？）\n"
			ENDSELECT
		CASE 2 TO 3
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「あたしのっ……　あたまが……♥　狂ってきてるぅっ♥\n　ぐちゃぐちゃに……　なっていますわ……♥　ぐちゃぐちゃにぃっ…♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「イきたいっ♥　もっとイきたいですわっ♥　もっともっと死ぬほど沢山イき狂いたいんですのぉっ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「あっっっっああっああっあっっーー♥\n　ぐちゃぐちゃ掻き回すの好いですわぁ……ぁ♥　飛んじゃううぅぅっ♥♥」\n"
			CASE "発情"
				CALL KOJO_16_0_ADD_MESSAGE, @"「オナニーがっ！　オナニーするのがとまりませんわっ♥　いやぁっ♥　あっ♥　あっあっ♥\n　イクっ♥♥　イッちゃ♥♥　イクイクイクイクうううぅぅぅううう♥♥♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「お願い……いたしますわっ……　イカせて…ください……ましっ……　イけなくて、あたしの身体……もう……！」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（寸止め……されて……っ……　あたしのカラダ……おかしくなってる……ぅっ…♥　イキ、たいっ……　イキたいイキたいイキたい…！！）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「ぅあﾞ～っ♥　ぅあﾞあﾞ～～っ……っ♥\n　カラダが熱く……て……もうガマンでき……ませんわっ♥♥\n　イカしてくれないと頭…おかしく…♥　狂って……しまいますぅ♥♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「ひぁっ♥　ひぐっ♥　おごっ♥　だれか……だれかとめてくださいましっ……♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「だめぇ♥　だめなんですのっ♥　こんなはしたない事でっ♥　気持ちよくなったらっっ♥♥　おっ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「とまらないっ♥　全然止まりませんわっっ♥　オナニー中毒になっちゃうぅぅっ♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「おなにいっ……！　おなにいい……ぃっ……！　がまん……できませんのっっ♥\n　もっとイキたいっ……♥　イキたいんですっ♥♥」\n"
			ENDSELECT
		CASEELSE
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱", "発情"
				CALL KOJO_16_0_ADD_MESSAGE, @"「おなにーであたし、ぐちゅぐちゅのとろとろ……ですわ…♥\n　あは……♥　あたし、もうおかしくなっちゃってますの……♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「もうイカせて下さいいましぃぃっ♥　狂っちゃうからっ♥　ぐるっぢゃうがらああああぁああ゛ぁ゛ぁ゛あ゛あ゛っ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「助け……っ、助けて下さいましぃぃっ♥♥　イケなくて……っ♥　くるひくて……っ♥　壊れてっ…しまいますわぁ…っ♥♥」\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「イキたいっ♥　イキたいんですの♥♥　もうっ、いじわる……しないでくださいましぃっ♥♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「こんにゃにっ……オナニーしてるんですからぁぁ……♥　そろそろイカせて下さいまひぃぃっ……♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	; 通常時
	ELSE
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"「おなにぃっ♥　おにゃにぃ……っ♥　しゅきぃっ♥　これしゅきなんですにょぉっ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「おっ……ごぉ……っ♥　ぎぼぢ…いぃいっ♥　これぎぼぢよすぎれすわぁああっ♥♥」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「はっ♥　あが……ぁぁ♥　うぉ…ぁっ♥♥　あっ♥あっ♥あっ♥　あああぁぁあ゛あ゛あ゛あ゛っっ♥♥♥」\n"
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_16_0_ADD_MESSAGE, @"\d頭の中が自慰をする事で埋め尽くされ、%CALLNAME:TARGET%は完全に周りが見えなくなっている…\d\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"\d自慰の快感に取り付かれた%CALLNAME:TARGET%は、オスを誘うフェロモンと淫らな声を撒き散らしている…\d\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"（少しだけ……　少しだけ触るだけで、この疼きはおさまる……はずですわ……）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（だ……めぇっ、こんな、はしたない、ことっ……！）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「ふあぁ……♥　んうっ……っおっ♥　……っ♥」\n（き…気持よくなんてありませんわ…！これはこの疼きを鎮める治療行為…なん……ですの♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（あっ♥　覚えてしまいますわ…っ！　あたしの身体が…オナニーを覚えてしまいますのっ！♥♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（カラダ……おかしくされちゃった……から……　あたしのせいじゃ……ありません…わ…♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（実際に死ぬほど犯されてるのに……孕み袋にされる妄想がとまりませんのっ……！）\n（もっと、もっと気持ちいいの沢山ほしいですわっ♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（こんな気持ちいいオナニー初めてですわ…っ！？）\n（このまま続けたらオナニー中毒になっちゃういますわ！）\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"「どうにかして……コレをっ……しずめませんとっ……！」\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"「こうしませんとっ……頭おかしくなっちゃいそう……ですのっ……♥\n　いっぱ……いっ……気持ちよく……しませんとっ♥♥」\n"
		ENDSELECT
		CALL KOJO_16_0_FIX_MESSAGE
	ENDIF
	
	; 戦闘中追加口上
	IF TFLAG:13
		; 寸止め or 自慰誘発中
		IF CFLAG:820
			; 寸止めカウンタの数値で分岐
			SELECTCASE CFLAG:820
			; 1回目の寸止めは誘発を含むので注意
			CASE 1
				SELECTCASE KOJO_16_0_KOJO_MODE
				CASE "発情"
				CASEELSE
					CALL KOJO_16_0_ADD_MESSAGE, @"（目の前に……敵がいる……のにっ！？　自慰なんてっ、してる場合じゃない……はずですのにぃっ！？）\n"
				ENDSELECT
			CASE 2 TO 3
				SELECTCASE KOJO_16_0_KOJO_MODE
				CASEELSE
				ENDSELECT
			CASEELSE
				SELECTCASE KOJO_16_0_KOJO_MODE
				CASEELSE
				ENDSELECT
			ENDSELECT
			
		; 通常時
		ELSE
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_16_0_ADD_MESSAGE, @"「見られながらのするのって、普段より……っ♥　ぞくぞくってして……すき……ですわっ♥♥」\n"
			CASE "発情"
				CALL KOJO_16_0_ADD_MESSAGE, @"（あ……ぁっ……　あたし…オナニーしてるところ、見られてますわ……っ……）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（今のあたし……おかしくなってる…オナニー大好き変態女になってますわぁ……♥）\n（たたかわなきゃ……いけない……のにぃ……♥）\n（手が……とまりませ……ん…♥♥）\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「……あっ……はっ……んああぁっ♥\n　こっち見な……いでくださっ……♥　んくうぅっ……♥」\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"「だめ……♥　なのにぃっ……♥\n　こんなはしたないこと……してるっ場合じゃ……ない…んですのっ♥♥」\n"
			ENDSELECT
			CALL KOJO_16_0_FIX_MESSAGE
			
			; ターン終了時自慰発生なら追加口上
			IF CFLAG:39 || KOJO_16_0_GET_PREV_PURE_ARG(0) == 48
				SELECTCASE KOJO_16_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_16_0_ADD_MESSAGE, @"「あっ♥　あっあっ♥♥　イきますっ♥　もうイッちゃいますわっ♥♥」\n"
				CASE "発情"
					CALL KOJO_16_0_ADD_MESSAGE, @"（イキたい……っ♥　いますぐイキ狂いたいぃ……っ♥♥）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"「うぁ……ぁ……♥　おまんこ掻き回すの…ガマン……できませんわぁ……っ♥♥」\n"
				CASEELSE
					CALL KOJO_16_0_ADD_MESSAGE, @"（一回だけっ……　一回イくことが出来れば、戦えるようになるはずですの……っ）\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"「ムリ……ですわっ……　オナニー我慢するの……ムリ……なんですのっ……」\n"
					CALL KOJO_16_0_ADD_MESSAGE, @"「ダ…メ……ぇ……　ガマンが…できませんわ……っ……」\n"
				ENDSELECT
			ENDIF
			CALL KOJO_16_0_FIX_MESSAGE
			
			; まだ仲間が行動中なら追加口上
			IF (TARGET != 1 && BASE:1:1 > 0) || (FLAG:10 && TARGET != FLAG:10 && BASE:(FLAG:10):1 > 0) || (FLAG:11 && TARGET != FLAG:11 && BASE:(FLAG:11):1 > 0)
				; まだ行動中キャラのIDを取得
				VARSET NAME_ID
				WHILE 1
					SELECTCASE RAND:3
					CASE 0
						IF BASE:1:1 > 0 && TARGET != 1
							NAME_ID = 1
						ENDIF
					CASE 1
						IF FLAG:10 && BASE:(FLAG:10):1 > 0 && TARGET != FLAG:10
							NAME_ID = FLAG:10
						ENDIF
					CASE 2
						IF FLAG:11 && BASE:(FLAG:11):1 > 0 && TARGET != FLAG:11
							NAME_ID = FLAG:11
						ENDIF
					ENDSELECT
					
					SIF NAME_ID
						BREAK
				WEND
				
				; 好感度によって分岐
				; 好感度:低
				IF CFLAG:NAME_ID:5 < 0
					SELECTCASE KOJO_16_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っ、ヒドイ……っ♥♥　ゴミを見る目でこっち見てますわ……っ♥♥」\n"
						CALL KOJO_16_0_ADD_MESSAGE, @"（ヒトをクズを見るみたいな目で見て……♥♥　%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%なんかオカズにしてあげますわっ♥♥）\n"
					CASE "発情"
						CALL KOJO_16_0_ADD_MESSAGE, @"（%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%に気付かれないように……　うう、やりにくいですわね……）\n"
					CASEELSE
						CALL KOJO_16_0_ADD_MESSAGE, @"（%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%には、見られないように……）\n"
					ENDSELECT
					
				; 好感度:警戒
				ELSEIF CFLAG:NAME_ID:5 < 50
					SELECTCASE KOJO_16_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っ……そんな目で見ない……でっ♥」\n"
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%がこっち見て…ますわ♥　興奮…しちゃったので…許して下さいましっ♥♥」\n"
					CASE "発情"
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っ、そんな軽蔑の眼差しを向けないで……　とまらないんですの……♥」\n"
					CASEELSE
						CALL KOJO_16_0_ADD_MESSAGE, @"（見ないでくださいまし……　%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%、ゴミを見るような目で見ないで……！）\n"
					ENDSELECT
					
				; 好感度:通常
				ELSEIF CFLAG:NAME_ID:5 < 200
					SELECTCASE KOJO_16_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_16_0_ADD_MESSAGE, @"「仲間に見られてますわ…♥　あっ♥あっ♥　コーフン…してしまいますわっ♥♥」\n"
					CASEELSE
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っ、こ、こっち見ないで……くださいまし……！　恥ずかしい……ですわっ……」\n"
					ENDSELECT
					
				; 好感度:高
				ELSEIF CFLAG:NAME_ID:5 >= 200
					SELECTCASE KOJO_16_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っ♥　とまりませんわっ♥　貴方が見てくださってると思うとあたし…あたし……っ♥♥」\n"
						CALL KOJO_16_0_ADD_MESSAGE, @"（%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%っ……♥　もっと見てくださいまし♥♥\n　今あたし、%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%の事考えながらシてるんですのよ……っ♥♥）\n"
					CASEELSE
						CALL KOJO_16_0_ADD_MESSAGE, @"（%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っ♥　%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%……っっ♥♥　ごめんなさいっ、あたし%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%の事考えながらぁ……っ♥♥）\n"
						CALL KOJO_16_0_ADD_MESSAGE, @"「%KOJO_16_0_CALLNAME_FROM_ID(NAME_ID)%に見られてますわ……っ♥　恥ずかしいっ♥　気持ちいいっ♥　恥ずかしいっ♥♥　気持ちいいぃぃ……っっ♥♥」\n"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
RETURN 1


