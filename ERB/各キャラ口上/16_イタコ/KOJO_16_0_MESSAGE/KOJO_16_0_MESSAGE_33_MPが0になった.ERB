@KOJO_16_0_SYSTEM_MESSAGE_33(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 催眠中
	IF KOJO_16_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; 催眠深度によって変化
			SELECTCASE CFLAG:43
			CASE 1
				CALL KOJO_16_0_ADD_MESSAGE, @"（こんな所で倒れたら……　頭の中まで……完全に、壊されます…のに…）\n"
			CASE 2
				CALL KOJO_16_0_ADD_MESSAGE, @"（あ……ぁ……　もっと……ご奉仕しないと……いけません……のに……）\n"
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"（もう……動けません……　せめて、生オナホとして……使って……くだ……さい……まし……）\n"
				CALL KOJO_16_0_ADD_MESSAGE, @"（申し訳……ありませんわ……　もう……意識……が……）\n"
			ENDSELECT
			RETURN 1
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_16_0_KOJO_MODE
	CASE "淫乱"
		CALL KOJO_16_0_ADD_MESSAGE, @"「もうだめです♥　あたまのなか……トロットロに……　とけて……しまいましたわぁ……♥♥」\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"「もうだめっ♥　こうさんっ♥　こうさんですわ…っ♥♥」\n"
	CASE "発情"
		CALL KOJO_16_0_ADD_MESSAGE, @"（もうこの魔物に逆らえませんわ……　あたし……）\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"（ああ……　身体に力が、はいりませんの……）\n"
	CASEELSE
		CALL KOJO_16_0_ADD_MESSAGE, @"（まずいですわ……　あたまが……　ぼやけて……）\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"（あたしの幸せ……　どこに……いってしまったんでしょう……）\n"
		CALL KOJO_16_0_ADD_MESSAGE, @"（ずんちゃん……きりちゃん……ごめんな……さい……）\n"
	ENDSELECT
	
	; まだ行動中の味方がいるなら追加口上
	IF (TARGET != 1 && CHECK_FINE(1)) || (FLAG:10 && TARGET != FLAG:10 && CHECK_FINE(FLAG:10)) || (FLAG:11 && TARGET != FLAG:11 && CHECK_FINE(FLAG:11))
		SELECTCASE KOJO_16_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_16_0_ADD_MESSAGE, @"（あたしが気持ちいいのを担当しますわね♥……みんな後は頼みましたわ♥）\n"
			CALL KOJO_16_0_ADD_MESSAGE, @"（他の方が頑張ってますし、あたしは快楽を貪っても良い……ですわよね……♥）\n"
		CASE "発情"
			CALL KOJO_16_0_ADD_MESSAGE, @"（あたしはもう……ダメなようです……　後を、お願いしますわ……）\n"
		CASEELSE
			CALL KOJO_16_0_ADD_MESSAGE, @"（後は……　任せ……ましたわ……）\n"
		ENDSELECT
	ENDIF
RETURN 1


