			ENDIF
			
		; 見当違いの所を防御
		CASE 50, 51, 52
			SELECTCASE KOJO_16_0_KOJO_MODE
			CASEELSE
				CALL KOJO_16_0_ADD_MESSAGE, @"「あっ…！嫌っ…ダメっ…！」\n"
			ENDSELECT
			CALL KOJO_16_0_PARAGRAPH_MESSAGE
			
			; 寄生攻撃
			IF ARG:2 == 0
				CALL KOJO_16_0_ADD_MESSAGE, @"\d%PARTS%を守る%CALLNAME:TARGET%を横目に、%ARGS:1%は無防備な膣を蹂躙している…\d\n"
			; 妊娠攻撃
			ELSE
