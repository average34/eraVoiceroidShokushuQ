@KOJO_DEFAULT_17_22(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)



;●睡眠
IF CFLAG:25 > 0

	CALL COMMON_KOJO_DEFAULT_SLEEP_TEST(1)

	RETURN 1
ENDIF


	
	; important属性付与
	; 敵のその他攻撃に汎用口上は無いけれど、専用口上なので設定
	CALL SET_IMPORTANT_FLAG
	
	; この攻撃が命中した場合、必ず催眠中となる
	; 記述口上は全て催眠中のものになるので注意
	SELECTCASE CFLAG:43
	; 催眠強度1
	CASE 1
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL ADD_RAND_MESSAGE, @"「あ……れ……？　身体……うまく……動……」"
			CALL ADD_RAND_MESSAGE, @"「うっ、動けない！？」"
			
		; 発情・感情操作系
		CASE 1
			CALL ADD_RAND_MESSAGE, @"（何を嫌がってたんだろ……　みんな気持ちいい事をしてくれるだけなのに……）"
			CALL ADD_RAND_MESSAGE, @"（あれ……？　なんかお腹の奥が……熱い……？）"
			
		; 無意識・認識操作系
		CASE 2
			CALL ADD_RAND_MESSAGE, @"「何をしたの……　頭がくらくらする……」"
		ENDSELECT

	; 催眠強度2
	CASE 2
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL ADD_RAND_MESSAGE, @"（もう、いい加減にしてよ！）"
			
		; 発情・感情操作系
		CASE 1
			CALL ADD_RAND_MESSAGE, @"（セックス♥　セックスしたいっ♥　どこでもいいからつっこんで欲しいっ♥）"
			CALL ADD_RAND_MESSAGE, @"（あっ♥　だめっ♥　子宮が降りてきちゃった♥）"
			
		; 無意識・認識操作系
		CASE 2
			CALL ADD_RAND_MESSAGE, @"（ご奉仕……　ご奉仕しなきゃ……）"
		ENDSELECT
		
	; 催眠強度最大、又はレッサーヒュプノの減衰あり催眠
	CASEELSE
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL ADD_RAND_MESSAGE, @"（完全に身体操られてる……）"
			CALL ADD_RAND_MESSAGE, @"（身体が……どうしたらいいの……どうしたら……！）"
			
		; 発情・感情操作系
		CASE 1
			CALL ADD_RAND_MESSAGE, @"「ああっ♥　あああぁっ♥　欲しいっ、気持ちいいのくださいっ♥」"
			CALL ADD_RAND_MESSAGE, @"「ひぃ……っ♥　我慢できなくなるぅ……っ♥　犯してっ、早く犯してぇっ♥♥」"
			CALL ADD_RAND_MESSAGE, @"「子宮が♥　赤ちゃんの部屋がきゅんきゅん止まらない♥♥　止まらないのぉ♥♥」"
			
		; 無意識・認識操作系
		CASE 2
			CALL ADD_RAND_MESSAGE, @"「どこを使って気持ちよくなりたいですか？\n　皆様が気持ちよくなる為なら%CALLNAME:TARGET%は何でもします」"
			
			; 素質[尻穴狂い]所持時 or A感度3以上 追加口上
			IF TALENT:75 || ABL:6 >= 3
				CALL ADD_RAND_MESSAGE, @"「どこを使って気持ちよくなりたいですか？\n　トロトロに蕩けたお尻の穴がオススメですよ」"
			ENDIF
			
			; 素質[淫壺]所持時 or V感度3以上 追加口上
			IF TALENT:78 || ABL:9 >= 3
				CALL ADD_RAND_MESSAGE, @"「どこを使って気持ちよくなりたいですか？\n　セックス専用に作り替えられたヌルヌルおまんこ、準備できてますよ」"
			ENDIF
			
			; 素質[キス魔]所持時 or M感度3以上 追加口上
			IF TALENT:79 || ABL:15 >= 3
				CALL ADD_RAND_MESSAGE, @"「どこを使って気持ちよくなりたいですか？\n　犯されてイクようになった変態お口まんこ、楽しんで下さい」"
			ENDIF
		ENDSELECT
	ENDSELECT
RETURN 1

