;茜のイベント用口上（外付け）です。
;この口上は美鈴口上を抜粋、改変して製作させていただきました。
;
;【この口上限定のFLAG】
;CFLAG:951⇒自販機遭遇済みフラグ
;CFLAG:952⇒祭壇お風呂の発情済みフラグ
;CFLAG:953⇒ケモミミの実（動物耳）体験済みフラグ
;CFLAG:954⇒動物耳の除去経験フラグ
;CFLAG:955⇒石碑の既読フラグ
;CFLAG:956⇒薬草入手フラグ
;（新設順のためCASEの順番通りの番号振りではありません。ややこしいですがご了承ください）

@EVENT_AKANE_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)


	SELECTCASE ARG:0
	;通常の固定イベント
	CASE 200
	;ARG:1にイベントID　ARG:2で同じイベントの詳細分岐
	;※イベントIDは固定イベント.ERDのIDと同一

		SELECTCASE ARG:1
			;美味しい湧き水（飲むとHP・MP・EP全快、飲まなかった場合は残る）
			CASE 130
				SELECTCASE ARG:2
					CASE 0
					PRINTFORML 「ごくっ…これは…！」
					CALL WAIT_AUTOMODE(400, 0)
					PRINTFORML 「美味すぎるっ！この迷宮にこんなおいしい水があるなんて…！」
					CALL WAIT_AUTOMODE(400, 0)
					CASE 1
					PRINTFORML （いやいやこんなんどう見たって罠やろ…）
					CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			;Hな湧き水（飲むとEPが1になる、飲まなかった場合は残る）
			CASE 131
				SELECTCASE ARG:2
					CASE 0
					PRINTFORML 「ごくっ…これは…！」
					CALL WAIT_AUTOMODE(400, 0)
					PRINTFORML 「…ぺっ、ぺっ！…変な味や～…」
					CALL WAIT_AUTOMODE(400, 0)
					PRINTFORML 「うっ、なんやコレっ！？…身体が…熱いぃ…っ❤」
					CALL WAIT_AUTOMODE(400, 0)
					CASE 1
					PRINTFORML （いやいやこんなのどう見たって罠やろ…）
					CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			;活力の湧き水（飲むと最大HPが上昇）
			CASE 132
				SELECTCASE ARG:2
					CASE 0
					PRINTFORML 「ごくっ…これは…！」
					CALL WAIT_AUTOMODE(400, 0)
					PRINTFORML 「うーん、あんまり美味しくあらへんなぁ…」
					CALL WAIT_AUTOMODE(400, 0)
					PRINTFORML 「…おぉ？！　なんか元気が湧いてきたで！」
					CALL WAIT_AUTOMODE(400, 0)
					CASE 1
					PRINTFORML （いやいやこんなのどう見たって罠やろ…）
					CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			;澄んだ泉（飲むとMP25％回復、入るとEP50％回復　無制限に使えるため拠点用）
			CASE 133
				SELECTCASE ARG:2
					;飲んでMP回復
					CASE 0
						PRINTFORML 「ごくっ…これは…！」
						CALL WAIT_AUTOMODE(400, 0)
						PRINTFORML 「なかなか美味しい水やな」
						CALL WAIT_AUTOMODE(400, 0)
					;入ってEP回復
					CASE 1
						PRINTFORML 「水や、水や！」（ｻﾞｯﾊﾟｧﾝ!）
						PRINTFORML 「ふぃ～全身に染み渡るわぁ…」
						CALL WAIT_AUTOMODE(400, 0)
					;無視
					CASE 2
						PRINTFORML （いやいやこんなのどう見たって罠やろ…）
						CALL WAIT_AUTOMODE(400, 0)
					;喪失から回復（させる側）
					CASE 3
						PRINTFORML 「今水を飲ませたるからな！」
						CALL WAIT_AUTOMODE(400, 0)
					;喪失から回復（する側）
					CASE 4
						PRINTFORMDL %CALLNAME:TARGET%は顔から全身に水を浴びせられた！
						PRINTFORML （ｼﾞｬﾊﾞﾊﾞﾊﾞ…）「うぷっ！…ウエッホ、ゲホッ…」
						CALL WAIT_AUTOMODE(400, 0)
						PRINTFORML 「っはぁ！…助かったわ…ありがとうな…」
						IF FLAG:AUTOMODE
							PRINTFORMDL どうやら正気を取り戻したようだ
							TWAIT FLAG:AUTOMODE * 400, 0
						ELSE
							PRINTFORMDW どうやら正気を取り戻したようだ
						ENDIF
					;薬草を入手
					CASE 5
						;前にゲットした
						IF CFLAG:956
							PRINTFORML 「お、生えとる生えとる。時間が経てばまた採れるのは便利やな」
							CALL WAIT_AUTOMODE(400, 0)
						;初ゲット
						ELSE
							PRINTFORML 「ん、これは…薬になりそうな野草やな」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML 「綺麗な水場の側やし、摘んだ後からも結構生えとるな」
							CALL WAIT_AUTOMODE(400, 0)
							CFLAG:956 = 1
						ENDIF
				ENDSELECT
			;白濁した泉（飲むとMP全快or発情、入ると速度+5or寄生（種類はランダム））
			CASE 134 
				SELECTCASE ARG:2
					;飲んでMP全回復
					CASE 0
						;発情中
						IF CFLAG:28
							PRINTFORML 「いただきます❤」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML （ｽﾞｽﾞｽﾞｯ）「…なんや、元気になるだけか～…」
							CALL WAIT_AUTOMODE(400, 0)
						;それ以外
						ELSE
							PRINTFORML 「あんまり飲みたくあらへんけどなぁ…」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML （ｺﾞｸｯ）「…甘い…この水、滋養強壮効果があるみたいやな」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;飲んで眠気
					CASE 1
						;発情中
						IF CFLAG:28
							PRINTFORML 「いただきます❤」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML （ｽﾞｽﾞｽﾞｯ）「…ん、あれ…なんか…眠いな…」
							CALL WAIT_AUTOMODE(400, 0)
						;それ以外
						ELSE
							PRINTFORML 「あんまり飲みたくあらへんけどなぁ…」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML （ｺﾞｸｯ）「…うっ！これ…睡眠…薬の…やった…か…」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;飲んで発情
					CASE 2
						;発情中
						IF CFLAG:28
							PRINTFORML 「いただきます❤」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML （ｽﾞｽﾞｽﾞｯ）「…っ！ふあっ❤身体が、火照ってっ❤気持ちいひぃ❤❤」
							CALL WAIT_AUTOMODE(400, 0)
						;それ以外
						ELSE
							PRINTFORML 「あんまり飲みたくあらへんけどなぁ…」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML （ｺﾞｸｯ）「……っ？！ひゃうっ❤身体っ❤熱ちゅいっ❤❤」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;入って基礎速度UP
					CASE 3
						;発情中
						IF CFLAG:28
							PRINTFORML 「あは♪どうなるか楽しみやな❤」（ｻﾞﾊﾟﾝ!）
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML 「………なんや、つまらんな～」
							CALL WAIT_AUTOMODE(400, 0)
						;それ以外
						ELSE
							PRINTFORML 「何か危なそうやけど…」（ｻﾞﾌﾟﾝ…）
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML 「…！！身体がちょいとだけ軽くなったで！見かけによらず秘湯やね」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;入って寄生
					CASE 4
						;発情中
						IF CFLAG:28
							PRINTFORML 「あは♪どうなるか楽しみやな～❤」（ｻﾞﾊﾟﾝ!）
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML 「ひんっ❤何か❤入って来るぅ❤❤」
							CALL WAIT_AUTOMODE(400, 0)
						;それ以外
						ELSE
							PRINTFORML 「何か危なそうなんやけど…」（ｻﾞﾌﾟﾝ…）
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORML 「…ひっ！なんかいる！？入ってこんといてぇっ…！」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;何もせず無視
					CASE 5
						;発情中
						IF CFLAG:28
							PRINTFORML 「ちょっと残念やけど…また今度やな❤」
							CALL WAIT_AUTOMODE(400, 0)
						;それ以外
						ELSE
							PRINTFORML （いやいやこんなのどう見たって罠やろ…）
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;入ってふたなる
					CASE 6
						;発情中
						IF CFLAG:28
							PRINTFORML 「あは♪どうなるか楽しみやな❤」（ｻﾞﾊﾟﾝ!）
							CALL WAIT_AUTOMODE(400, 0)
							;既にふたなりだった
							IF CFLAG:42
								PRINTFORML 「…んっ❤ひやっ❤おちんぽに刺激がっ❤❤」
								IF FLAG:AUTOMODE
									PRINTFORMDL %CALLNAME:TARGET%は既にふたなりだったため、あまり効果が無かったようだ
									TWAIT FLAG:AUTOMODE * 400, 0
								ELSE
									PRINTFORMDW %CALLNAME:TARGET%は既にふたなりだったため、あまり効果が無かったようだ
								ENDIF
							ELSE
								PRINTFORML 「んっ❤…クリがっ❤なんか変やっ❤…ひああっっ❤」
								IF FLAG:AUTOMODE
									PRINTFORMDL なんと%CALLNAME:TARGET%の股間に陰茎が生えた！
									TWAIT FLAG:AUTOMODE * 400, 0
								ELSE
									PRINTFORMDW なんと%CALLNAME:TARGET%の股間に陰茎が生えた！
								ENDIF
							ENDIF
						;それ以外
						ELSE
							PRINTFORML 「何か危なそうなんやけど…」（ｻﾞﾌﾟﾝ…）
							CALL WAIT_AUTOMODE(400, 0)
							IF CFLAG:42
								PRINTFORML 「…ひっ❤股間にっ❤刺激がっ❤❤」
								IF FLAG:AUTOMODE
									PRINTFORMDL %CALLNAME:TARGET%は既にふたなりだったため、あまり効果が無かったようだ
									TWAIT FLAG:AUTOMODE * 400, 0
								ELSE
									PRINTFORMDW %CALLNAME:TARGET%は既にふたなりだったため、あまり効果が無かったようだ
								ENDIF
							ELSE
								PRINTFORML 「…ひあっ、なんか股間が変にっ！…んひあっ❤！！」
								CALL WAIT_AUTOMODE(400, 0)
								PRINTFORML 「…ひぃ…ひぃ…！？　な、なんやコレぇ？！」
								IF FLAG:AUTOMODE
									PRINTFORMDL なんと%CALLNAME:TARGET%の股間に陰茎が生えた！
									TWAIT FLAG:AUTOMODE * 400, 0
								ELSE
									PRINTFORMDW なんと%CALLNAME:TARGET%の股間に陰茎が生えた！
								ENDIF
							ENDIF
						ENDIF
				ENDSELECT
			;不思議な泉（飲むとHP・MP・EP全快、入ると寄生を含む全状態異常治療＆処女ありモードの場合処女も回復）
			CASE 135
				SELECTCASE ARG:2
					;飲んでHP・MP・EP全快
					CASE 0
						PRINTFORML 「ごくっ…これは…！」
						CALL WAIT_AUTOMODE(400, 0)
						PRINTFORML 「おおっ、凄い！体に力が漲ってくるんがわかるで…！」
						CALL WAIT_AUTOMODE(400, 0)
					;入って全状態異常治療
					CASE 1
						PRINTFORML 「ちょいと失礼して…」（ｻﾞﾌﾟﾝ…）
						CALL WAIT_AUTOMODE(400, 0)
						PRINTFORML 「ん～♪体の不調が流れ出てく気がするわ…」
						CALL WAIT_AUTOMODE(400, 0)
					;無視
					CASE 2
						PRINTFORML 「今は遠慮しとくかな…」
						CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			;天井からスライム（危機回避判定失敗でCPが0＝丸裸に）
			CASE 136
				SELECTCASE ARG:2
					;回避失敗
					CASE 1
						;発情中
						IF CFLAG:28
							PRINTFORML 「あっ❤これ、プルプルしてて好きかもしれん❤」
							CALL WAIT_AUTOMODE(400, 0)
						;全裸
						ELSEIF BASE:CP == 0
							PRINTFORML 「ひえっ…ヌメヌメが直に伝わって気持ち悪っ！！」
							CALL WAIT_AUTOMODE(400, 0)
						;その他
						ELSE
							PRINTFORML 「んぶえっ！？　急に何や！？」
							PRINTFORML 「あ～！　服がぜーんぶ溶かされてもうた！」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;回避成功
					CASE 2
						;発情中
						IF CFLAG:28
							PRINTFORML （…ちょっと惜しかったかもしれんなぁ…）
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「ひゃあ！…あんなんに圧し掛かられるとかゾッとするわ…」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
				ENDSELECT
			;壁から触手　危機回避判定失敗で数回ランダム箇所絶頂
			CASE 137
				SELECTCASE ARG:2
					;回避失敗
					CASE 1
						;発情中
						IF CFLAG:28
							PRINTFORML 「そこ❤そこ好きや❤お願い❤もっとぉ❤」
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「ちょっ！？壁がっ……んぐぅ～！」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;回避成功
					CASE 2
						;発情中
						IF CFLAG:28
							PRINTFORML 「もう…するならちゃんと当てーや…❤」
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「壁が…っ！　危ない危ない…」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
				ENDSELECT
			;落とし穴・触手（危機回避判定（易しめ）失敗で多数ランダム絶頂＋寄生攻撃）
			CASE 138
				SELECTCASE ARG:2
					;回避失敗
					CASE 1
						;発情中
						IF CFLAG:28
							PRINTFORML 「へ？……ひあぁ～～！！」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORMDL 真っ直ぐに落ちた茜は触手に絡みつかれた！
							PRINTFORML 「やんっ❤そないにがっつかんといて❤…あひぃっ❤」
							PRINTFORML （ひぁっ♪いい❤ぜんぶきもちいぃ❤）
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「へ？……ひあぁ～～！！」
							CALL WAIT_AUTOMODE(400, 0)
							PRINTFORMDL 真っ直ぐに落ちた茜は触手に絡みつかれた！
							PRINTFORML 「嫌～っ！触らんといて…はひゃぁんっ❤」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;回避成功
					CASE 2
						;発情中
						IF CFLAG:28
							PRINTFORMDL 幸か不幸か茜は落ちずに済んだようだ
							PRINTFORML 「逃げ場が無いのは流石にな❤」
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORMDL 茜はすんでのところで避けた！
							PRINTFORML 「う～わ、グッチョグチョの触手まみれやん…気持ち悪っ…」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;寄生（CASE 1（回避失敗）から繋がる）
					CASE 3
						;発情中
						IF CFLAG:28
							PRINTFORML 「何か…んっ❤来とるっ❤あかんのにぃ❤」
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「ひぃい！？…やめて、入ってこんといて…いやぁあっ！！」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
				ENDSELECT
			;桃色催淫ガス（自慰中毒判定失敗で強制自慰）
			CASE 139
				SELECTCASE ARG:2
					;回避失敗
					CASE 1
						;発情中
						IF CFLAG:28
							PRINTFORML 「うっ、この香りは…❤」
							PRINTFORML 「あっ❤おまんこっ、熱くなっとるぅ❤」
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「しまっ…、ゲッホ、ゴホ…！」
							PRINTFORML 「何や……っ！？あそこが、熱いぃ…❤」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;回避成功
					CASE 2
						;発情中
						IF CFLAG:28
							PRINTFORML （思わず息止めてもうたけど、ちょっと惜しかったかもしれんなぁ…）
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「…ぷはぁ、何とかなったみたいやな？」
							PRINTFORML （明らかにアレな色しとった…危ない危ない）
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
				ENDSELECT
			;誰かの落とし物（荷物袋が落ちている。開けると着替えセット（54）とエナジードリンク（52）を入手。無視した場合は残る）
			CASE 140
				SELECTCASE ARG:2
					;拾った
					CASE 0
						PRINTFORML 「慌てて落っことしたんか、あえて置いて行ったんか、それとも…」
						PRINTFORML 「まぁ、ありがたく頂いとくで」
						CALL WAIT_AUTOMODE(400, 0)
					;放置した
					CASE 1
						PRINTFORML 「な～んか、怪しいなぁ…一旦置いとこ」
						CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			;罠の落とし物（荷物袋が落ちている。が、こちらは中から触手。雑魚一体orヒュプノ＋相手側先制で強制戦闘。無視した場合は残る）
			CASE 141
				SELECTCASE ARG:2
					;拾った
					CASE 0
						;発情時
						IF CFLAG:28
							PRINTFORML 「さて、中身は…って敵や！？」
							PRINTFORML （でも、ある意味プレゼントかも…しれへん❤）
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORML 「さて、中身は…って敵！？騙されたぁ！！」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					;放置した
					CASE 1
						PRINTFORML 「な～んか、怪しいなぁ…一旦置いとこ」
						CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			;喋る触手（A・B・C・Vそれぞれ感覚Lv3を連れて行くとエロイベント後レアアイテムをくれる。コンプリートで全武器１つずつゲット＋喋る触手撤退）
			CASE 142
				SELECTCASE ARG:2
					;開始
					;ARG:3が0は尻、1は胸、2はC、3は膣
					CASE 0

					;終了
					CASE 3
						SELECTCASE ARG:3
						;A
						CASE 0
							PRINTFORM 「あひぃぃいぎゅぅうぅ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　あひゅっ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　おひり…おひり熱ゅぃいぃ
							CALL HEARTCB(0xD02830, 2)
							PRINTFORML 」
							CALL WAIT_AUTOMODE(400, 0)
						;B
						CASE 1
							PRINTFORM 「んひぃぃぃっ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　うちのっおっぱぃいっ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　もっひょ淫ッ乱っ！にぃぃぃっ
							CALL HEARTCB(0xD02830, 2)
							PRINTFORML ！！」
							CALL WAIT_AUTOMODE(400, 0)
						;C
						CASE 2
							;ふたなり
							IF CFLAG:42 || TALENT:121
								PRINTFORM 「ほぎょぉぉぉっっ
								CALL HEARTCB(0xD02830)
								PRINTFORM 　おぉお、おちんっちん……こ…こわりぇちゃ……ッ
								CALL HEARTCB(0xD02830)
								PRINTFORML ！！」
								CALL WAIT_AUTOMODE(400, 0)
								IF FLAG:AUTOMODE
									PRINTFORMDL 茜のペニスは、そよ風に撫でられただけで射精するほど敏感に開発されてしまった。
									TWAIT FLAG:AUTOMODE * 400, 0
								ELSE
