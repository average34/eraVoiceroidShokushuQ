				CALL WAIT_AUTOMODE(400, 0)
				IF FLAG:AUTOMODE
					PRINTFORMDL 茜は未知の感覚に身体を震わせている！
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMDW 茜は未知の感覚に身体を震わせている！
				ENDIF
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「来るっ❤ちんぽ汁来るぅ❤…んおおぉおおぉっ❤❤」
						CALL WAIT_AUTOMODE(400, 0)
					CASE 1
						PRINTFORML 「せーえき、出るっ❤あぁああぁああぁ❤」
						CALL WAIT_AUTOMODE(400, 0)
				ENDSELECT
			ENDIF
		;射精中毒２以上
		ELSEIF ABL:12 > 1
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「うぅっ、あかんのにぃ…ひうぅううぅっ❤」
