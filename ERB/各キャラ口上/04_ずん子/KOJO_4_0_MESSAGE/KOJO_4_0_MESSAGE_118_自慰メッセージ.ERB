@KOJO_4_0_SYSTEM_MESSAGE_118(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM NAME_ID

	; 睡眠中
	IF CFLAG:25
		CALL KOJO_4_0_ADD_MESSAGE, @"「……♥　……っ♥　……んぅっ♥　……あぅっ♥」\n"
		CALL KOJO_4_0_PARAGRAPH_MESSAGE
		
		CALL KOJO_4_0_ADD_MESSAGE, @"\dどうやら%CALLNAME:TARGET%は、夢の中でも自慰をしているらしい…\d\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d眠ったまま自慰をする%CALLNAME:TARGET%の手の動きが激しくなっていく…\d\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d眠ったまま自慰をする%CALLNAME:TARGET%は、甘い声をあげるだけで目を覚まさない…\d\n"
		RETURN 1
	ENDIF
	
	; 催眠中
	IF KOJO_4_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; 寸止め or 自慰誘発中
			IF CFLAG:820
				; 寸止めカウンタの数値で分岐
				SELECTCASE CFLAG:820
				; 1回目の寸止めは誘発を含むので注意
				CASE 1
					CALL KOJO_4_0_ADD_MESSAGE, @"「頭の中が、自慰でいっぱいになっています……　自慰を…します……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「自慰をさせていただきます……ので……　絶頂させて、ください……」\n"
				CASE 2 TO 3
					CALL KOJO_4_0_ADD_MESSAGE, @"「イカせて……ください……　おねがい……っ……　ですからっ……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「寸止め……されて……　おまんこ掻き回すの………とまらない……です……」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「このままイケないと……っ♥　%CALLNAME:TARGET%は……っ♥　オナニー中毒の……変態に、なっちゃいます……っ♥」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「は……っ♥　あ……ぐぅ…っ♥　はひぃ……っ……♥♥」\n"
					CALL KOJO_4_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_4_0_ADD_MESSAGE, @"\d思考と身体を操られているにも関わらず、%CALLNAME:TARGET%の声は繰り返される寸止めに蕩けきっている…\d\n"
				ENDSELECT
				
			; 通常時
			ELSE
				SELECTCASE CFLAG:43
				CASE 1
					CALL KOJO_4_0_ADD_MESSAGE, @"（あれ？　どうしてわたし、オナニーしてるの……？　目の前に敵が……いるのに……？）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（ひっ！？　わたしオナニーしてるっ！？　どうして！？）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（手が勝手に大事なとこを…っ！？　いやぁっ！　とまって！　とまってくださいっ！！）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（身体が……操られてるっ！？　ど、どうにかして抜け出さな…ひぐぅっ♥）\n"
				CASE 2
					CALL KOJO_4_0_ADD_MESSAGE, @"（オナニーすれば…このピンチを乗り切れる……はず…？）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（自分を壊すぐらいおまんこ掻き回せば、なんとかなるかもしれません……）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「隙を見せましたね！　あなたの弱点はお見通しです！　さあ、わたしの本気オナニーをとくとご覧あれ！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「こっちですよ！　本気オナニーによる潮吹きダンスを見せてあげます！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「どう…ですかっ！　私のオナニー…！　早く、降参、してください！」\n"
					CALL KOJO_4_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_4_0_ADD_MESSAGE, @"\d催眠で思考を書き換えられた%CALLNAME:TARGET%は、敵を倒すためには自慰をするしかないと思い込んでいる…\d\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"（オナニーを……　本気でオナニーして、潮吹きダンスしないと……）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「誰かに見られながらのオナニー　すごい……興奮します……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「オナニーすごいです……私の頭の中、全部きもちいい事で一杯になってます……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「%CALLNAME:TARGET%は、ところかまわずオナニーする……　変態女です……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「興奮したら……　いつでも……　犯してください……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（いやらしく……色んなお汁を垂れ流すトコ……見ていただかないと……）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「…はい　今から喜んでオナニーさせていただきます……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「……いまから……オナニーします…ので…、細部までじっくり……御覧ください……ね…」\n"
					CALL KOJO_4_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_4_0_ADD_MESSAGE, @"\dうつろな瞳を宙に向け涎を滴らせながら、%CALLNAME:TARGET%は義務的に快感を与え続けている\d\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"\d肉便器として犯して貰う為、%CALLNAME:TARGET%は媚びた声をあげながら自慰を見せつけた\d\n"
				ENDSELECT
			ENDIF
			RETURN 1
		ENDSELECT
	ENDIF

	; 汎用口上
	; 寸止め or 自慰誘発中
	IF CFLAG:820
		; 寸止めカウンタの数値で分岐
		SELECTCASE CFLAG:820
		; 1回目の寸止めは誘発を含むので注意
		CASE 1
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_4_0_ADD_MESSAGE, @"「もうガマンできませんっ♥　今すぐ気が狂うほど気持ちよくなりたいんです……っ♥　んあぁ……ぁっ♥♥」\n"
			CASE "発情"
				CALL KOJO_4_0_ADD_MESSAGE, @"（はしたない事なの……にっ！　頭の中が、オナニーで一杯になって……ますぅっ！？）\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"（手が……っ、勝手に…ぐちゅぐちゅって…っ！　止めなきゃ♥　止めなきゃいけないのにぃぃっ♥♥）\n"
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"（どうして……　こんな時におな……にぃを…？　やだっ！？手が止まりませんっ！？）\n"
			ENDSELECT
		CASE 2 TO 3
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_4_0_ADD_MESSAGE, @"「わたしのっ……　あたまが……♥　狂ってきてるぅっ♥\n　ぐちゃぐちゃに……　なってる……♥　ぐちゃぐちゃにぃっ…♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「イきたいっ♥　もっとイきたいですっ♥　もっともっと死ぬほど沢山イき狂いたいんですぅっ♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「あっっっっああっああっあっっーー♥\n　ぐちゃぐちゃ掻き回すの大好きぃぃい……ぃ♥　飛んじゃううぅぅっ♥♥」\n"
			CASE "発情"
				CALL KOJO_4_0_ADD_MESSAGE, @"「オナニーがっ！　オナニーするのがとまらいですっ♥　いやぁっ♥　あっ♥　あっあっ♥\n　イクっ♥♥　イッちゃ♥♥　イクイクイクイクうううぅぅぅううう♥♥♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「お願い……しますっ……　イカせて…ください……っ……　イけなくて、私の身体……もう……！」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"（寸止め……されて……っ……　私のカラダ……おかしくなってる……ぅっ…♥　イキ、たいっ……　イキたいイキたいイキたい…！！）\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「ぅあﾞ～っ♥　ぅあﾞあﾞ～～っ……っ♥\n　カラダが熱く……て……もうガマンでき……ませんっ♥♥\n　イカしてくれないと頭…おかしく…♥　狂って……しまうからぁ♥♥」\n"
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「ひぁっ♥　ひぐっ♥　おごっ♥　だれか……だれかとめてくださいっ……♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「だめぇ♥　だめなんですっ♥　こんなはしたない事でっ♥　気持ちよくなったらっっ♥♥　おっ♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「とまらないっ♥　全然止まらないんですっっ♥　オナニー中毒になっちゃうぅぅっ♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「おなにいっ……！　おなにいい……ぃっ……！　がまん……できませんっっ♥\n　もっとイキたいっ……♥　イキたいんですっ♥♥」\n"
			ENDSELECT
		CASEELSE
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASE "淫乱", "発情"
				CALL KOJO_4_0_ADD_MESSAGE, @"「おなにーで私ぐちゅぐちゅのとろとろ……です…♥\n　あは……♥　わたし、もうおかしくなっちゃってる……♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「もうイカせて下さいいぃぃっ♥　狂っちゃうからっ♥　ぐるっぢゃうがらああああぁああ゛ぁ゛ぁ゛あ゛あ゛っ♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「助け……っ、助けて下さいぃぃっ♥♥　イケなくて……っ♥　くるひくて……っ♥　壊れちゃいそうだからぁ…っ♥♥」\n"
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「イキたいっ♥　イキたいんです♥♥　もうっ、いじわる……しないでくださいぃっ♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「こんにゃにっ……オナニーしてるんですからぁぁ……♥　そろそろイカせて下さいぃぃっ……♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	; 通常時
	ELSE
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「おなにぃっ♥　おにゃにぃ……っ♥　しゅきぃっ♥　これしゅきなんですぅっ♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「おっ……ごぉ……っ♥　ぎぼぢ…いぃいっ♥　これぎぼぢよすぎるんですぅううっ♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「はっ♥　あが……ぁぁ♥　うぉ…ぁっ♥♥　あっ♥あっ♥あっ♥　あああぁぁあ゛あ゛あ゛あ゛っっ♥♥♥」\n"
			CALL KOJO_4_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_4_0_ADD_MESSAGE, @"\d頭の中が自慰をする事で埋め尽くされ、%CALLNAME:TARGET%は完全に周りが見えなくなっている…\d\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"\d自慰の快感に取り付かれた%CALLNAME:TARGET%は、オスを誘うフェロモンと淫らな声を撒き散らしている…\d\n"
		CASE "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"（少しだけ……　少しだけ触るだけで、この疼きはおさまる……はずだから……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（だ……めぇっ、こんな、はしたない、ことっ……！）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ふあぁ……♥　んうっ……っおっ♥　……っ♥」\n（気持よくなんてありません…！これはこの疼きを鎮める治療行為…なん……です♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（あっ♥　覚えちゃう…っ！　私の身体が…オナニーを覚えてしまいますっ！♥♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（カラダ……おかしくされちゃった……から……　わたしのせいじゃ……ありません…よね…♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（実際に死ぬほど犯されてるのに……孕み袋にされる妄想がとまりませんっ……！）\n（もっと、もっと気持ちいいの沢山ほしいっ♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（こんな気持ちいいオナニー初めてです…っ！？）\n（このまま続けたらオナニー中毒になっちゃう！）\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「どうにかして……コレをっ……しずめななければっ……！」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「こうしないとっ……頭おかしくなっちゃいそう……なのでっ……♥\n　いっぱ……いっ……気持ちよく……しないとっ♥♥」\n"
		ENDSELECT
		CALL KOJO_4_0_FIX_MESSAGE
	ENDIF
	
	; 戦闘中追加口上
	IF TFLAG:13
		; 寸止め or 自慰誘発中
		IF CFLAG:820
			; 寸止めカウンタの数値で分岐
			SELECTCASE CFLAG:820
			; 1回目の寸止めは誘発を含むので注意
			CASE 1
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "発情"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"（目の前に……敵がいる……のにっ！？　自慰なんてっ、してる場合じゃない……はずなのにぃっ！？）\n"
				ENDSELECT
			CASE 2 TO 3
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
				ENDSELECT
			CASEELSE
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
				ENDSELECT
			ENDSELECT
			
		; 通常時
		ELSE
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASE "淫乱"
				CALL KOJO_4_0_ADD_MESSAGE, @"「見られながらのするのって、普段より……っ♥　ぞくぞくってして……すき……ですっ♥♥」\n"
			CASE "発情"
				CALL KOJO_4_0_ADD_MESSAGE, @"（あ……ぁっ……　わたし…オナニーしてるとこ、見られちゃう……っ……）\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"（今のわたし……おかしくなってる…オナニー大好き変態女になってますぅ……♥）\n（たたかわなきゃ……いけない……のにぃ……♥）\n（手が……とまりませ……ん…♥♥）\n"
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「……あっ……はっ……んああぁっ♥\n　こっち見な……いでくださっ……♥　んくうぅっ……♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「だめ……♥　なのにぃっ……♥\n　こんなはしたないこと……してるっ場合じゃ……ない…んですっ♥♥」\n"
			ENDSELECT
			CALL KOJO_4_0_FIX_MESSAGE
			
			; ターン終了時自慰発生なら追加口上
			IF CFLAG:39 || KOJO_4_0_GET_PREV_PURE_ARG(0) == 48
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「あっ♥　あっあっ♥♥　イきますっ♥　もうイッちゃいますっ♥♥」\n"
				CASE "発情"
					CALL KOJO_4_0_ADD_MESSAGE, @"（イキたい……っ♥　いますぐイキ狂いたいぃ……っ♥♥）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「うぁ……ぁ……♥　おまんこ掻き回すの…ガマン……できないぃ……っ♥♥」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"（一回だけっ……　一回イくことが出来れば、戦えるようになるはず……っ）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「ムリ……ですっ……　オナニー我慢するの……ムリ……なんですっ……」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「ダ…メ……ぇ……　ガマンが…できません……っ……」\n"
				ENDSELECT
			ENDIF
			CALL KOJO_4_0_FIX_MESSAGE
			
			; まだ仲間が行動中なら追加口上
			IF (TARGET != 1 && BASE:1:1 > 0) || (FLAG:10 && TARGET != FLAG:10 && BASE:(FLAG:10):1 > 0) || (FLAG:11 && TARGET != FLAG:11 && BASE:(FLAG:11):1 > 0)
				; まだ行動中キャラのIDを取得
				VARSET NAME_ID
				WHILE 1
					SELECTCASE RAND:3
					CASE 0
						IF BASE:1:1 > 0 && TARGET != 1
							NAME_ID = 1
						ENDIF
					CASE 1
						IF FLAG:10 && BASE:(FLAG:10):1 > 0 && TARGET != FLAG:10
							NAME_ID = FLAG:10
						ENDIF
					CASE 2
						IF FLAG:11 && BASE:(FLAG:11):1 > 0 && TARGET != FLAG:11
							NAME_ID = FLAG:11
						ENDIF
					ENDSELECT
					
					SIF NAME_ID
						BREAK
				WEND
				
				; 好感度によって分岐
				; 好感度:低
				IF CFLAG:NAME_ID:5 < 0
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っ、ヒドイ……っ♥♥　ゴミを見る目でこっち見てます……っ♥♥」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"（ヒトをクズを見るみたいな目で見て……♥♥　%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%なんかオカズにしてあげますっ♥♥）\n"
					CASE "発情"
						CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%に気付かれないように……　うう、やりにくいです……）\n"
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%には、見られないように……）\n"
					ENDSELECT
					
				; 好感度:警戒
				ELSEIF CFLAG:NAME_ID:5 < 50
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っ……そんな目で見ない……でっ♥」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%がこっち見て…ます♥　興奮…しちゃったので…許して下さいっ♥♥」\n"
					CASE "発情"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っ、そんな軽蔑の眼差しを向けないで……　とまらないんです……♥」\n"
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"（見ないでください……　%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%、ゴミを見るような目で見ないで……！）\n"
					ENDSELECT
					
				; 好感度:通常
				ELSEIF CFLAG:NAME_ID:5 < 200
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_4_0_ADD_MESSAGE, @"「仲間が見てる…♥　あっ♥あっ♥　コーフン…しますっ♥♥」\n"
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っ、こ、こっち見ないで……ください……！　恥ずかしい……のでっ……」\n"
					ENDSELECT
					
				; 好感度:高
				ELSEIF CFLAG:NAME_ID:5 >= 200
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASE "淫乱"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っ♥　とまりませんっ♥　貴方が見てくれてると思うとわたし…わたし……っ♥♥」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%っ……♥　もっと見てください♥♥\n　今わたし、%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%の事考えながらシてるんです……っ♥♥）\n"
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っ♥　%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%……っっ♥♥　ごめんなさいっ、わたし%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%の事考えながらぁ……っ♥♥）\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(NAME_ID)%に見られてます……っ♥　恥ずかしいっ♥　気持ちいいっ♥　恥ずかしいっ♥♥　気持ちいいぃぃ……っっ♥♥」\n"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
RETURN 1


