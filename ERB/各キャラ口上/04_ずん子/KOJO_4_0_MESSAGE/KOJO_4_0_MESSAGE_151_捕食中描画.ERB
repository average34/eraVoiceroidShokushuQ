; □ 汎用口上
@KOJO_4_0_SYSTEM_MESSAGE_151(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM CSV_MAX_HP
	; 催眠中
	IF KOJO_4_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF
	
	; 通常時
	; 残り生命力(最大HP)で分岐
	CSVBASE 4,0
	CSV_MAX_HP = RESULT
	; 残り生命力66%以上
	IF MAXBASE:0 > (CSV_MAX_HP / 100 * 66)
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"（犯されはじめて……　どのくらい、たったんでしょうか……♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（わたし……きっと全身、精液の臭い……　染み付いちゃってます……♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（何回出せば……　気が済むのでしょうか……♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（私のナカ、こいつのおちんちんの形に……されちゃって……ます……♥）\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"（わたし　ここで　死ぬんでしょうか………）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（私が孕むまで…ううん、孕んでも犯し続ける気がでしょうか…）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（今は耐えないと……　誰かが、助けてくれるかもしれないから……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（抗い続けたら早く壊れちゃうから……ある程度、受け入れないと……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（心を、無に……身を任せて、体力を温存して……それで……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（どんどん……　ちから……　うばわれて……る……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（わたしが……　消えていく……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（あたま……　はたらかなく……　なって…ます…）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（いつまで……続くのでしょうか……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（あたたかい……）\n"
		ENDSELECT
		
	; 残り生命力33%以上
	ELSEIF MAXBASE:0 > (CSV_MAX_HP / 100 * 33)
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"（気持ちいい……♥　頭のなかまで、ぐちゃぐちゃで……思考が、纏まらないですっ♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「おﾞっ♥　おﾞまんこっ、使っていただいてっ♥　ありがとうござい……まﾞすっ♥　おごぉっ♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「イﾞっちゃう……っ♥　またイﾞっぢゃいﾞまﾞ……っ♥　あﾞあﾞあﾞああああぁっ♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（あはは……♥　わた、し、完全に魔物様の、孕み袋、に………♥）\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"（たす…けて…♥　のうみそぜんぶ……　きもちいいこと……だけに……♥　なり……かけてっ……♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「おなかっ……ぁ♥　もう、いっぱい…だからぁっ♥　出さない……で……あぁぁああﾞあﾞあﾞあﾞ♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（ぜんしん……快楽、の……暴力に晒され…て……もう……意識を保つ……ことも……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「とけ……る…っ♥　わたしの、意識、が……♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（おね…がい…　だれか……　だれか、たす……け……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（わたし……からだ…も……こころも……おかしく、されちゃったんですね……）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（これは、夢………次に目が覚めたら、きっと……）\n"
		ENDSELECT
		
	; 残り生命力0%以上
	ELSE
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「きへる、きへますぅぅぅぅぅ！　はいってぎてる！！\n　子宮の奥の奥まであづいのだくざんんんんんん！！ッ♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「だしてっ♥♥　もっとだしてくださいっ♥♥♥\n　わたしが壊れるぐらい中を満たしてえええええぇぇええ♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「じぬっ♥♥　イキすぎて死んじゃうううぅぅぅっ♥♥♥\n　ひぎゃっ♥♥♥　あああぁぁあああﾞあﾞあﾞあﾞあﾞあﾞあﾞあﾞ――――――――――――ｯｯｯ♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「足りないっ♥♥♥　足りないのっ♥♥　壊れていいからっ♥♥♥\n　快楽で私を満たしてくださいいいぃぃぃ♥♥♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「あだまがっ♥　わたじのあだまがもうつかいものにならにゃっひぎぁっ♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「あﾞーーーっ♥♥　あﾞあﾞーーーっ♥♥\n　ああああぁぁぁああﾞあﾞあﾞあﾞあﾞあﾞｯ―――――――――――♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「のうみぞっ♥　のうみぞがアグメずる事じか考えられなっびぎあおおおおぉぉぉっ♥\n　だず――げ――――――♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「イグッ♥　イグイグイグゥうぅぅぅ♥♥♥♥\n　イグの、止まらな――――びぎひぃひぃぃっっぃぃぃっっ♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「こわれっ♥♥　ごわれぢゃうがらっ♥♥♥\n　少し、すこしだけやすま―――――――んほおおおぉぉぉおおおっっ～～ッ♥♥♥」\n"
		ENDSELECT
	ENDIF
RETURN 1


