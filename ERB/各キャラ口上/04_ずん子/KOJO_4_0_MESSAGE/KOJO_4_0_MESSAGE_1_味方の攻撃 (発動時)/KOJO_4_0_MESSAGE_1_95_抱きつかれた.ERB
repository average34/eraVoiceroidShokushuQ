
; ------------------------------------------------------------------------------
; ■ 1 味方の攻撃 (発動時) - 95 抱きつかれた
; ------------------------------------------------------------------------------

; □ 0 抱きつかれたまま
@KOJO_4_0_SYSTEM_MESSAGE_1_95_X_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 睡眠時・喪失時・諦め
	SIF CFLAG:25 || CFLAG:29 || TFLAG:16 == 2
		RETURN 0
	
	; 催眠中
	IF KOJO_4_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; 催眠深度によって変化
			SELECTCASE CFLAG:43
			CASE 1
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「どうですか？　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%、気持ちいいです？」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%……っ！　離れてください……っ！）\n"
				ENDSELECT
				CALL KOJO_4_0_PARAGRAPH_MESSAGE
				CALL KOJO_4_0_ADD_MESSAGE, @"\d催眠に掛かった%CALLNAME:TARGET%の身体は、思考とは裏腹に%CALLNAME:(ARG:2)%への奉仕を続けている…\d\n"
				CALL KOJO_4_0_FIX_MESSAGE
				
				; 相手にペニスありなら追加口上
				IF GET_PENIS(ARG:2)
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"「あっ♥　あっ♥　あっ♥　気持ちいい……っ♥　気持ちいいです……っ♥」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「もっと……っ、もっと犯してぇ……っ　奧まで突いてぇ……っ」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%……っ　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%のおちんちんが、わたしの中に……）\n"
					ENDSELECT
					CALL KOJO_4_0_PARAGRAPH_MESSAGE
					CALL KOJO_4_0_ADD_MESSAGE, @"\d催眠に掛かった%CALLNAME:TARGET%の身体は、思考とは裏腹に%CALLNAME:(ARG:2)%のペニスへ奉仕を続けている…\d\n"
					CALL KOJO_4_0_FIX_MESSAGE
				ENDIF
				
			CASE 2
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"（わたしの身体……　性欲処理に使われてます……）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%……っ♥　興奮しすぎです……っ♥♥）\n"
				ENDSELECT
				
				; 相手にペニスありなら追加口上
				IF GET_PENIS(ARG:2)
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%、興奮したちんぽはわたしのおまんこに一杯射精しないとだめですよ」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"（精液……ッ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%の精液がわたしの中に……っ♥）\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「おちんぽ勃起、治まるまでは付き合ってあげますよ　溜まってるの全部膣内に射精してくださいっ」\n"
					ENDSELECT
				ENDIF
				
			CASEELSE
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"（わたしは性処理奴隷……　わたしは性処理奴隷……）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「好きなだけ……　わたしの身体で……　発散、してくださいね……」\n"
				ENDSELECT
				
				; 相手にペニスありなら追加口上
				IF GET_PENIS(ARG:2)
					SELECTCASE KOJO_4_0_KOJO_MODE
					CASEELSE
						CALL KOJO_4_0_ADD_MESSAGE, @"（わたしはみんなの孕み袋……　わたしはみんなの孕み袋……）\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「仲間のおちんぽを気持ちよくするのも……　大事なお仕事です……」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「いくらでも……ご奉仕します……から……　中に、おねがいします……」\n"
					ENDSELECT
				ENDIF
			ENDSELECT
			RETURN 1
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_4_0_KOJO_MODE
	CASE "淫乱"
		CALL KOJO_4_0_ADD_MESSAGE, @"「あっ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%、積極的ですねっ♥♥」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%との営み♥　敵さんに見せつけましょう♥♥」\n"
	CASE "発情"
		CALL KOJO_4_0_ADD_MESSAGE, @"「やめっ、%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%っ……　そんな事されたらわたし……」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%っ！　今は戦闘中ですよっ！」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"（んっ……　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%、興奮しすぎて周りが見えてませんね……っ……）\n"
	CASE "強気"
		CALL KOJO_4_0_ADD_MESSAGE, @"「こんな時に何を考えてるんですか！　そういうのは今じゃありませんっ！」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"「いくら興奮してるとはいえ、時と場所をわきまえてくださいっ！」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"「くぅ……っ……！　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%、後でおしおきしますからね……！」\n"
	CASEELSE
		CALL KOJO_4_0_ADD_MESSAGE, @"「今こんな事してる場合じゃありませんよ！？　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%！　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%っ！！」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%っ！　離して！　離してください！！」\n"
		CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%っ！　正気に戻って…！！」\n"
	ENDSELECT
	
	; 相手のペニス有無で追加口上
	; ペニスあり
	IF GET_PENIS(ARG:2)
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「んぁっ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%のおちんぽ気持ちいいっ♥　もっと奧まで突いてくださいっ♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%におまんこっ、掻き回されてるっ♥　魔物ににみられながら、本気セックスしてますっ♥♥」\n"
			
			; 相手のペニス素質(竿の長さ)
			SELECTCASE TALENT:(ARG:2):170
			; 短小
			CASE -1
				CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%……♥　ちっちゃいチンポで夢中で腰振っちゃって可愛い…♥♥」\n"
			; 爆根
			CASE 2
				CALL KOJO_4_0_ADD_MESSAGE, @"「あが……っ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%……のッ、チンポぉっ♥　おおきスギ……てっ♥　おなか……ぼこぉ…って…ッ♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「この怒張……ごっ♥　おおき…すぎ、て…！　子宮ごと、潰されちゃって…　ひぎぃっ！　…ますっ…♥♥」\n"
			; 超巨根
			CASE 3
				CALL KOJO_4_0_ADD_MESSAGE, @"「お……あ…っ！、%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%…の……チンポ…！　おおき、すぎ…！♥♥」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「お、おなかが！　おなか…が…孕まされたみたいに…ぼこぉって……♥　あが……おごっ…♥♥」\n"
			ENDSELECT
			
		CASE "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"（%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%のおちんちんが……っ♥　わたしの中をかき回して……っ♥♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「抜いてっ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%っ♥　おちんちん抜いてっ♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「うはぁ…っ♥　はあぁ……っ♥　はあ…っッッ♥♥　おまんこ、ずぽずぽしたら…ダメ…っ♥♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「くぅぅっ！　抜いてっ！　抜いてくださいっ！　こんな事してる場合…じゃぁ！　ないん、ですっ！」\n"
		ENDSELECT
	
	; ペニスなし
	ELSE
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「あはっ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%、レズセックス上手なんですね♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%は、女の子同士でこういう事するの好きなんですね♥」\n"
		CASE "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"（おまた同士……がっ、こすられて…っ♥）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（ああっ♥　%KOJO_4_0_CALLNAME_FROM_ID(ARG:2)%のクリと……っ、わたしのクリ……っ♥　こすれ……っ♥♥）\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「離して……くださいっ！　女の子同士だし、今は、戦闘中です！」\n"
		ENDSELECT
	ENDIF
RETURN 1
