		ENDSELECT
		
	; 射精中毒 Lv2
	ELSEIF ABL:12 == 2
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「焦らさないでっ♥　いっぱい出したいからっ♥　もっと強く♥♥」\n"
		CASE "発情"
