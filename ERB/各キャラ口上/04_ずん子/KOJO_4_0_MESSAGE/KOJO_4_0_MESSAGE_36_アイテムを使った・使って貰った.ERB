; □ 67 包帯
@KOJO_4_0_SYSTEM_MESSAGE_36_67(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM TEMP

	; 自分以外のキャラにアイテムを使って貰った場合は何も喋らずに帰る
	SIF ARG:2 != 0
		RETURN 0
		
	; 口上呼び出し前にCP回復が発生するので回復後の数値を考えて
	TEMP = BASE:CP * 100 / MAXBASE:CP
	
	; 服:新品
	IF TEMP >= 100
	; 服:通常
	ELSEIF TEMP > 66
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
