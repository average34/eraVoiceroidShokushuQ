; ■ 36 アイテムを使った・使って貰った
; ------------------------------------------------------------------------------

; □ 54 着替えセット
@KOJO_4_0_SYSTEM_MESSAGE_36_54(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 前の服状態による口上
	; 裸だった
	IF KOJO_4_0_PREV_BASE:3 == 0
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「服を着ると少し安心できますね」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「裸は恥ずかしいですからね」\n"
		ENDSELECT
		
	; 新品だった
	ELSEIF KOJO_4_0_PREV_BASE:3 == MAXBASE:3
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "強気"
			CALL KOJO_4_0_ADD_MESSAGE, @"「んー♪よし、頑張りますか！」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「よし、新しい服を着て心機一転です」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ちょっともったいなかったかもしれません」\n"
		ENDSELECT
	
	; 破れていた
	ELSE
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「結構気になりますからね」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ほんの少しの傷で大事に至ることだってありますから」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「…どうしてぴったりなんでしょう」\n"
		ENDSELECT
	ENDIF
	
	; 服装による追加口上
	SELECTCASE CFLAG:501
	; 00:いつもの服
	CASE 0
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「やっぱりこの服が一番動きやすいですね♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「どうしてわたしの服が落ちているのでしょうか…　何かの罠…？」\n"
		ENDSELECT
	; 01:普通の服
	CASE 1
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「私に普通の服は似合わないと思いますが…」\n"
		ENDSELECT
	; 02:体操着
	CASE 2
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「この服、結構動きやすいんですよね」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「この服を着てると周りの視線が少し気になるんですけども」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「動きやすいのは良いですが、攻撃はあまり防げそうにありませんね」\n"
		ENDSELECT
	; 03:メイド服
	CASE 3
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「おかえりなさいませご主人様！なんちゃって」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「こういうフリフリな服も良いですね」\n"
		ENDSELECT
	; 04:パジャマ
	CASE 4
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「暖かいお布団で眠りたくなります」\n"
		ENDSELECT
	; 05:巫女服
	CASE 5
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「私の普段着にそっくりです」\n"
		ENDSELECT
	; 06:スクール水着
	CASE 6
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「えっと、ここで着るのは流石に場違いかなーって…」\n"
		ENDSELECT
	; 07:マイクロビキニ
	CASE 7
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ほ、ほとんど丸見え…♥　この状態で魔物に襲われたら…♥♥」\n"
		CASE "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"「な、何も着てないよりはマシ…でしょうか」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「こ、これは服と言えないのでは」\n"
		ENDSELECT
	; 08:ボディスーツ
	CASE 8
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「むむむ…こういう服って体のラインが見えちゃって嫌なんですけど…」\n"
		ENDSELECT
	; 09:セーラー服
	CASE 9
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「私の学校の制服…？どうしてここに…？」\n"
		ENDSELECT
	; 10:半ズボン
	CASE 10
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「ちょっとわがままを言うなら、キュロットが良かったですね」\n"
		ENDSELECT
	; 11:妊婦服
	CASE 11
		; 妊娠・寄生されている
		IF CFLAG:36 >= 3 || (CFLAG:22 && CFLAG:36 == 0)
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「マタニティドレス…今の私には丁度良いのはその通りなんですが…」\n"
			ENDSELECT
			
		; 通常状態
		ELSE
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「ここでこれを着ると、孕ませて欲しいって公言してるようなものなのでは…」\n"
			ENDSELECT
		ENDIF
	; 12:ランジェリードレス
	CASE 12
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「こんな服着てたら、誘ってるって思われちゃいます♥」\n"
		CASE "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"「うぅ…　この服、透け透けです…っ…」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「この服で、魔物を誘い出す囮になれというんですか…？」\n"
		ENDSELECT
	; 13:セーター
	CASE 13
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「もこもこしててあったかいです♪」\n"
		ENDSELECT
	; 14:チャイナドレス
	CASE 14
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「ほわちゃー！っと危ない。激しく動いたら見えちゃいます」\n"
		ENDSELECT
	; 15:貞操帯
	CASE 15
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「むぅ…　しばらくの間気持ちよくなるのは難しくなりますね…」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「こんなのを付ける日が来るとは思いませんでした…\n　でも、仕方ないですよね。安全第一で行きましょう…」\n"
		ENDSELECT
	; 16:ビキニアーマー
	CASE 16
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「こんな重いの着たら、魔物に簡単に捕まっちゃう…♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「お、重い…ですね　防御力は期待できると思いますが…」\n"
		ENDSELECT
	; 17:園児服
	CASE 17
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ふふっ♥　いけないお店のコスプレみたい♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「つまりこういう服の子を犯すのが大好きな魔物が居る、ということですね♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「こ、こう極端な子供服を着るのはちょっと、ううん。かなり恥ずかしいですね…」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（……し、知り合いに会いませんようにっ！）\n"
		ENDSELECT
	; 18:エプロン
	CASE 18
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「もしかして炊事中にこの迷宮に落ちてしまった方がいらっしゃるのでしょうか」\n"
		ENDSELECT
	; 19:ローブ
	CASE 19
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「魔法は使えませんが、気分だけは魔法使いです♪」\n"
		ENDSELECT
	; 20:ウェディングドレス
	CASE 20
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「触手のお嫁さんになれ、ということでしょうか♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「この純白のドレスが白い液体でいやらしくコーティングされちゃう未来が見えます…♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「タコ姉さまが面白がって着せようとして来たことは有りますが…」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「将来は恐らく白無垢を着ると思うので、今だけ、ですね」\n"
		ENDSELECT
	; 21:ふんどし
	CASE 21
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「履きなれては居ますけども」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「服は、何処かにありませんか…？」\n"
		ENDSELECT
	; 22:触手服
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「ひゃぁ！？…　これ、本当に服なんですか！？」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ん…　ぬるぬるって、動いてます……」\n"
		ENDSELECT
	; 23:スリングショット
	CASE 23
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「これ、お股が強く押さえつけられて…んっ♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「あの…これって服ではなく紐なのでは…？」\n"
		ENDSELECT
	ENDSELECT
	
	; 過去に触手スーツを経験済みなら追加口上あり
	; ただし触手服選択時は除く
	IF GETBIT(CFLAG:900, 2) && CFLAG:501 != 22
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱"
			CALL KOJO_4_0_ADD_MESSAGE, @"「少し、あの触手で出来た服を期待したんですが」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「またあのぬるぬるした服、着てみたいですね…♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「この服は普通の服なんでしょうか？　暫くしたら動き始めたりしませんか…？」\n"
		CASE "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"（よかったです…　この服は大丈夫なようですね…）\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（今回は普通の服でありますように…）\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「ふぅ…これは普通の服だったみたいです」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「もう変な服は嫌です…」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「この服は…　大丈夫ですよね…？」\n"
		ENDSELECT
	ENDIF
RETURN 1


