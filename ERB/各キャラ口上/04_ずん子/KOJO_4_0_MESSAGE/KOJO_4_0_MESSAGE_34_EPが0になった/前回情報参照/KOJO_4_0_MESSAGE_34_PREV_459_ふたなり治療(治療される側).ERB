
; ------------------------------------------------------------------------------
; ■ 34 EPが0になった - 前回情報参照 459 ふたなり治療(治療される側)
; ------------------------------------------------------------------------------

@KOJO_4_0_SYSTEM_MESSAGE_34_PREV_459(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 優先口上指定
	CALL KOJO_4_0_SET_IMPORTANT_FLAG()
	
	; 追加地の文関数(前)を登録
	CALL KOJO_4_0_REGIST_DESCRIPTION_BEFORE("KOJO_4_0_SYSTEM_MESSAGE_34_DESCRIPTION_BEFORE")
	
	; 治療中の連続絶頂でペニス外れちゃった
	IF !GET_PENIS(TARGET)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d%CALLNAME:TARGET%のクリトリスは寄生された時の感覚が消えず、射精するペニスの如く激しく脈打っている！\d\n"
		RETURN 1
	ENDIF

	; 治療方法によって分岐
	; 足
	IF (KOJO_4_0_GET_PREV_ARG(0) == 144 && KOJO_4_0_GET_PREV_ARG(1) == 10) || (KOJO_4_0_GET_PREV_ARG(0) == 465 && KOJO_4_0_GET_PREV_ARG(1) == 0)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d足蹴にされ\d"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d汚物のように扱われ\d"
	
	; 口
	ELSEIF (KOJO_4_0_GET_PREV_ARG(0) == 459 && KOJO_4_0_GET_PREV_ARG(1) == 3) || (KOJO_4_0_GET_PREV_ARG(0) == 465 && KOJO_4_0_GET_PREV_ARG(1) == 2)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d淫らに蠢く舌に舐め取られ\d"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d口内に柔らかく包まれ\d"
		
	; 手
	ELSEIF (KOJO_4_0_GET_PREV_ARG(0) == 459 && KOJO_4_0_GET_PREV_ARG(1) == 4) || (KOJO_4_0_GET_PREV_ARG(0) == 465 && KOJO_4_0_GET_PREV_ARG(1) == 4)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d撫でさすられ\d"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d淫らに上下する手によって\d"
		
	; V
	ELSEIF (KOJO_4_0_GET_PREV_ARG(0) == 459 && KOJO_4_0_GET_PREV_ARG(1) == 0) || (KOJO_4_0_GET_PREV_ARG(0) == 465 && KOJO_4_0_GET_PREV_ARG(1) == 6)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d蜜壺にくわえ込まれ\d"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d強制的な子作りによって\d"
		
	; B
	ELSEIF (KOJO_4_0_GET_PREV_ARG(0) == 459 && KOJO_4_0_GET_PREV_ARG(1) == 1)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d柔らかな胸で扱き上げられ\d"
		CALL KOJO_4_0_ADD_MESSAGE, @"\d興奮し膨れ上がった乳頭で刺激され\d"
		
	; A
	ELSEIF (KOJO_4_0_GET_PREV_ARG(0) == 459 && KOJO_4_0_GET_PREV_ARG(1) == 2)
		CALL KOJO_4_0_ADD_MESSAGE, @"\d淫らに蕩けた尻穴にくわえ込まれ\d"
		CALL KOJO_4_0_ADD_MESSAGE, @"\dアヌスをほじくりながら\d"
	ENDIF
	CALL KOJO_4_0_PARAGRAPH_MESSAGE
	
	CALL KOJO_4_0_ADD_MESSAGE, @"\d先走りを溢れさせた\d"
	CALL KOJO_4_0_ADD_MESSAGE, @"\dガチガチに勃起した\d"
	CALL KOJO_4_0_ADD_MESSAGE, @"\d淫臭をまき散らす\d"
	CALL KOJO_4_0_PARAGRAPH_MESSAGE
	
	CALL KOJO_4_0_ADD_MESSAGE, @"\d%CALLNAME:TARGET%のペニスは、\d"
	CALL KOJO_4_0_PARAGRAPH_MESSAGE
	
	CALL KOJO_4_0_ADD_MESSAGE, @"\d射精する事だけを考えるよう思考を支配していく\d"
	CALL KOJO_4_0_ADD_MESSAGE, @"\d無様に脈打ちながら絶頂へと登り詰めていく！\d"
	CALL KOJO_4_0_ADD_MESSAGE, @"\d快感に負け今にも爆発しそうだ！\d"
	CALL KOJO_4_0_ADD_MESSAGE, @"\d雄と雌両方の快感を脳に刻み込んでいく\d"
	CALL KOJO_4_0_ADD_MESSAGE, @"\d全身を射精寸前の快感で満たしていく！\d"
	CALL KOJO_4_0_PARAGRAPH_MESSAGE
	
	CALL KOJO_4_0_ADD_MESSAGE, @"\n"
RETURN 1
