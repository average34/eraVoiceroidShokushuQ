				CALL KOJO_4_0_ADD_MESSAGE, @"「ふーっ♥　ふーっ♥　おまっ…んこ…♥　赤ちゃんのっ……お部屋ぁッッ♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	CASE 2 TO 3
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「あそこが……あついです……っ♥　寸止めで、おかし、く……♥♥」\n"
		ENDSELECT
		
	CASE 4 TO 5
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「もうガマンできないんですっ♥♥　イキたいっ♥♥　トロトロのおまんこでイキたいんですっ♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"（イかせてっ♥　イかせてくださいいっ♥♥　おまん……こ……でっ……イか、せてええぇぇっッッ♥♥♥）\n"
		ENDSELECT
		
	CASE 6 TO 8
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"「ぐっちゃぐちゃのおマンコっッッっ……でっ、無様イキっしてみせますッ……から……ぁっ♥♥　もう、もう寸止めやめてくださいぃぃぃっ♥♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「あはははッ♥♥　まだおまんこイかせてもらえないいっッ♥♥♥　狂っちゃあぁぁぅぅッッ♥♥♥♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「ぐちゃぐちゃオマンコおぉぉッ♥♥　イカせてえぇぇぇっ♥　イカへぇてくださぃぃっ……♥♥」\n"
			CALL KOJO_4_0_ADD_MESSAGE, @"「んひいいぃぃっ♥♥　イカせてぇっ♥♥　エロまんこ……っ、もっともっと虐めてぇえぇぇっッ♥♥」\n"
		ENDSELECT
	
	CASEELSE
		SELECTCASE KOJO_4_0_KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_4_0_ADD_MESSAGE, @"「あﾞぁﾞあﾞあﾞっ♥♥　気持ちいいぃぃィぃっ♥♥\n　変態堕落マンコぉ……っ♥♥　もう、もうイイイイイッちゃイイイィィィそううなのぉッ♥♥♥」\n"
		CASEELSE
			CALL KOJO_4_0_ADD_MESSAGE, @"「イカせ、てえぇぇぇっ♥　イカへぇて、くださいぃぃっ……♥♥　\nもう、おまんこ壊されても……かまわないか……らぁッッっ♥♥」\n"
