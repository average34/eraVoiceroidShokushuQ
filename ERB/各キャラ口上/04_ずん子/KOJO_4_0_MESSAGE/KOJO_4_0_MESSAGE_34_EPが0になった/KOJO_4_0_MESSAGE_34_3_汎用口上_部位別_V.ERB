@KOJO_4_0_SYSTEM_MESSAGE_34_3(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 追加地の文関数(前)を登録
	CALL KOJO_4_0_REGIST_DESCRIPTION_BEFORE("KOJO_4_0_SYSTEM_MESSAGE_34_DESCRIPTION_BEFORE")
	
	; 睡眠 or 喪失時は喋らずに帰る
	SIF CFLAG:25 || CFLAG:29
		RETURN 0
		
	; 催眠中
	IF KOJO_4_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_4_0_ADD_MESSAGE, @"「イキ……ます……　おまたの穴に、気持ちいいのが……　きます……」\n"
			CASE 1
				CALL KOJO_4_0_ADD_MESSAGE, @"「イキます……　おまんこイキます……　イキ……っ　」\n"
			CASE 2
				CALL KOJO_4_0_ADD_MESSAGE, @"「あっ……　あっ……　イキます……　交尾穴、絶頂します……」\n"
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「本気汁もらしながら、交尾穴イキします……」\n"
				CALL KOJO_4_0_ADD_MESSAGE, @"「これから精液便所変態女は絶頂します……　無様おまんこ、イキまくります……」\n"
				
				; NEXTコマンド 押し倒され なら追加口上
