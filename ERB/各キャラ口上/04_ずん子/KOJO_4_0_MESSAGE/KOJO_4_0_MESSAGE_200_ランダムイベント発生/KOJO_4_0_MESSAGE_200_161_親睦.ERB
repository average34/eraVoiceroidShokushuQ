
; ------------------------------------------------------------------------------
; ■ 200 ランダムイベント発生 - 161
; ------------------------------------------------------------------------------

; □ 事前準備
@KOJO_4_0_SYSTEM_MESSAGE_200_161(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 催眠中
	IF KOJO_4_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF
	
	IF CFLAG:(FLAG:10):28 >= 1 && CFLAG:(FLAG:11):28 >= 1 && CFLAG:1:28 >= 1 
		CFLAG:951 = FLAG:11
	ELSEIF CFLAG:(FLAG:10):28 <= 0 && CFLAG:(FLAG:11):28 >= 1
		CFLAG:951 = FLAG:11
	ELSEIF CFLAG:(FLAG:10):28 >= 1 && CFLAG:(FLAG:11):28 <= 0
		CFLAG:951 = FLAG:10
	ELSEIF CFLAG:(FLAG:11):28 >= 1 && CFLAG:(FLAG:10):28 >= 1 && CFLAG:1:28 <= 0
		CFLAG:951 = FLAG:11
	ENDIF
	
	IF NO:1 != 4
		IF ARG:2 == 1 && (ARG:3 == 0 || ARG:3 == 2)
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「この迷宮の淫気に当てられたら、仕方ないですよね…♥\n"
				CALL KOJO_4_0_PARAGRAPH_MESSAGE
				CALL KOJO_4_0_ADD_MESSAGE, @"　%KOJO_4_0_CALLNAME_FROM_ID(1)%が可愛すぎるのがいけないんですよ…♥」\n"
			ENDSELECT
		ELSEIF ARG:2 == 1 && ARG:3 == 1
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「残念、もうちょっとだったんですが…♥」\n"
			ENDSELECT
		ELSEIF ARG:2 == 0 && ARG:3 == 2
			SELECTCASE KOJO_4_0_KOJO_MODE
			CASEELSE
				CALL KOJO_4_0_ADD_MESSAGE, @"「…確かに一人でしてくださいって言いましたが…本当にするんですね」\n"
			ENDSELECT
		ENDIF
	ELSE
		
		; □ 発情無し+引きはがした
		IF ARG:2 == 0 && ARG:3 == 2
			; 通常時
			; 好感度：それほど高くない
			IF CFLAG:(CFLAG:951):5 < 400 && CFLAG:951 != 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「この迷宮の淫気に当てられたら、仕方ないですよね…♥」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「くっ…！　次に襲って来たら、覚悟してください…っ！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「わたしは…っ、%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%のオナニー道具ではありません！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「少しは遠慮してくださいっ！」\n"
				ENDSELECT
				
			; 好感度：普通(or 犯人不明)
			ELSEIF CFLAG:(CFLAG:951):5 < 800 || CFLAG:951 == 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"（…私を襲ってしまうくらい溜まってたんでしょうか…？）\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「…もっと、ムードを考えないとダメですよ」\n"
				ENDSELECT
			; 好感度：高い
			ELSE
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「あ、えっと…%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%。　そ、その気持ちはとっても嬉しいんですが…\n　こ、こういうのは一時の感情任せじゃなくて、もっと、こう…」\n\dずん子は顔を赤くして乙女チックな事を呟いているが、%CALLNAME:(CFLAG:951)%は自慰に集中しているようだ…\d\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（…どうして私、少し残念に思ってるんでしょうか）\n"
				ENDSELECT
			ENDIF
			
		; □ 発情無し+救出された
		ELSEIF ARG:2 == 0 && ARG:3 == 1
			; 通常時
			; 好感度：それほど高くない
			IF CFLAG:(CFLAG:951):5 < 400 && CFLAG:951 != 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「この迷宮の淫気に当てられたら、仕方ないですよね…♥」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「くっ…！　次に襲って来たら、覚悟してください…っ！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「わたしは…っ、%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%のオナニー道具ではありません！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「少しは遠慮してくださいっ！」\n"
				ENDSELECT
				
			; 好感度：普通(or 犯人不明)
			ELSEIF CFLAG:(CFLAG:951):5 < 800 || CFLAG:951 == 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「仲間を襲ってしまうくらい溜まってたんでしょうか…？」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「た、助かりました…」\n"
				ENDSELECT
			; 好感度：高い
			ELSE
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「あ、えっと…%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%なら…別にそのまま襲ってくれても、良かったんですが……」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"（…どうして私、少し残念に思ってるんでしょうか）\n"
				ENDSELECT
			ENDIF
			
		; □ 発情無し+身を委ねた
		; □ 発情あり+身を委ねた
		ELSEIF (ARG:2 == 0 && ARG:3 == 0) || (ARG:2 == 1 && ARG:3 == 0) 
		
			; 通常時
			; 好感度：それほど高くない
			IF CFLAG:(CFLAG:951):5 < 400 && CFLAG:951 != 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「付き合ってあげますので…っ♥　わたしも楽しませてくださいっ♥♥」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「この迷宮の淫気に当てられたら、仕方ないですよね…♥」\n"
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「ぐぅっ…！　仲間同士で、どうして…こんなコトを…っ！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"（…っ！　気持ちよさそうな顔しながら犯して、ますね…っ）\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「わたしは…っ、%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%のオナニー道具なんかじゃ…っ！」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「もうやめてください…！　おねがい…、おねがいですから…！」\n"
					IF GET_PENIS(CFLAG:951)
						CALL KOJO_4_0_ADD_MESSAGE, @"「もう射精さないでぇ…　わたしの…なか…　破裂しちゃう…っ…」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「少しは遠慮してください…っ！　…ひっ！？　また中でおっきくなって…っ！」\n"
					ENDIF
				ENDSELECT
				
			; 好感度：普通(or 犯人不明)
			ELSEIF CFLAG:(CFLAG:951):5 < 800 || CFLAG:951 == 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「仲間を襲ってしまうくらい溜まってたんですか？\n　ほら…いいですよ…♥」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「ああっぁああ～っ♥　またぁ…♥　またイ……っクうぅっ♥」\n"
					IF GET_PENIS(CFLAG:951)
						CALL KOJO_4_0_ADD_MESSAGE, @"「仲間を襲ってしまうくらい溜まってたんですか？\n　ほら…いいですよ…♥　もっと奥まで突き込んで、精液…びゅーびゅーしましょっ♥」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「ああっぁああ～っ♥　またぁ…♥　またイ……っクうぅっ♥\n　おちんぽぉ…されすぎ…てぇっ♥　おまんこ、壊れちゃう……っ♥♥」\n"
					ENDIF
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「ガマンできないなら…　仕方ないと思いますが…」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「すこし…やすませて……ください…　イキっぱなしは…キツ…ぃ…っ…」\n"
				ENDSELECT
			; 好感度：高い
			ELSE
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%なら…良いですよ…♥\n　もっと、全てを忘れさせてくれるぐらい、激しくしても…♥」\n"
					IF GET_PENIS(CFLAG:951)
						CALL KOJO_4_0_ADD_MESSAGE, @"「中に出されるの…すきっ♥　…っあっ…またナカにぃ♥　どぷどぷ…ってぇ…♥\n　ぃいい…です…っ♥　射精されながら…イクのぉ…♥　すごい…い…ですっ♥♥」\n"
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%のちんぽぉ♥　ビクビクってぇ…あばれてます…♥\n　あふれちゃうくらい、たくさん膣内射精してくださいねっ♥」\n"
					ENDIF
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「ひぐぅっ♥　また…　またイカされて…♥」\n"
					IF GET_PENIS(CFLAG:951)
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%…のっ、おちんちん…　きもちよさそうに…　ヒクヒクして…ますね……♥」\n"
					ENDIF
				ENDSELECT
			ENDIF
			
		; □ 発情あり+合意
		ELSEIF  ARG:2 == 1 && ARG:3 == 2
			
			; 好感度：普通(or 犯人不明)
			IF CFLAG:(CFLAG:951):5 < 800 || CFLAG:951 == 0
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASE "淫乱"
					CALL KOJO_4_0_ADD_MESSAGE, @"「付き合ってあげますので…っ♥　わたしも楽しませてくださいっ♥♥」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「この迷宮の淫気に当てられたら、仕方ないですよね…♥」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「仲間を襲ってしまうくらい溜まってたんですか？\n　ほら…いいですよ…♥」\n"
					IF GET_PENIS(CFLAG:951)
						CALL KOJO_4_0_ADD_MESSAGE, @"「仲間を襲ってしまうくらい溜まってたんですか？\n　ほら…いいですよ…♥　もっと奥まで突き込んで、精液…びゅーびゅーしましょっ♥」\n"
					ENDIF
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「ガマンできないなら…　仕方ないと思いますので…」\n"
				ENDSELECT
			; 好感度：高い
			ELSE
				SELECTCASE KOJO_4_0_KOJO_MODE
				CASEELSE
					CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%なら…良いですよ…♥\n　もっと、全てを忘れさせてくれるぐらい、激しくしても…♥」\n"
					CALL KOJO_4_0_ADD_MESSAGE, @"「優しく…してくださいね…♥」\n"
					IF GET_PENIS(CFLAG:951)
						CALL KOJO_4_0_ADD_MESSAGE, @"「%KOJO_4_0_CALLNAME_FROM_ID(CFLAG:951)%…のっ、おちんちん…　きもちよさそうに…　ヒクヒクして…ますね……♥」\n"
					ENDIF
				ENDSELECT
			ENDIF
			
		ENDIF
	
	ENDIF
	
RETURN 1
