
; ------------------------------------------------------------------------------
; ■ 118 自慰メッセージ
; ------------------------------------------------------------------------------

; □ 汎用口上
; ◆ 汎用口上
@KOJO_8_0_SYSTEM_MESSAGE_118(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM NAME_ID

	; 睡眠中
	IF CFLAG:25
		CALL KOJO_8_0_ADD_MESSAGE, @"「……♥　……っ♥　……んぁっ♥　……あぅんっ♥」\n"
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		
		CALL KOJO_8_0_ADD_MESSAGE, @"\dどうやら%CALLNAME:TARGET%は、夢の中でも自慰をしているらしい…\d\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"\d眠ったまま自慰をする%CALLNAME:TARGET%の手の動きが激しくなっていく…\d\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"\d眠ったまま自慰をする%CALLNAME:TARGET%は、甘い声をあげるだけで目を覚まさない…\d\n"
		RETURN 1
	ENDIF
	
	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; 寸止め or 自慰誘発中
			IF CFLAG:820
				; 寸止めカウンタの数値で分岐
				SELECTCASE CFLAG:820
				; 1回目の寸止めは誘発を含むので注意
				CASE 1
					CALL KOJO_8_0_ADD_MESSAGE, @"「頭の中が、自慰をする事でいっぱいになっています……　自慰をします……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「自慰をします……ので……　絶頂させて、ください……」\n"
				CASE 2 TO 3
					CALL KOJO_8_0_ADD_MESSAGE, @"「イカせて……ください……　イカせ……てっ……　くださ……い……っ……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「寸止め……されて……　オナニー、とまらなく……　なってます……」\n"
				CASEELSE
					CALL KOJO_8_0_ADD_MESSAGE, @"「このままイケないと……っ♥　%CALLNAME:TARGET%は……っ♥　オナニーの事だけを考える……変態小学生に、なります……っ♥」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「はへ……っ♥　はへ……っ♥　はへぇ……っ……♥♥」\n"
					CALL KOJO_8_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_8_0_ADD_MESSAGE, @"\d思考と身体を操られているにも関わらず、%CALLNAME:TARGET%の声は繰り返される寸止めに蕩けきっている…\d\n"
				ENDSELECT
				
			; 通常時
			ELSE
				SELECTCASE CFLAG:43
				CASE 1
					CALL KOJO_8_0_ADD_MESSAGE, @"（あれ？　わたし、なんでオナニーしてるんだっけ……？　目の前に敵が……いるのに……？）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"（やだっ！？　わたしオナニーしてるっ！？　なんでぇっ！？）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"（手が勝手にっ！？　やだぁっ！　とまって！　とまってよぉっ！！）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"（身体……操られてるっ！？　こんなの、いやあぁっ！？）\n"
				CASE 2
					CALL KOJO_8_0_ADD_MESSAGE, @"（このピンチを乗り切るには　オナニーして……何とかしないと……）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"（本気オナニーでイキまくれば、なんとかなるかも……）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「ふふん♪　あなたなんか怖くないです！　わたしオナニー得意なんだから！」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「こっち見なさい！　オナニーでイキまくるとこ見せたげる！」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「ほっ、ほらぁっ！　こっちはオナニーしてんのよ！　観念しなさい！」\n"
					CALL KOJO_8_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠で思考を書き換えられた%CALLNAME:TARGET%は、敵を倒すためには自慰をするしかないと思い込んでいる…\d\n"
				CASEELSE
					CALL KOJO_8_0_ADD_MESSAGE, @"（オナニーしなきゃ……　本気でオナニーして、はしたなく絶頂しなきゃ……）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「見られながらのオナニー　すごい……興奮します……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「オナニーすごいです……きもちいいです……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「%CALLNAME:TARGET%は、ところかまわずオナニーする……　変態です……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「興奮したら……　いつでも……　犯してください……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"（いやらしく……いじくるトコ……見てもらわないと……）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「変態マゾ小学生は……　今からオナニーさせていただきます……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「……いまから……オナニーします……、じっくり……御覧ください……」\n"
					CALL KOJO_8_0_PARAGRAPH_MESSAGE
					
					CALL KOJO_8_0_ADD_MESSAGE, @"\dうつろな瞳を宙に向け涎を滴らせながら、%CALLNAME:TARGET%は義務的に快感を与え続けている\d\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"\d肉便器として犯して貰う為、%CALLNAME:TARGET%は媚びた声をあげながら自慰を見せつけた\d\n"
				ENDSELECT
			ENDIF
			RETURN 1
		ENDSELECT
	ENDIF

	; 汎用口上
	; 寸止め or 自慰誘発中
	IF CFLAG:820
		; 寸止めカウンタの数値で分岐
		SELECTCASE CFLAG:820
		; 1回目の寸止めは誘発を含むので注意
		CASE 1
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_8_0_ADD_MESSAGE, @"「もうガマンできないっ♥　今すぐ気持ちよくなりたいぃ……っ♥　んあぁ……ぁっ♥♥」\n"
			CASE "発情"
				CALL KOJO_8_0_ADD_MESSAGE, @"（イヤなの……にぃっ！　頭ん中、オナニーすることで一杯になって……るぅっ！？）\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"（手ぇ……っ、勝手にぃ……っ！　止まって♥　止まってくださいよぉっ♥♥）\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"（なんで……　おなに……ぃっ……　とまらないですっ！？）\n（これじゃまるで……　痴女じゃないですか……っ）\n"
			ENDSELECT
		CASE 2 TO 3
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_8_0_ADD_MESSAGE, @"「わたしのっ……　あたまぁ……♥　おかしくなってるぅっ♥\n　ぐちゅぐちゅに……　とけちゃってぇ……♥　ぐちゅぐちゅに……　してぇ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「イクのぉ♥　もっといっぱいイクのぉっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「あっっっっああっああっあっっーー♥\n　ひとりえっち好きぃぃい……ぃ♥　ぎぼぢいぃぃいいっ♥♥」\n"
			CASE "発情"
				CALL KOJO_8_0_ADD_MESSAGE, @"「オナニーとまんないっ！　オナニーとまんないよぉっ！　やだぁっ♥　あっ♥　あっあっ♥\n　イクっ♥♥　イッちゃうっ♥♥　イクイクイクイクうっ♥♥　イッ――――！？」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「いいかげん……にっ……　イカせてくださいよぉ……っ……　寸止め、辛いですうぅ……っ……」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"（寸止め……されてぇ……っ……　カラダ……変になって……っ……　イキたいっ……　イキたいよぉっ……）\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「ぅあﾞ～っ♥　ぅあﾞあﾞ～～っ……っ♥\n　カラダあっつく……てぇ……ガマンできない……のぉっ♥♥\n　イカないと頭おかしくなるぅ♥　狂っちゃうっよ……ぉ♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「やだ、やだ、やだぁ……♥　だれか……とめてよぉっ……♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「だめぇ♥　だめなのにぃっ♥　気持ちいいのとまんないっ♥♥　とめらんないっっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「だめぇっ……　だめなのにぃ……　とまんないのっ♥　がまんできないのぉっ♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「おなにいっ……！　おなにいい……ぃっ……！　がまん……できないよぉぉっ♥\n　イキたいっ……♥　イキたいのぉっ♥♥」\n"
			ENDSELECT
		CASEELSE
			SELECTCASE KOJO_MODE
			CASE "淫乱", "発情"
				CALL KOJO_8_0_ADD_MESSAGE, @"「おなにーとまんないのぉ……♥\n　やだぁ……♥　わたし、ばかになっちゃったぁ……♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「もうイカせてえぇっ♥　狂っちゃうっ♥　ぐるっぢゃうよぉぉっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「ゆるひ……っ、もうゆるひへええぇっ♥♥　イケないの……っ♥　くるひいのおぉっ♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「イキたいっ♥　イキたいのぉ♥♥　もうっ、いじわる……やめへぇぇっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「こんにゃにっ……オナニーしてるんだからぁぁ……♥　いいかげんイカせてよぉ……♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	; 通常時
	ELSE
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「おなにぃっ♥　おにゃにぃ……っ♥　しゅきぃっ♥　これしゅきぃっ♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「おっ……へぇ……っ♥　ぎぼぢいぃっ♥　これぎぼぢいぃっ♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「はっ♥　あへ……ぁぁ♥　うぁぁっ♥♥　あっ♥あっ♥あっ♥　あああぁぁっっ♥♥♥」\n"
			CALL KOJO_8_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_8_0_ADD_MESSAGE, @"\d頭の中が自慰をする事で埋め尽くされ、%CALLNAME:TARGET%は完全に周りが見えなくなっている…\d\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"\d自慰の快感に取り付かれた%CALLNAME:TARGET%は、オスを誘うフェロモンと淫らな声を撒き散らしている…\d\n"
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"（ちょっとだけ……　ちょっとだけさわれば、多分おさまる……からぁ……）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（だ……めぇっ、だめなのにぃっ……！）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「ふあぁ……♥　ううっ……っんっ♥　……っ♥」\n（気持よくなんてない……これは火照りを鎮めるだけなん……だからぁ）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（クセになっちゃう！　ひとりえっち、クセになっちゃうよぉっ！）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（カラダ……おかしくされちゃった……から……　わたしのせいじゃ……ないしぃ♥）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（現実でいっぱい犯されてるのに……種付けされる妄想とまんないっ……！）\n（もっとほしい、気持ちいいのもっとほしいっ♥）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（こんな気持ちいいオナニー知らないっ！？）\n（戻れなくなっちゃう！）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（気持ちよくなることしか……考えられない……）\n（このままじゃぁ、ところかまわずオナニーする変態になっちゃうよぉ♥）\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「なんとか……コレっ……しずめないとぉっ……！」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「こうしないとっ……頭バカになっちゃいそう……だからあっ……♥\n　いっぱ……いっ……気持ちよく……しないとっ♥♥」\n"
		ENDSELECT
		CALL KOJO_8_0_FIX_MESSAGE
	ENDIF
	
	; 戦闘中追加口上
	IF TFLAG:13
		; 寸止め or 自慰誘発中
		IF CFLAG:820
			; 寸止めカウンタの数値で分岐
			SELECTCASE CFLAG:820
			; 1回目の寸止めは誘発を含むので注意
			CASE 1
				SELECTCASE KOJO_MODE
				CASE "発情"
				CASEELSE
					CALL KOJO_8_0_ADD_MESSAGE, @"（目の前に……敵がいる……のにっ！？　自慰なんてっ、後にすればいい……のにぃっ！？）\n"
				ENDSELECT
			CASE 2 TO 3
				SELECTCASE KOJO_MODE
				CASEELSE
				ENDSELECT
			CASEELSE
				SELECTCASE KOJO_MODE
				CASEELSE
				ENDSELECT
			ENDSELECT
			
		; 通常時
		ELSE
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_8_0_ADD_MESSAGE, @"「見られながらのオナニーって、いつもより……っ♥　きもちいいからぁ、すきなのぉ……っ♥♥」\n"
			CASE "発情"
				CALL KOJO_8_0_ADD_MESSAGE, @"（やだ……ぁっ……　わたしのオナニー、見られちゃう……っ……）\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"（今のわたし……バカになってるっ、オナニーバカになってるぅ……♥）\n（たたかわなきゃ……いけない……のにぃ……♥）\n（手ぇ……とまんない……です♥♥）\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「……あっ……はっ……んああぁっ♥\n　こっち見る……なっ……♥　んくうぅっ……♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「だめ……♥　なのにぃっ……♥\n　こんなこと……してるっ場合じゃ……ないのにぃっ♥♥」\n"
			ENDSELECT
			CALL KOJO_8_0_FIX_MESSAGE
			
			; ターン終了時自慰発生なら追加口上
			IF CFLAG:39 || KOJO_8_0_GET_PREV_PURE_ARG(0) == 48
				SELECTCASE KOJO_MODE
				CASE "淫乱"
					CALL KOJO_8_0_ADD_MESSAGE, @"「あっ♥　あっあっ♥♥　イッちゃうっ♥　こんなのすぐイッちゃうっ♥♥」\n"
				CASE "発情"
					CALL KOJO_8_0_ADD_MESSAGE, @"（イキたい……っ♥　いますぐイキたいぃ……っ♥♥）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「うぁ……ぁ……♥　ひとりえっち、ガマン……できないぃ……っ♥♥」\n"
				CASEELSE
					CALL KOJO_8_0_ADD_MESSAGE, @"（一回だけっ……　一回だけイケれば、ちゃんと戦えるからぁ……っ）\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「ムリぃ……っ……　オナニー我慢……ムリぃ……っ……」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「ダメ……ぇ……　ガマン、できない……っ……」\n"
				ENDSELECT
			ENDIF
			CALL KOJO_8_0_FIX_MESSAGE
			
			; まだ仲間が行動中なら追加口上
			IF (TARGET != 1 && BASE:1:1 > 0) || (FLAG:10 && TARGET != FLAG:10 && BASE:(FLAG:10):1 > 0) || (FLAG:11 && TARGET != FLAG:11 && BASE:(FLAG:11):1 > 0)
				; まだ行動中キャラのIDを取得
				VARSET NAME_ID
				WHILE 1
					SELECTCASE RAND:3
					CASE 0
						IF BASE:1:1 > 0 && TARGET != 1
							NAME_ID = 1
						ENDIF
					CASE 1
						IF FLAG:10 && BASE:(FLAG:10):1 > 0 && TARGET != FLAG:10
							NAME_ID = FLAG:10
						ENDIF
					CASE 2
						IF FLAG:11 && BASE:(FLAG:11):1 > 0 && TARGET != FLAG:11
							NAME_ID = FLAG:11
						ENDIF
					ENDSELECT
					
					SIF NAME_ID
						BREAK
				WEND
				
				; 好感度によって分岐
				; 好感度:低
				IF CFLAG:NAME_ID:5 < 0
					SELECTCASE KOJO_MODE
					CASE "淫乱"
						CALL KOJO_8_0_ADD_MESSAGE, @"「あーっ♥　気持ちいいぃっ♥♥　ちょっと%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%、こっち見ないでよぉ♥♥」\n"
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っ、ヒドイ……っ♥♥　ゴミを見る目でこっち見てる……っ♥♥」\n"
						CALL KOJO_8_0_ADD_MESSAGE, @"（ヒトをクズを見るみたいな目で見てぇ……♥♥　%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%なんかオカズにしちゃうんだから♥♥）\n"
					CASE "発情"
						CALL KOJO_8_0_ADD_MESSAGE, @"（%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%に気付かれないように……　うう、やりにくいよぉ……）\n"
					CASEELSE
						CALL KOJO_8_0_ADD_MESSAGE, @"（%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%には、こんなトコ見られないようにしないと……）\n"
					ENDSELECT
					
				; 好感度:警戒
				ELSEIF CFLAG:NAME_ID:5 < 50
					SELECTCASE KOJO_MODE
					CASE "淫乱"
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っ……そんな目で見ないで……よぉっ♥」\n"
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%がこっち見てるぅ♥　コーフンしちゃったんだから仕方ないでしょぉ♥♥」\n"
					CASE "発情"
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っ、そんな軽蔑するみたいな目で見ないでぇ……　だめぇ、とまんないのぉ……」\n"
					CASEELSE
						CALL KOJO_8_0_ADD_MESSAGE, @"（見ないで……　%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%、ゴミみたいに見ないで……）\n"
					ENDSELECT
					
				; 好感度:通常
				ELSEIF CFLAG:NAME_ID:5 < 200
					SELECTCASE KOJO_MODE
					CASE "淫乱"
						CALL KOJO_8_0_ADD_MESSAGE, @"「仲間に見られてオナニー♥　すごいっ♥　コーフンするぅっ♥♥」\n"
					CASEELSE
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っ、こっち見ないで……ぇ……　恥ずかしい……っ……」\n"
					ENDSELECT
					
				; 好感度:高
				ELSEIF CFLAG:NAME_ID:5 >= 200
					SELECTCASE KOJO_MODE
					CASE "淫乱"
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っ♥　とまんないっ♥　わたしオナニーとまんなくなってるぅ……っ♥♥」\n"
						CALL KOJO_8_0_ADD_MESSAGE, @"（%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%っ……♥　もっと見てぇ♥♥\n　今わたし、%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%の事考えながらシてるんだよぉ……っ♥♥）\n"
					CASEELSE
						CALL KOJO_8_0_ADD_MESSAGE, @"（%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っ♥　%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……っっ♥♥　ごめんねっ、わたし%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%の事考えながらぁ……っ♥♥）\n"
						CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%に見られてる……っ♥　恥ずかしいっ♥　気持ちいいっ♥　恥ずかしいっ♥♥　気持ちいいぃぃ……っっ♥♥」\n"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
RETURN 1


