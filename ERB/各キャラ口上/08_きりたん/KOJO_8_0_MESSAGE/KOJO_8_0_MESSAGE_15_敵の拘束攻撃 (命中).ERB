@KOJO_8_0_SYSTEM_MESSAGE_15_963(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 優先口上指定
	CALL KOJO_8_0_SET_IMPORTANT_FLAG()
	
	; 睡眠・喪失・諦め時
	SIF CFLAG:25 || CFLAG:29 || TFLAG:16 == 2
		RETURN 0
	
	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_8_0_ADD_MESSAGE, @"「ああっ、身体…操られてぇ♥　このままじゃエッチな事させられちゃうっ♥♥」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「ああっ…この感じ…♥　わたしの身体、操られてる♥♥」\n"
	CASE "発情"
		CALL KOJO_8_0_ADD_MESSAGE, @"「や…っ、やだぁっ…！？　身体が勝手に動く…っ！？」\n"
	CASEELSE
		CALL KOJO_8_0_ADD_MESSAGE, @"「なっ、何コレっ！？　身体が動かない…っ！？」\n"
	ENDSELECT
	
	; 地の文表示
	CALL KOJO_8_0_PARAGRAPH_MESSAGE
	CALL KOJO_8_0_ADD_MESSAGE, @"\M"
RETURN 1
