		ENDSELECT
	ENDIF
	
	; PTメンバーなしのソロプレイ追加口上
	IF GET_MEMBER_SUM() == 1
		SELECTCASE KOJO_MODE
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"（助けてくれる仲間も…いないのにっ…　わたし…触手におねだりなんて…してぇっ…）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（わたし…おねだりしてる…っ…　誰も見てないからって、ガマンできなくなってる…）\n"
