@KOJO_8_0_SYSTEM_MESSAGE_120_PREV_461_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 優先口上指定
	CALL KOJO_8_0_SET_IMPORTANT_FLAG()
	
