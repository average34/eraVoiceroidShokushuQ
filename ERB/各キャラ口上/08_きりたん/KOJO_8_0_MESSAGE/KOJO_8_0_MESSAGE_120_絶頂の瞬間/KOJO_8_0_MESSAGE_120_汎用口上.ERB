				CALL KOJO_8_0_ADD_MESSAGE, @"「わたし……っ……　イキました……ぁっ♥　イキ……顔っ……、見てください……ぃっ♥♥」\n"
				
				
				; 連続絶頂時 or 寸止め時 追加口上
				{
				IF (ARG:1 == 0 && GETBIT(ARG:3, 1)) ||
				   (ARG:1 == 1 && GETBIT(ARG:3, 2)) ||
				   (ARG:1 == 2 && GETBIT(ARG:3, 3)) ||
				   (ARG:1 == 3 && GETBIT(ARG:3, 4)) ||
				   (ARG:1 == 5 && GETBIT(ARG:3, 5)) ||
				   (ARG:1 == 9 && GETBIT(ARG:3, 6)) ||
				   CFLAG:820
				}
					CALL KOJO_8_0_ADD_MESSAGE, @"「はぁ……ぁあぁ……っッ♥♥　イキまくり……っ、ですうぅぅっ♥　あたま壊れちゃううぅぅぅぅッッっッ♥♥」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「んおﾞぉﾞぉﾞ……っﾞ♥♥　またっ、またイﾞグうぅぅっ♥　連続イキ、とまりましぇぇ……んんッっ♥♥」\n"
					CALL KOJO_8_0_ADD_MESSAGE, @"「またっ、イキましたぁぁ……っ♥　連続でっ、イキましたぁぁ……っ♥♥」\n"
				ENDIF
				CALL KOJO_8_0_PARAGRAPH_MESSAGE
				
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠状態の%CALLNAME:TARGET%は、肉便器としての悦びに震えながら絶頂を報告した\d\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠の力で肉便器だと思い込まされた%CALLNAME:TARGET%は、蕩けきった声で絶頂を報告した\d\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠肉便器へ堕ちた%CALLNAME:TARGET%は、淫らな絶頂顔を見せつけながら絶頂を報告した\d\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠肉便器へ堕ちた%CALLNAME:TARGET%は、心の底から嬉しそうに絶頂を報告した\d\n"
				
				; 連続絶頂時 or 寸止め時 追加口上
				{
				IF (ARG:1 == 0 && GETBIT(ARG:3, 1)) ||
				   (ARG:1 == 1 && GETBIT(ARG:3, 2)) ||
				   (ARG:1 == 2 && GETBIT(ARG:3, 3)) ||
				   (ARG:1 == 3 && GETBIT(ARG:3, 4)) ||
				   (ARG:1 == 5 && GETBIT(ARG:3, 5)) ||
