			ENDIF
			
			CALL KOJO_8_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_8_0_ADD_MESSAGE, @"\d執拗な責めに耐えきれず強制絶頂させられた%CALLNAME:TARGET%は、悔しそうに触手を睨み付けたまま身体を震わせている…\d\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"\d触手に絡め取られ強制絶頂させられた%CALLNAME:TARGET%の無抵抗な身体に、じっくりと牝の悦びが刻み込まれていく…\d\n"
		ENDSELECT
		RETURN 1
	ENDIF
	
	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
