					CALL KOJO_8_0_ADD_MESSAGE, @"「イッて……ますっ……　わたし……また……イッて……ます……っ」\n"
				ENDIF
				CALL KOJO_8_0_PARAGRAPH_MESSAGE
				
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠状態の%CALLNAME:TARGET%は、小さく身体を震わせながら事務的に絶頂を報告した\d\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠状態の%CALLNAME:TARGET%は、小さく身体を震わせながら呆けた顔で絶頂を報告した\d\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"\d催眠状態の%CALLNAME:TARGET%は、だらしなく涎をたらしたまま絶頂を報告した\d\n"
			
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「あたまの中ぁ……っ♥　全部きもちいいっ……♥　イッてますっ♥　わたし今イッてますぅ……っ♥♥」\n"
