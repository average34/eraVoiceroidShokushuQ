	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_8_0_ADD_MESSAGE, @"「もっと…ぉ♥　もっとぉ…っ♥♥」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「きもちい…っ♥　ぜんぶきもちいいぃっ…っ♥♥」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「ひゃぁ…っ♥　あああっ♥　あたまのなか…まっしろ…にぃっ…♥」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「イクのぉ…っ♥　イクのとまらなひのぉ♥　感じしゅぎ…るぅっ…♥」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「きもちよすぎてぇ♥　あたまのなかまで…っ、ぜんぶ…とけちゃうよぉ…♥」\n"
