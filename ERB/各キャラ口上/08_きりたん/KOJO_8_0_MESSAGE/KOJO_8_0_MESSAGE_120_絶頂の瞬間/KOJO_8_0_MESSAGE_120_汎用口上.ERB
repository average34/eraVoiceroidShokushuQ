
; ------------------------------------------------------------------------------
; ■ 120 絶頂の瞬間 - 汎用口上
; ------------------------------------------------------------------------------

@KOJO_8_0_SYSTEM_MESSAGE_120(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM  NAME_ID

	; 特殊優先口上を呼び出す
	CALL KOJO_8_0_SYSTEM_MESSAGE_120_PRIOR(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	SIF KOJO_8_0_GET_IMPORTANT_FLAG()
		RETURN 1

	; 攻撃種類別口上を呼び出す
	CALL KOJO_8_0_DEPLOY_MESSAGE_SINGLE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6, @"KOJO_8_0_SYSTEM_MESSAGE_120_COMMAND_{TFLAG:50}")
	SIF KOJO_8_0_GET_IMPORTANT_FLAG()
		RETURN 1
		
	; 上記呼び出しの個別口上は内部で追加地の文を呼び出し、口上登録まで完結している
	; こちらは汎用口上で利用する追加地の文、個別口上呼び出しより先に定義するとエラーを起こすので注意
	; 追加地の文関数(前)を登録
	CALL KOJO_8_0_REGIST_DESCRIPTION_BEFORE("KOJO_8_0_SYSTEM_MESSAGE_120_DESCRIPTION_BEFORE")
	
	; まだ行動中キャラのIDを取得
	; まだ仲間が行動中の場合のみ
	VARSET NAME_ID
	IF (BASE:1:1 > 0 && TARGET != 1) || (FLAG:10 && BASE:(FLAG:10):1 > 0 && TARGET != FLAG:10) || (FLAG:11 && BASE:(FLAG:11):1 > 0 && TARGET != FLAG:11)
		WHILE 1
			SELECTCASE RAND:3
			CASE 0
				IF BASE:1:1 > 0 && TARGET != 1
					NAME_ID = 1
				ENDIF
			CASE 1
				IF FLAG:10 && BASE:(FLAG:10):1 > 0 && TARGET != FLAG:10
					NAME_ID = FLAG:10
				ENDIF
			CASE 2
				IF FLAG:11 && BASE:(FLAG:11):1 > 0 && TARGET != FLAG:11
					NAME_ID = FLAG:11
				ENDIF
			ENDSELECT
			
			SIF NAME_ID
				BREAK
		WEND
	ENDIF
	
	; 睡眠中
	IF CFLAG:25
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「んぅ…っ♥　はぁ…んっ♥　あっ♥あっ♥あっ♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「ん、んは…っ…　あ、あぁ……っ」\n"
		ENDSELECT
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		
		CALL KOJO_8_0_ADD_MESSAGE, @"\d眠ったまま絶頂に達した%CALLNAME:TARGET%は、荒い息をつきながら身をよじらせ小さく震えている…\d\n"
		RETURN 1
	ENDIF

	; 戦意喪失中
	IF CFLAG:29
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「ひあﾞあﾞ…っ…♥　あﾞあﾞ…っ…♥　うぁぁっ……♥」\n"
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「うぁ…ぁっ…　ガマン、できな…いっ♥」\n"
		CASE "強気"
			CALL KOJO_8_0_ADD_MESSAGE, @"「や…だ…　きもち…　わる…」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「うあ…ぁ……」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「あっ…　あ…　ぅ…ぁ…」\n"
		ENDSELECT
		
		; 行動可能な仲間が居るなら追加口上(頻度が高すぎると違和感があるので確率を下げておく)
		IF NAME_ID && RAND:2
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%っ♥♥　わたしイッてるっ♥♥　イカされてるよぉ…っ♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「%KOJO_8_0_CALLNAME_FROM_ID(NAME_ID)%……　たすけ…っ……　たすけてぇ……」\n"
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF

	; 諦め済み
	IF TFLAG:16 == 2
		SELECTCASE KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「ひぁっ……♥　あああぁぁっ……♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「もっとぉ……♥　もっと気持ちいいのちょうだぁい♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「いくうぅっ♥　いっちゃううぅぅっ♥　あはは♥　わたし……こんなのでいってるうぅ♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「……うぁ………ぁ…………♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「あっ！あっ！あっ！きもちいいっ！きもちいいよぅ♥♥」\n"
			CALL KOJO_8_0_PARAGRAPH_MESSAGE
			
			CALL KOJO_8_0_ADD_MESSAGE, @"\dだらしなく歪んだ顔を見られるのも気にせず、%CALLNAME:TARGET%は肉欲の虜となっている…\d\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"\d一度の絶頂では物足りないのか、%CALLNAME:TARGET%は更なる快感を得ようと腰を振り続けている\d\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"\d涙と涎にまみれた顔で淫らな笑顔を浮かべながら、%CALLNAME:TARGET%はとめどない絶頂に震えている…\d\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"\d全てを諦めた%CALLNAME:TARGET%は、牝の顔でとろけるような快楽を貪っている…\d\n"

