		ENDSELECT
		
	; 射精中毒 Lv2
	ELSEIF ABL:12 == 2
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「焦らさないでぇ♥　ちんぽバキバキになってるからぁ♥　もっと強くぅ♥♥」\n"
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「もっと…ぉ…　つよくして…っ…　もどかしい、ですよぉ…っ♥」\n"
