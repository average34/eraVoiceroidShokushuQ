@KOJO_8_0_SYSTEM_MESSAGE_34_1(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 追加地の文関数(前)を登録
	CALL KOJO_8_0_REGIST_DESCRIPTION_BEFORE("KOJO_8_0_SYSTEM_MESSAGE_34_DESCRIPTION_BEFORE")
	
	; 睡眠 or 喪失時は喋らずに帰る
	SIF CFLAG:25 || CFLAG:29
		RETURN 0
		
	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	; 寸止め回数によって分岐
	; 1回目の寸止めは自慰誘発でも使っているので、絶頂寸止め以外の表現が必要
	SELECTCASE CFLAG:820
	CASE 0 TO 1
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			; B感覚 + 素質[淫乱] + 素質[淫乳] で分岐
			SELECTCASE (ABL:7 + TALENT:74 + TALENT:76)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「あーっ♥　あーっ♥　おっぱいっ、さきっぽ痺れてぇぇっ♥♥　気持ちいいイイいいっっ♥♥」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「あはっ♥　あははぁぁっ♥　えっち乳首イキたがってるぅぅっ♥　ビクビクしてるよぉぉっ♥♥」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「きたぁぁっ♥　気持ちいいのきたあぁぁっ♥♥　ドスケベ乳首こわれちゃうッ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「えへへぇぇ♥♥　イクぅぅっ♥♥　マゾおっぱいイクうぅぅっ♥♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「くるﾞぅっ♥　くるﾞうぅっッ♥　オﾞッパイアﾞグメぇぇっッ♥　んﾞあﾞあﾞぁぁぁぁぁっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「おﾞひぃっ♥♥　おﾞっぱ……っ♥♥　おﾞっぱいイグッ♥♥　おﾞっぱいイイぃグうぅッッ♥♥♥」\n"
			ENDSELECT

		CASE "発情"
			; B感覚 + 素質[淫乱] + 素質[淫乳] で分岐
			SELECTCASE (ABL:7 + TALENT:74 + TALENT:76)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「何これ……っ！？　おっぱいが、ジンジンしてぇ……！？」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「うぅぅ……っ……　おっぱいが……　おっぱいがヘンだよぉ……」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「ダメえぇぇぇっ！！　おっぱい……っ！！　おっぱいイッちゃうよおぉぉぉっ！！」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「あついいぃぃっ！！　おっぱいがあついよぉぉぉっ！！」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「ダメっ♥　もうダメぇぇっ♥　おっぱい熱いっ♥♥　熱いのおおぉぉっッ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「ごめんなさいっ♥　えっちな胸でごめんなさいっ♥　イカせるの許してえぇぇっ♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「くるっ♥　きちゃうぅ……っ♥　おっぱいイクっ♥　おっぱいイクうぅぅっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「イクっ♥　イクっ♥　イクううぅぅぅっ♥♥　許してぇぇっ♥♥　えっちなおっぱい許してぇぇっっ♥♥♥」\n"
			ENDSELECT
			
		CASE "強気"
			; B感覚 + 素質[淫乱] + 素質[淫乳] で分岐
			SELECTCASE (ABL:7 + TALENT:74 + TALENT:76)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「何で……！？　胸でこんな……！？」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「く……っ！？　胸がゾクゾクするぅ……っ！？」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「くそぉ……っ！　胸で気持ちよくなる……なんてぇ……っ！！」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「おっぱ……っ♥　おっぱい……っ♥　とけちゃうぅ……ぅぅっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「感じでないっ♥　胸で感じてなんか……気持ちよくなんかないぃっ♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「コレだめぇ……っ♥　止まんないっ……♥　おっぱい気持ちいいの止まんないッっっッ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「ふーっ♥　ふーっ♥　おっぱいっ……♥　おっぱいぃっ……ッッ♥♥」\n"
			ENDSELECT
			
		CASEELSE
			; B感覚 + 素質[淫乱] + 素質[淫乳] で分岐
			SELECTCASE (ABL:7 + TALENT:74 + TALENT:76)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「う、ウソでしょう……！？　胸なんかで、こんなに感じて――！？」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「やだぁぁぁ……！！　またおっぱいヘンになっちゃうよぉおぉっ……！！」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「ダメえぇぇっ♥　おっぱい……っ♥　どんどんエッチになっちゃううぅっ……っ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「キちゃうっ……！！　おっぱい気持ちよくなるの止まんないいぃっ……！！」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「クるうぅっ♥　またおっぱいに気持ちイイのクるうぅぅぅっ♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	CASE 2 TO 3
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「おっぱい……痺れてぇ……、気持ちいいだけになって……ぇ……♥　寸止めでバカになってるゥゥゥゥっ～♥♥」\n"
		ENDSELECT
		
	CASE 4 TO 5
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「もうガマンできないのぉっ♥♥　イキたいっ♥♥　おっぱいでイキたいですうぅぅっ♥♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（おっぱいでイキたいっ♥　おっぱいでイキたいっ♥♥　おっぱい……でえぇっ……イキたひいぃぃっッッ♥♥♥）\n"
		ENDSELECT
		
	CASE 6 TO 8
		SELECTCASE KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「スケベ乳首っッッっ……でっ、無様イキっしますッ……から……ぁっ♥♥　もう寸止めやめてぇぇぇっ♥♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「あはははッ♥♥　またおっぱい寸止めえぇェっ♥♥♥　脳みそがオぉかしくなるゥ♥♥♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「んひいいぃぃっ♥♥　イカせてぇっ♥♥　エロおっぱい……っ、もっと虐めてぇえぇぇっッ♥♥」\n"
		ENDSELECT
	
	CASEELSE
		SELECTCASE KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「イイぃぃっ♥♥　気持ちいいイイぃぃィぃっ♥♥\n　ガチエロ乳首……っ♥♥　もうすぐイイイイイッちゃイイイィィィそううなのぉッ♥♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「イカせてえぇぇぇっ♥　イカへぇてぇよぉぉっ……♥♥　\nもうイケればぁ、おっぱい壊されても……かまわないか……らぁあぁぁッッっ♥♥」\n"
		ENDSELECT
	ENDSELECT
RETURN 1
