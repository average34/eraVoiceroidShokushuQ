; □ 06 乳搾り(強制)
@KOJO_8_0_SYSTEM_MESSAGE_34_PREV_144_6(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 優先口上指定
	CALL KOJO_8_0_SET_IMPORTANT_FLAG()
	
	; 追加地の文関数(前)を登録
	CALL KOJO_8_0_REGIST_DESCRIPTION_BEFORE("KOJO_8_0_SYSTEM_MESSAGE_34_DESCRIPTION_BEFORE")
	
	; 母乳体質素質で分岐
	; [母乳体質]
	IF TALENT:130 == 1
		CALL KOJO_8_0_ADD_MESSAGE, @"\d搾乳される快感に耐えられず、%CALLNAME:TARGET%の乳頭は激しく母乳を噴き出した！\d\n"
		
	; [母乳異常分泌]
	ELSEIF TALENT:130 == 2
		CALL KOJO_8_0_ADD_MESSAGE, @"\d%CALLNAME:TARGET%は搾乳される快感に屈し、興奮しきった乳頭から飲みきれない程の母乳を滴らせた！\d\n"
		
	; [母乳製造装置]
	ELSEIF TALENT:130 == 3
		CALL KOJO_8_0_ADD_MESSAGE, @"\d執拗に絞られた%CALLNAME:TARGET%の胸は、匂い立つ母乳を射精するように幾度となく噴き出した！\d\n"
	ENDIF
RETURN 1


