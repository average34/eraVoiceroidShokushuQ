			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"（イクッ♥　イクイクイクううぅっ♥♥　おまんこ顔晒して、イっクうぅぅぅっっッ♥♥）\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「そっ、そこイイっ♥♥　コリコリ好きいぃっ♥♥　おまんこ奧コリコリ好きいぃぃぃぃっッ♥♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「えへへぇぇ♥♥　イクぅぅっ♥♥　ドスケベ牝ガキまんこイクうぅぅっ♥♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「ひいぃっ♥　んひいぃぃっッ♥　ちんっぽっ穴アﾞグメぇぇっッ♥　んﾞひあﾞぁぁぁぁぁっ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「おﾞひぃっ♥♥　おﾞまﾞんご……っ♥♥　おﾞまﾞんごイグッ♥♥　おﾞまﾞんごイイぃグうぅッッ♥♥♥」\n"
			ENDSELECT

		CASE "発情"
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「この感じ……っ！？　おまたの穴に、気持ちいいのクる……っ！？」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「うぅぅ……っ……　これ以上、おまたの穴……刺激された……らぁぁ……」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「あついいぃぃっ！！　あかちゃんの穴あついよぉぉぉっ！！　助けてえぇぇっ！！」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「イキたくないっ♥　おまんこイキたくないいぃぃっ♥♥　頭ばかになっちゃうよおぉぉ♥♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「おﾞっ♥　おﾞまﾞんごとけるうぅぅ……っ♥　もﾞうううぅだめぇぇっっっ……っッ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"「イクっ♥　イクっ♥　イクううぅぅぅっ♥♥\n　ごめんなさいっ♥♥　えっちなおまんこでごめんなさいいぃっ♥♥♥」\n"
			ENDSELECT
			
		CASE "強気"
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「あ……っ！？　アソコ……があぁ……っ！？」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「くうぅ……っ！　あつい……っ！　おまたあついぃ……っっ！！」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「感じでないっ♥　おマンコで感じてなんか……気持ちよくなんかないぃっ♥♥」\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「ふーっ♥　ふーっ♥　おマンコっ……♥　おマンコぉっ……ッッ♥♥」\n"
			ENDSELECT
			
		CASEELSE
			; V感覚 + 素質[淫乱] + 素質[淫壷] で分岐
			SELECTCASE (ABL:9 + TALENT:74 + TALENT:78)
			CASE 0
				CALL KOJO_8_0_ADD_MESSAGE, @"「うああぁぁっ……！？　アソコが……じんじんするうぅ……っ！？」\n"
			CASE 1
				CALL KOJO_8_0_ADD_MESSAGE, @"「やだぁぁぁ……！！　またアソコで気持ちよくなっちゃうよぉおぉっ……！！」\n"
			CASE 2
				CALL KOJO_8_0_ADD_MESSAGE, @"「ダメえぇぇっ♥　おまた……っ♥　どんどんエッチになっちゃううぅっ……っ♥♥」\n"
				CALL KOJO_8_0_ADD_MESSAGE, @"（おまんこイッちゃう……♥　またおまんこイッちゃううぅぅ……っッ♥♥）\n"
			CASEELSE
				CALL KOJO_8_0_ADD_MESSAGE, @"「クるうぅっ♥　またおまんこに気持ちイイのクるうぅぅぅっ♥♥」\n"
			ENDSELECT
		ENDSELECT
		
	CASE 2 TO 3
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「おマンコ……あついよぉぉ……♥　寸止めでバカになってるゥゥゥゥっ～♥♥」\n"
		ENDSELECT
		
	CASE 4 TO 5
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「もうガマンできないのぉっ♥♥　イキたいっ♥♥　おまんこでイキたいですうぅぅっ♥♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"（おまんこでイキたいっ♥　おまんこでイキたいっ♥♥　おまんこ……でえぇっ……イキたひいぃぃっッッ♥♥♥）\n"
		ENDSELECT
		
	CASE 6 TO 8
		SELECTCASE KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「エロマンコっッッっ……でっ、無様イキっしますッ……から……ぁっ♥♥　もう寸止めやめてぇぇぇっ♥♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「あはははッ♥♥　まだおまんこイケないいっッ♥♥♥　脳みそ狂ちゃあぁぁぅぅッッ♥♥♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「ぐちゃぐちゃオマンコおぉぉッ♥♥　イカせてえぇぇぇっ♥　イカへぇてぇよぉぉっ……♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「んひいいぃぃっ♥♥　イカせてぇっ♥♥　エロまんこ……っ、もっと虐めてぇえぇぇっッ♥♥」\n"
		ENDSELECT
	
	CASEELSE
		SELECTCASE KOJO_MODE
		CASE "淫乱", "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「イイぃぃっ♥♥　気持ちいいイイぃぃィぃっ♥♥\n　変態ガキマンコぉ……っ♥♥　もうすぐイイイイイッちゃイイイィィィそううなのぉッ♥♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「イカせてえぇぇぇっ♥　イカへぇてぇよぉぉっ……♥♥　\nもうイケればぁ、おまんこ壊されても……かまわないか……らぁあぁぁッッっ♥♥」\n"
		ENDSELECT
	ENDSELECT
RETURN 1
