
; ------------------------------------------------------------------------------
; ■ 25 敵の拘束攻撃 (回避)
; ------------------------------------------------------------------------------

; □ 汎用口上
@KOJO_8_0_SYSTEM_MESSAGE_25(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_8_0_ADD_MESSAGE, @"「捕まえたいなら、もっとねっとりと絡みつかなきゃね♪」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「その程度のテクじゃわたしを捕まえるなんて無理無理♪」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「わたしを捕まえたいならもっとがんばって♥」\n"
	CASE "発情"
		CALL KOJO_8_0_ADD_MESSAGE, @"「よ、避けられた……」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「あ、危なかった……」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「あんなのに捕まったら……」\n"
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		CALL KOJO_8_0_ADD_MESSAGE, @"\d%ARGS:1%の攻撃を回避できた事に、%CALLNAME:TARGET%は火照る身体を抱きしめながら安堵の声を漏らした…\d\n"
	CASE "強気"
		CALL KOJO_8_0_ADD_MESSAGE, @"「ふふん♪　そんな大振りじゃわたしを捕まえるなんて無理ですね♪」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「わたしを捕まえようなんて数万年早いですよ！」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「小学生一人捕まえられないとか、才能ないんじゃないですかぁ？」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「遅い♪　遅い、おそ～い♪」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「ふっふ～ん♪　華麗に回避です♪」\n"
	CASEELSE
		CALL KOJO_8_0_ADD_MESSAGE, @"「残念でした、逃げるのは得意なんですよ」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「あっぶな……！」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「ふぅ、危ない危ない……」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「あんなのに捕まるとか、ぞっとしないです……」\n"
	ENDSELECT
	CALL KOJO_8_0_FIX_MESSAGE
	
	; 服を破って拘束を回避時追加口上
	IF BASE:3 != PREV_BASE:3
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「捕まえる前に服を破るのって、えっちだぁ♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「捕まえるフリをして、服を破るのが目的ですか？　やらし～んだぁ♪」\n"
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「服がなければ今頃……」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「た、助かったけど……　服が……」\n"
		CASE "強気"
			CALL KOJO_8_0_ADD_MESSAGE, @"「もう！　服は貴重品なのに！」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「破れた服の分、痛い目にあってもらいますよ！」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「わたしの服は高いんですよ？」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「服がなければ危なかったですね……」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「ああもう破けちゃったじゃないですか…　捕まるよりマシかな…」\n"
		ENDSELECT
	ENDIF
RETURN 1


