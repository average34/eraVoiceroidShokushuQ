
; ------------------------------------------------------------------------------
; ■ 441 触手スーツ常駐時
; ------------------------------------------------------------------------------

; □ 01 A陵辱開始
@KOJO_8_0_SYSTEM_MESSAGE_441_1(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	; A感度LVで分岐
	SELECTCASE TALENT:101
	CASE 0
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「わたし、そっちの趣味は無いんですよねえ」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「やあぁっ、入ってこない…でぇっ！？」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「このバカ服！　何してくれてんのよ！！」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「おっ、おしりっ…　気持ち悪いっ！？」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「うぁ…っ！？　おしり…いじられてる…っ！？」\n"
		ENDSELECT
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		CALL KOJO_8_0_ADD_MESSAGE, @"\dあまりの気色悪さに服を引き剥がそうとした瞬間、%CALLNAME:TARGET%の腕は別の触手に捕らえられた！\d\n"
		
	CASE 1
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「おしりより、おまんこイジメてほしいなぁ♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「もう、おしり責めとかマニアックな服ねえ♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"（服の下で、おしりの穴拡げられてる…っ……）\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「ううっ、ムズムズするよぉ……」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「何でおしりなんていじるんですかぁ……　うう、気持ち悪いなあ……」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「この変態服…ぅ、おしりいじるの…やめてくださいよぉ……」\n"
		ENDSELECT
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		CALL KOJO_8_0_ADD_MESSAGE, @"\d急な感覚に%CALLNAME:TARGET%は服を押さえたが、それは蠢く触手を尻穴に押しつける形になった\d\n"
		
	CASE 2
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「ああっ♥　おしりで気持ちよくなるの、スキになってきちゃった…かもぉ♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「やめてくださいよぉ……　そっちで気持ちよくなるのイヤですぅ……」\n"
		ENDSELECT
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		CALL KOJO_8_0_ADD_MESSAGE, @"\d性感帯として開発された尻穴を舐めほじられると、%CALLNAME:TARGET%は甘い声で喘ぎ始めた…\d\n"
		
	; A感度LV3以上は地の文が待ちかねたに変化
	CASEELSE
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「はっ♥　あっ♥　あひぃ…っ♥　おひっ…りがぁ……っ♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「こわひて…ぇっ♥　あらひのおひり…こわひてぇ…っ♥♥」\n"
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「うしろの穴っ♥　ほじほじ…やめてぇ…っ♥♥」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「おひり…ッ♥　いじっちゃ、らめなの…おッ♥♥」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「んﾞひ…っ♪　おﾞっ…おﾞひりぃ…っ♪　きたぁ…っ♪」\n"
		ENDSELECT
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		CALL KOJO_8_0_ADD_MESSAGE, @"\d快感にほぐれきった尻穴を嬲られるたび、%CALLNAME:TARGET%の表情が蕩けていく…\d\n"
	ENDSELECT
RETURN 1


; □ 12 セルフ膣内射精
@KOJO_8_0_SYSTEM_MESSAGE_441_12(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 催眠中
	IF KOJO_8_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_8_0_ADD_MESSAGE, @"「にゃかだしっ♥　にゃかだしとまんにゃいっ♥♥　じぶんのせーしで、きもちよく…なるぅっ♥♥」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「自分の精液でぇ…♥　おなかのなか、たぷたぷして…るぅ…♥\n　もっと射精させてぇ…♥　おちんぽびゅびゅーって、射精するのぉ♥♥」\n"
	CASE "発情"
		CALL KOJO_8_0_ADD_MESSAGE, @"「わたしぃっ…　自分に…っ　膣内射精…してるぅ…！？\n　ひぃ…っ…！　だめぇ…ぇっ…！　とまんないよぉ…ぉぉっ！？」\n"
	CASEELSE
		CALL KOJO_8_0_ADD_MESSAGE, @"「これぇッ、まさかぁ！？　わたしの膣内にぃ…っ　わたしの精子流し込まれ…てぇぇっ！？」\n"
		CALL KOJO_8_0_ADD_MESSAGE, @"「いやああぁぁっ！　入ってくるぅっ！？　わたしの精子入ってくるううぅっっ！？」\n"
	ENDSELECT
	
	; 妊娠していない or 妊娠中(判明前)なら追加口上
	IF !CFLAG:36 || CFLAG:36 == 2
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_8_0_ADD_MESSAGE, @"「妊娠っ♥　自分の精液でぇ、妊娠しちゃうよぉぉっ…♥」\n"
		CASE "発情"
			CALL KOJO_8_0_ADD_MESSAGE, @"「だめぇっ！　ナカにぃっ、ナカに出すのやめてえぇぇっ！　自分の精液で、赤ちゃん出来ちゃうぅぅっ！？」\n"
		CASEELSE
			CALL KOJO_8_0_ADD_MESSAGE, @"「冗談でしょぉっ！？　自分と、子作りなんてええぇっ！？」\n"
			CALL KOJO_8_0_ADD_MESSAGE, @"「出すなぁっ！　出すなあああっ！　自分の精液で赤ちゃん産むなんてヘンですよぉぉっ！？」\n"
		ENDSELECT
		CALL KOJO_8_0_PARAGRAPH_MESSAGE
		CALL KOJO_8_0_ADD_MESSAGE, @"\d自分自身への種付けを強要された%CALLNAME:TARGET%のペニスは、いつも以上に濃縮され煮詰まった精液を吐きだしている！\d\n"
	ENDIF
RETURN 1
