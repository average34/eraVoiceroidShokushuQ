;　東方触手宮　ゆかり口上

;口上基礎知識
;　口上を書く時は基本的に ARG:0 と ARG:1 の中に「シチュエーション」が入っているので、
;　それに応じて分岐を行い、適切なメッセージを表示する……というのが基本方針になります
;　
;　なお、TARGET に口上を表示するキャラが指定された状態になっているので、
;　自分の名前や自分のCFLAGを見たい時などは CALLNAME:TARGET とか CFLAG:1 とかで参照できます
;
;フラグ活用　ビット演算に１が開いてるのは単なる趣味、個人的に２の倍数で覚えたほうがわかりやすいので
;CFLAG:800　ビット演算　2 娘と初遭遇　4 孫と　8 ひ孫と
;CFLAG:801　戦闘回数
;CFLAG:802　ビット演算　2 出産経験　4 娘出産経験　8 幼体出産経験　16 寄生経験
;CFLAG:803　妊娠悦びフラグ
;CFLAG:805　口上非表示管理　1で探索時、2で出産時、3で自慰、4で戦闘開始、5でリロード二刀流
;CFLAG:806　連続絶頂管理　絶頂するたびに+1、他のところいくと０に
;CFLAG:898　ビット演算　2で迷宮内でマキに会った　15 あかり
;CFLAG:899　ビット演算　2 マキ死亡確認　15 あかり死亡確認
;CFLAG:900　ゲームオーバーになったのが自分の娘のせいかどうか
;CFLAG:901～989　アイテム習得管理　下二桁にアイテムの番号
;CFLAG:990　マキのふたなり治療を膣でした回数
;CFLAG:999　ビット演算　2 マキリタイア（発見未発見問わず）　15 あかり


;修正、加筆、転載、再配布はご自由に
;改変は元の内容をあまり削除しないレベルでお願いします

;口上選択時のタイトル
@KOJO_TITLE_3_0
RESULTS = ボイスロイド触手宮　ゆかり口上


;口上選択時の詳細説明
;フリーフォーマットです。ご自由にどうぞ
@KOJO_DETAIL_3_0
PRINTFORML 娘との会話や孕ませ系、ステータス変化による分岐に注力した口上。
PRINTFORML キャラは全般的にクール。微妙にツンデレ。
PRINTFORML なお、この口上を使用すると娘も喋ります。

;フクシアピンク　娘はカーネションピンク
@KOJO_DEFAULT_COLOR_3_0
IF CFLAG:104
	RETURN RGBCOLOR(220, 100, 240)
ELSE
	RETURN RGBCOLOR(200, 40, 220)
ENDIF

;口上本体
@KOJO_3_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

;孫以降をさよならする
IF CFLAG:104 >= 2
	RESETCOLOR
	RETURN 0
ENDIF
;いつか汎用口上ができたらこっちにチェンジする
;（孫以降と口上の性格に合わないものを除外）
;IF CFLAG:104 >= 2 || CFLAG:100 != 14 || CFLAG:100 != 12 || CFLAG:100 != 11 || CFLAG:100 != 8  || CFLAG:100 != 6 || CFLAG:100 != 5
;	RETURN 0
;ENDIF


;ここからコールゆかり娘までの順番は絶対に並び替えてはいけない！
;地の文は一部を除きデフォルトのものを使用します
;除くのはオープニング、エンディング、ゲームオーバー、裸で時間経過
SIF ARG:0 != 203 && ARG:0 != 206 && ARG:0 != 210 && ARG:0 != 157 && ARG:0 != 46
	CALL SYSTEM_MESSAGE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

;連続絶頂の管理
IF ARG:0 == 120 || ARG:0 == 122
	CFLAG:806 += 1
;EPダメージ・MP0になった・EPが0になった・自慰全般・自慰でイけなかった・発情が収まった・
;母乳を吸われる・射精関連・触手スーツ以外の時にフラグをウォッシュ（回数蓄積の無効化）
ELSEIF ARG:0 != 14 && ARG:0 != 33  && ARG:0 != 34 && ARG:0 != 118 && ARG:0 != 119 && ARG:0 != 121 && ARG:0 != 123 && ARG:0 != 440 && ARG:0 != 441 && ARG:0 != 442 && ARG:0 != 451 && ARG:0 != 452 && ARG:0 != 454 && ARG:0
	CFLAG:806 = 0
ENDIF
;戦意喪失は21以上149以下か160以上なら一部を除いて逃がす
;ここで逃がさないもの（＝除外してRETURN 1させないもの）
;戦闘中（20以下）MP0(33)服が破ける(35)ターン終了時自慰(48)自慰メッセージ(118)自慰で絶頂失敗(119)
;戦意喪失中に絶頂(122)母乳を吸われる(123)出産系イベント(127と129)喪失中に気付け薬を使われた(144)
;エンディング(204、206、208、210)
SIF CFLAG:29 && ARG:0 != 33 && ARG:0 != 48 && ARG:0 != 118 && ARG:0 != 119 && ARG:0 != 122 && ARG:0 != 123 && ARG:0 != 127 && ARG:0 != 129 && ARG:0 != 144 && ARG:0 != 202 && ARG:0 != 204 && ARG:0 != 206 && ARG:0 != 208 && ARG:0 != 210 && ((ARG:0 > 20 && ARG:0 < 150) || ARG:0 >= 160)
	RETURN 1


;娘は専用口上へ
IF CFLAG:104 == 1
	CALL KOJO_YUKARIdaughter(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	RETURN 1
ENDIF

;文字色設定
SETCOLOR CFLAG:700

;睡眠　ここに来るのはほぼ間違いなく戦闘中に何か食らった時だけだろうし……多分
;叩き起こされたときは除く
IF CFLAG:25 && ARG:0 != 41
	IF RAND:3 == 0
		PRINTFORMW 「ん……あむっ……」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「あぅ……」
	ELSE
		PRINTFORMW 「……ひくっ」
	ENDIF
	RESETCOLOR
	;口上記入済み
	RETURN 1
ENDIF

SELECTCASE ARG:0
	CASE 1
		CALL BATTLE_MIKATA_KOUDOU_HATUDOU_3_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	;HPダメージ
	CASE 12
		;妊娠中ランダム
		IF RAND:2 == 0 && CFLAG:36 >= 3
			PRINTFORMW 「お、おなかはダメですってっ……」
		;戦意喪失or諦めor深い催眠
		ELSEIF CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1
			PRINTFORMW 「あ……うぁ……」
		;MP1/4以下＆拘束中or催眠
		ELSEIF (MAXBASE:MP/4 >= BASE:MP && CFLAG:20) || CFLAG:43
			PRINTFORMW 「こ……これ以上は……！」
		;HP1/4以下or拘束中orMP1/4以下
		;HPはダメージ後の数値を見る
		ELSEIF (BASE:HP - ARG:2 < MAXBASE:HP/4) || CFLAG:20 ||  MAXBASE:MP/4 >= BASE:MP
			IF RAND:2 == 0
				PRINTFORMW 「避けないと……」
			ELSE
				PRINTFORMW 「くっ……まずい、ですね」
			ENDIF
		;その他
		ELSE
			IF RAND:2 == 0
				PRINTFORMW 「イタタっ！」
			ELSE
				PRINTFORMW 「このっ、これくらいで……」
			ENDIF
		ENDIF
	;EPダメージ
	CASE 14
		;戦意喪失or諦めor深い催眠でランダム
		IF RAND:3 == 0 && (CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1)
			;コウノトリ、フコウノトリの暗示系攻撃
			IF ARG:1 ==  387 || ARG:1 ==  388
				PRINTFORM 「ほしぃ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　みてるだけじゃイヤです、なかに白いのくださいっ
				CALL HEARTCB(0xD02830)
				PRINTFORMW 」
			ELSE
				PRINTFORMW 「んあぁ……も、もっと！　もっとしてください！」
			ENDIF
		;MP1/4以下or発情中or戦意喪失or諦めor深い催眠
		ELSEIF MAXBASE:MP/4 >= BASE:MP || CFLAG:28 || CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1
			IF RAND:2 == 0
				PRINTFORM 「ハァッ、すごい、すごいぃ……
				CALL HEARTCB(0xD02830)
				PRINTFORMW 」
			ELSE
				PRINTFORM 「ふあぁっ、いい、そこぉ、とろけちゃいますぅ……
				CALL HEARTCB(0xD02830)
				PRINTFORMW 」
			ENDIF
		;EPダメージ4500以上か浅い催眠ランダム
		ELSEIF RAND:3 == 0 && (ARG:3 >= 4500 || CFLAG:43)
			PRINTFORMW 「ひっ、あふぁっ！？　き……きてるぅ、なんかきてぇ……！」
		;EPダメージ1500以上か浅い催眠かEP3000未満
		ELSEIF ARG:3 >= 1500 || CFLAG:43 || BASE:EP < 3000
			;コウノトリ、フコウノトリの暗示系攻撃
			IF ARG:1 ==  387 || ARG:1 ==  388
				PRINTFORMW 「うわ、何てモノ見せてるんですか……！？」
			;EPダメージ3000以上か催眠
			ELSEIF RAND:2 == 0 && (ARG:3 >= 3000 || CFLAG:43)
				PRINTFORMW 「くぅ……こんなの、もぉ……」
			ELSE
				PRINTFORMW 「や、やだっ、はなれっ！？」
			ENDIF
		;コウノトリ、フコウノトリの暗示系攻撃
		ELSEIF ARG:1 ==  387 || ARG:1 ==  388
			PRINTFORMW 「……何見せてるんですか」
		;その他
		ELSE
			IF RAND:2 == 0
				PRINTFORMW 「この……気色悪いんですよ！」
			ELSE
				PRINTFORMW 「ぶっ飛ばされたいんですか？」
			ENDIF
		ENDIF
	;拘束攻撃
	CASE 15
		;戦意喪失or諦めor深い催眠
		IF CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1
			PRINTFORMW 「…………あは♪」
		;発情中or催眠
		ELSEIF CFLAG:28 || CFLAG:43
			PRINTFORMW （だ、だめ……わたし、期待しちゃって……！）
		ELSE
			PRINTFORMW 「しまっ……！？」
		ENDIF
	;寄生攻撃
	CASE 16
		;頻度的にはあんまり出ないのに明らかに力入れまくっているというね
		;初期は孕ませか寄生かで区分するつもりだったけど地の文が大して変わらんので諦める
;		IF ARG:2

		;コウノトリの空気妊娠
		IF ARG:1 == 842
			;妊娠出産経験済みかつ戦意喪失or諦めor深い催眠でランダム
			IF RAND:2 == 0 && (CFLAG:802 & 2) && (CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1)
				PRINTFORM 「はぁあっ、きてますぅ……
				CALL HEARTCB(0xD02830)
				PRINTFORM 　中に入ってくるの、わかるぅ……
				CALL HEARTCB(0xD02830)
				PRINTFORMW 」
			;妊娠出産経験済みか寄生出産経験済みか深い催眠か精液中毒2以上
			ELSEIF (CFLAG:802 & 2) || (CFLAG:802 & 16) || CFLAG:43 > 1 || ABL:14 >= 2
				;MP1/4以下or発情中or戦意喪失or諦めか妊娠悦びフラグ一定以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:28 || CFLAG:29 || TFLAG:16 == 2 || CFLAG:803 >= 3
					PRINTFORM 「だ、だめ……これ吸ったら、わたしぃ……
					CALL HEARTCB(0xD02830)
					PRINTFORMW 」
				ELSE
					PRINTFORMW 「気色悪いですよ……！」
				ENDIF
			ELSE
				;HP0orMP1/4以下or発情中or戦意喪失or諦め
				IF BASE:HP < 1 || MAXBASE:MP/4 >= BASE:MP || CFLAG:28 || CFLAG:29 || TFLAG:16 == 2
					PRINTFORMW 「うあっ……なに、これ……」
				ELSE
					PRINTFORMW 「うぅっ……よく分かりませんけど悪寒が……」
				ENDIF
			ENDIF
		ELSE
			;妊娠出産経験済みか寄生出産経験済みか深い催眠
			IF (CFLAG:802 & 2) || (CFLAG:802 & 16) || CFLAG:43 > 1
				;戦意喪失or諦めor深い催眠でランダム
				IF RAND:2 == 0 && (CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1)
					IF RAND:2 == 0
						PRINTFORMW 「しゅきぃ%UNICODE(0x2665)%　じゅぽじゅぽされながら出されるのしゅごくいぃですぅ……♪」
					ELSE
						IF CFLAG:22 && (CFLAG:36 == 0 || CFLAG:36 >= 3)
							PRINTFORMW 「だしてっ、もっとだしてっ、せーえき出してください、赤ちゃん溺れるくらいぃ♪」
						ELSE
							PRINTFORMW 「だしてっ、いっぱいだしてっ、赤ちゃん作りたいんですからぁ♪」
						ENDIF
					ENDIF
				;発情中or戦意喪失or諦めか妊娠悦びフラグ一定以上か深い催眠か精液中毒2以上
				ELSEIF CFLAG:28 || CFLAG:29 || TFLAG:16 == 2 || CFLAG:803 >= 3 || CFLAG:43 > 1 || ABL:14 >= 2
					IF RAND:2 == 0
						PRINTFORM 「んうっ！？　せーえき、きてる、おく、とどいてるぅ
						CALL HEARTCB(0xD02830)
						PRINTFORMW 」
					ELSE
						IF CFLAG:22 && (CFLAG:36 == 0 || CFLAG:36 >= 3)
							PRINTFORMW 「あ、あぁっ……お腹ぁ、お腹に赤ちゃんいるのにぃ……♪」
						;出産なし
						ELSEIF EXP:22 == 0
							PRINTFORMW 「あ、あぁっ……出てますよ、いっぱい、すごいぃ……♪」
						ELSE
							PRINTFORMW 「あ、あぁっ……できちゃいます、だめなのにぃ、また子供できちゃいますうぅ……♪」
						ENDIF
					ENDIF
				ELSE
					;ランダムかMP1/4以下
					IF RAND:2 == 0 || MAXBASE:MP/4 >= BASE:MP
						IF CFLAG:22 && (CFLAG:36 == 0 || CFLAG:36 >= 3)
							PRINTFORMW 「い、いや、子供が……！」
						ELSE
							PRINTFORMW 「い、いや、中出しはいや、また子供作るのいやぁぁぁぁぁっ！」
						ENDIF
					ELSE
						PRINTFORMW 「だめ、射精はもうだめ、だめですってばぁ！」
					ENDIF
				ENDIF
			ELSE
				;戦意喪失or諦めでランダム
				IF RAND:3 == 0 && (CFLAG:29 || TFLAG:16 == 2)
					PRINTFORMW 「これ、いぃ、なかにだされるのきもちいぃですぅ！」
				;発情中or戦意喪失or諦めor浅い催眠か精液中毒2以上
				ELSEIF CFLAG:28 || CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 || ABL:14 >= 2
					IF RAND:2 == 0
						PRINTFORM 「んうっ、びゅくびゅくあついの、とどいてるぅ……
						CALL HEARTCB(0xD02830)
						PRINTFORMW 」
					ELSE
						PRINTFORM 「んっ……
						CALL HEARTCB(0xD02830)
						PRINTFORM 　なに……？　これ、きもちよくて……
						CALL HEARTCB(0xD02830)
						PRINTFORMW 」
					ENDIF
				ELSE
					;ランダムかMP1/4以下
					IF RAND:2 == 0 || MAXBASE:MP/4 >= BASE:MP
						PRINTFORMW 「ひっ……な、なにこれ、何してるんですか……！？」
					ELSE
						PRINTFORMW 「うぅ、気持ち悪い……離れてください……！」
					ENDIF
				ENDIF
			ENDIF
		ENDIF
;		ELSE
;		ENDIF
	;通常攻撃回避
	CASE 22
		;MP1/4以下or発情中
		IF MAXBASE:MP/4 >= BASE:MP || CFLAG:28
			PRINTFORMW 「なんとか避けられましたね……」
		ELSE
			PRINTFORMW 「そういう攻撃なら慣れてるんですよ」
		ENDIF
	;衣服攻撃回避
	CASE 23
		;MP1/4以下or発情中
		IF MAXBASE:MP/4 >= BASE:MP || CFLAG:28
			PRINTFORMW 「避けられたみたいですね……」
		ELSE
			PRINTFORMW 「ありゃ、服でも狙ってました？　残念でした」
		ENDIF
	;性攻撃回避
	CASE 24
		;MP1/4以下or発情中
		IF MAXBASE:MP/4 >= BASE:MP || CFLAG:28
			PRINTFORMW 「い、今は絶対だめです！」
		ELSE
			PRINTFORMW 「んくっ……これは避けないと」
		ENDIF
	;拘束・寄生・その他攻撃回避
	CASE 25 TO 27
		;諦め時スルー
		IF TFLAG:16 == 2
			RESETCOLOR
			RETURN 1
		;MP1/4以下or発情中
		ELSEIF MAXBASE:MP/4 >= BASE:MP || CFLAG:28
			PRINTFORMW 「ぐ……冗談じゃありませんよ！」
		ELSE
			PRINTFORMW 「冗談じゃないですよ」
		ENDIF
	;HP0
	CASE 32
		;諦め中でも深い催眠でもないでも戦意喪失でもなく戦闘中
		SIF TFLAG:16 != 2 && CFLAG:43 < 2 && CFLAG:29 == 0 && TFLAG:13
			PRINTFORMW 「た、体力が……」
	;戦意喪失
	CASE 33
		PRINTFORMW 「た、戦わなきゃ……んんっ♪　わらひが、たたかわなきゃ、みんながぁ……」
	;服が破ける
	CASE 35
		;戦意喪失or諦めor深い催眠
		IF CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1
			PRINTFORMW 「服なんてどうでもいいじゃないですかぁ？　はやくぅ……♪」
		;MP1/4以下or発情中or要睡眠以上or妊娠中or催眠
		ELSEIF MAXBASE:MP/4 >= BASE:MP || CFLAG:28 || FLAG:7 >= 7 || CFLAG:36 >= 3 || CFLAG:43
			PRINTFORMW 「しまった、服が……！」
		;全裸
		ELSEIF ARG:1 == 0
			PRINTFORMW 「きゃっ！？　こ、この、ほんといやらしいですねぇ」
		;半裸
		ELSEIF ARG:1 == 1
			PRINTFORMW 「くっ！　もっと厚着をしてくるべきでしたね……」
		;破れ始めた
		ELSEIF ARG:1 == 2
			PRINTFORMW 「げっ、服が……」
		ENDIF
	;攻撃されて起きた
	CASE 41
		PRINTFORMW 「あうっ！　な、なんですか……！？」
	;裸で時間経過
	CASE 46
		IF RAND:2 == 0
			RESETCOLOR
			CALL SYSTEM_MESSAGE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
		ELSE
			;発情or諦めor深い催眠
			IF CFLAG:28 || TFLAG:16 == 2 || CFLAG:43 > 1
				PRINTFORM 「はぁ、はぁっ……
				CALL HEARTCB(0xD02830)
				PRINTFORML 」
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMDW 耐え切れないように、ゆかりは熱っぽい表情で自分の指を咥えている……
					CASE 1
						PRINTFORMDW 熱い吐息を吐きながら、もじもじとゆかりは股間を擦りあわせた……
				ENDSELECT
			;拘束中
			ELSEIF CFLAG:20
				PRINTFORMDW 裸の状態で体を締め付けられ、ゆかりは必死にもがいている……
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「は、肌に、くっかないでください……！」
					CASE 1
						PRINTFORMW 「この、はり付かないでくださいってば……！」
				ENDSELECT
			;EP1/4以下かMP1/4以下or催眠
			ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:EP/4 >= BASE:EP || CFLAG:43
				PRINTFORMDW 魔物からの視線に気づき、ゆかりは顔を赤らめながら胸を隠した……
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW （い、今されたら、だめ……）
					CASE 1
						PRINTFORMW （変なことされるの、もう無理……）
				ENDSELECT
			ELSE
				PRINTFORMD 魔物から視線を向けられている事に気付いたゆかりは、不快そうに
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMDW 舌打ちをした……
					CASE 1
						PRINTFORMDW 睨み返した……
				ENDSELECT
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「……さっさと倒さないと」
					CASE 1
						PRINTFORMW 「いい気分じゃないですねぇ……」
					CASE 2
						PRINTFORMW 「どうせ着てても、すぐ破かれるんですよね」
				ENDSELECT
			ENDIF
		ENDIF
	;ターン終了時自慰　面倒なので強制自慰と共通に
	CASE 48
		;諦めか戦意喪失
		IF CFLAG:29 || TFLAG:16 == 2
			PRINTFORM 「足りない……たりませんよぉ……
			CALL HEARTCB(0xD02830)
			PRINTFORMW 」
		ELSEIF RAND:2 == 0
			PRINTFORML 「た……たたかわなきゃ、たたかわなきゃ……あ、あぁっ！
			PRINTFORMW 　だめ、もう……だめぇっ、がまんできません……んはぁ♪」
		ELSE
			PRINTFORM 「はぁっ……んぁっ♪　気持ちよくしなくちゃ、動けませんよ
			CALL HEARTCB(0xD02830)
			PRINTFORMW 　勝つためには、しょうが、ないんですぅ……♪」
		ENDIF
	;戦闘開始
	CASE 54
		CFLAG:801 += 1
		;睡眠
		IF TFLAG:14 == 3
