	;パーティ加入
	CASE 109
		;戦意喪失ではない霊夢がいる
		IF (NO:1 == 1 && CFLAG:1:29 == 0) || (NO:(FLAG:10) == 1 && CFLAG:(FLAG:10):104 == 0 && CFLAG:(FLAG:10):29 == 0) || (NO:(FLAG:11) == 1 && CFLAG:(FLAG:11):104 == 0 && CFLAG:(FLAG:11):29 == 0)
