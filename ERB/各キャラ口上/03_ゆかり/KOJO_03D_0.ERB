							PRINTFORML 「疲れても諦めないもんね、みんな」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					ENDIF
				;発射
				ELSE
					;MP1/4以下or妊娠中or要睡眠以上
					IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
						PRINTFORML 「もう潰れちゃえ！」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						IF RAND:2 == 0
							PRINTFORML 「そこだぁ、真正面！」
							CALL WAIT_AUTOMODE(400, 0)
						ELSE
							PRINTFORM 「今の私は、
							IF NO:1 == 1
								PRINTFORM ママ
							ELSEIF NO:1 == 2
								PRINTFORM マキおねーちゃん
							ELSEIF NO:1 == 22
								PRINTFORM 紫お……ねぇさん
							ELSE
								PRINTFORM %CALLNAME:1%
							ENDIF
							PRINTFORML の味方だから！」
							CALL WAIT_AUTOMODE(400, 0)
						ENDIF
					ENDIF
				ENDIF
			;ばら撒き弾
			CASE 14
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「も～、ちょこまか逃げないで！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					PRINTFORML 「逃がさないよ？」
					CALL WAIT_AUTOMODE(400, 0)
				ENDIF
			;貫通弾
			CASE 15
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「消えちゃえっ！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					PRINTFORML 「まとめて退治してあげるね」
					CALL WAIT_AUTOMODE(400, 0)
				ENDIF
			;消耗武器
			;弱攻撃
			CASE 20
				;MP1/4以下か要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || FLAG:7 >= 7
					PRINTFORML 「無駄打ちしたら怒られるもん……」
					CALL WAIT_AUTOMODE(400, 0)
				;HP1/4以下か妊娠中
				ELSEIF MAXBASE:HP/4 >= BASE:HP || CFLAG:36 >= 3
					PRINTFORML 「当てるくらいならできるよ！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「逃がさないよーだ」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「避けさせてなんて、あげないから♪」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;中攻撃
			CASE 21
				;MP1/4以下か要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || FLAG:7 >= 7
					PRINTFORML 「えいっ！」
					CALL WAIT_AUTOMODE(400, 0)
				;HP1/4以下か妊娠中
				ELSEIF MAXBASE:HP/4 >= BASE:HP || CFLAG:36 >= 3
					PRINTFORML 「これくらいならできるかな……」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「これはどう？」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「普通ならこれくらいがちょうどいいのかな？　私たちは普通じゃないみたいだけど」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;強攻撃
			CASE 22
				;MP1/4以下か要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || FLAG:7 >= 7
					PRINTFORML 「えーい、当たってぇ！」
					CALL WAIT_AUTOMODE(400, 0)
				;HP1/4以下か妊娠中
				ELSEIF MAXBASE:HP/4 >= BASE:HP || CFLAG:36 >= 3
					PRINTFORML 「もうっ、これで倒れてぇ……！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「痛くしないとわからないかな？」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「床のタイルになっちゃえ！」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;節約攻撃
			CASE 23
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「いっぱい使ったら怒られるかな……」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「ママは節約に気を遣わなきゃいけない人なんだって」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「武器を用意するのもそれなりに手間がかかる、だっけ？」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;みだれうち
			CASE 24
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「まとめて吹き飛んでくれたら……！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					PRINTFORML 「少しもったいないかなぁ」
					CALL WAIT_AUTOMODE(400, 0)
				ENDIF
			;一点集中
			CASE 25
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「し、しつこいよっ！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「これで終わってくれるといいな」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「いい加減飽きてきたし、もう負けていいよ？」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			
			;非消耗武器
			;弱攻撃
			CASE 30
				;MP1/4以下か要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || FLAG:7 >= 7
					PRINTFORML 「ぷちぷちする感触は気持ちいいけど……」
					CALL WAIT_AUTOMODE(400, 0)
				;HP1/4以下か妊娠中
				ELSEIF MAXBASE:HP/4 >= BASE:HP || CFLAG:36 >= 3
					PRINTFORML 「軽く振るくらいならできるよ」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「これくらいで十分だよ」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「これなら届くね！」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;中攻撃
			CASE 31
				;MP1/4以下か要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || FLAG:7 >= 7
					PRINTFORML 「おもいっきり近づくしかないかな……」
					CALL WAIT_AUTOMODE(400, 0)
				;HP1/4以下か妊娠中
				ELSEIF MAXBASE:HP/4 >= BASE:HP || CFLAG:36 >= 3
					PRINTFORML 「うーん、もうっ！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「少しは効くのかな？」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「いい加減、負けてよ」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;強攻撃
			CASE 32
				;MP1/4以下か要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || FLAG:7 >= 7
					PRINTFORML 「はやく潰れてっ！」
					CALL WAIT_AUTOMODE(400, 0)
				;HP1/4以下か妊娠中
				ELSEIF MAXBASE:HP/4 >= BASE:HP || CFLAG:36 >= 3
					PRINTFORML 「うぅっ、体が重いよぉ……」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 1
						PRINTFORML 「吹き飛ばしてあげるから！」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「普通に叩きつけるくらいじゃ、みんなは負けを認めないよね？」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;下段狙い
			CASE 33
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「こういう時は、まずけんせーだっけ……」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「足を止めちゃうよ！」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「みんな、物理的に足を止めないと駄目だもん」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;急所狙い
			CASE 34
				;MP1/4以下or妊娠中or要睡眠以上
				IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
					PRINTFORML 「来ない……でっ！」
					CALL WAIT_AUTOMODE(400, 0)
				ELSE
					IF RAND:2 == 1
						PRINTFORML 「みんなは、痛くしないとわかんないの」
						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「ほんとは狙っちゃいけないんだって、なんでかな？」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;拘束離脱
			CASE 42
				IF ARG:3 == 0
					;MP1/4以下か妊娠中
					IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3
						PRINTFORML 「あうぅっ……離してぇ！」
						CALL WAIT_AUTOMODE(400, 0)
					;HP1/4以下
					ELSEIF MAXBASE:HP/4 >= BASE:HP
						PRINTFORML 「うっ……だめ、力が……」
						CALL WAIT_AUTOMODE(400, 0)
					;その他
					ELSE
						PRINTFORML 「もうっ、はやく離してぇ！」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;力溜め
			CASE 43
				IF ARG:2 == 0
					;MP1/4以下か妊娠中
					IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3
						PRINTFORML 「う、うぅんっ……！」
						CALL WAIT_AUTOMODE(400, 0)
					;HP1/4以下
					ELSEIF MAXBASE:HP/4 >= BASE:HP
						PRINTFORML 「このままじゃだめ、待たなきゃ……」
						CALL WAIT_AUTOMODE(400, 0)
					;その他
					ELSE
						PRINTFORML 「うぅ、早く出たいよぉ……」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;諦め
			CASE 44
				IF ARG:2 == 0
					PRINTFORML 「もともと、私は……みんなの、仲間だもん。
