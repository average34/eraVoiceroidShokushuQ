		CFLAG:805 = 0
		RESETCOLOR
		;口上記入済み
		RETURN
	;絶頂
	CASE 120
		;連続絶頂２回めor４回め以降ランダム
		IF CFLAG:806 == 2 || (CFLAG:806 > 3 && RAND:4 == 0)
				PRINTFORM 「ま……たぁっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　もっと、ほしくなっちゃうよぉ……
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
		;自慰
		ELSEIF CFLAG:805 == 3
			IF FLAG:AUTOMODE
				PRINTFORML 「はぁっ、いいっ……じぶん交尾きもちよかったぁ……♪」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「はぁっ、いいっ……じぶん交尾きもちよかったぁ……♪」
			ENDIF
		;諦め時orMP1/4以下or発情中or催眠
		ELSEIF TFLAG:16 == 2 || MAXBASE:MP/4 >= BASE:MP || CFLAG:28 || CFLAG:43
			;諦め時or深い催眠専用
			IF RAND:3 == 0 && (TFLAG:16 == 2 || CFLAG:43 > 1)
				IF FLAG:AUTOMODE
					PRINTFORML 「あはっ……♪　これでやめちゃやだよ……ずっとイかせ続けてね……♪」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「あはっ……♪　これでやめちゃやだよ……ずっとイかせ続けてね……♪」
				ENDIF
			ELSEIF RAND:2 == 0
				PRINTFORM 「きたぁっ♪　イっちゃう、みんなとこーびしてイっちゃうよぉ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			ELSE
				IF FLAG:AUTOMODE
					PRINTFORML 「い、いいよ、きもちいいのっ、そのまま――あ、ああああああぁんっ♪」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「い、いいよ、きもちいいのっ、そのまま――あ、ああああああぁんっ♪」
				ENDIF
			ENDIF
		ELSE
			IF RAND:2 == 0
				PRINTFORM 「や、やだっ……これ以上はやだ、やだぁぁぁぁぁぁっ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			ELSE
				IF FLAG:AUTOMODE
					PRINTFORML 「や、やめてよ、やめっ――や、やああぁぁぁぁっ！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「や、やめてよ、やめっ――や、やああぁぁぁぁっ！」
				ENDIF
			ENDIF
		ENDIF

		;自慰絶頂管理
		SIF CFLAG:805 == 3
			CFLAG:805 = 0

	;喪失中絶頂　諦め時流用
	CASE 122
		;自慰のときはこっちでない
		IF RAND:3 == 0 && CFLAG:805 != 3
			IF FLAG:AUTOMODE
				PRINTFORML 「あはっ……♪　これでやめちゃ、やだよ……もっとぉ……♪」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「あはっ……♪　これでやめちゃ、やだよ……もっとぉ……♪」
			ENDIF
		ELSEIF RAND:2 == 0
				PRINTFORM 「ま……たぁっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　きもちいいの、すてき……
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
		ELSE
			PRINTFORM 「イっちゃうぅ……♪　みんなとこーびしてイっちゃうよぉ……
			CALL HEARTCB(0xD02830)
			IF FLAG:AUTOMODE
				PRINTFORML 」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 」
			ENDIF
		ENDIF
	;寄生・出産イベント　喪失中も出る　念のためフラグを回収しておく
	CASE 127
			;寄生出産経験
			SIF (CFLAG:802 & 16) == 0
				CFLAG:802 |= 16
	;妊娠・出産イベント　喪失中でも出る
	CASE 129
			;妊娠出産経験
			SIF (CFLAG:802 & 2) == 0
				CFLAG:802 |= 2
	;処女喪失　触手娘が気にするとは思えないなぁ、霊夢から貞操無頓着引き継ぐし
	CASE 136
		IF ARG:3 == 0
			SELECTCASE ARG:1
				;敵の攻撃
				CASE 1
					IF FLAG:AUTOMODE
						PRINTFORML 「い、いたいっ！　なに、これ……！？」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「い、いたいっ！　なに、これ……！？」
					ENDIF
				;自慰
				CASE 2
					IF FLAG:AUTOMODE
						PRINTFORML 「い、いたいっ！　……これって、処女ってやつなのかな？
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「い、いたいっ！　……これって、処女ってやつなのかな？
					ENDIF
					IF FLAG:AUTOMODE
						PRINTFORML 　よくわかんないし、どうでもいいけど……」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 　よくわかんないし、どうでもいいけど……」
					ENDIF
			ENDSELECT
		ENDIF
	;完全捕食
	CASE 152
		IF FLAG:AUTOMODE
			PRINTFORML （やっぱり……私の居場所は、こっちだったんだ……
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW （やっぱり……私の居場所は、こっちだったんだ……
		ENDIF
		IF FLAG:AUTOMODE
			PRINTFORML 　ずっと、一緒にいようね……みんな……♪）
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 　ずっと、一緒にいようね……みんな……♪）
		ENDIF
	CASEELSE
		;汎用　行あけ区分のためこっちから戻す
		RESETCOLOR
		;口上記入済み
		RETURN
ENDSELECT

