	IF RAND:3 == 0
		IF FLAG:AUTOMODE
			PRINTFORML 「にゅう……」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「にゅう……」
		ENDIF
	ELSEIF RAND:2 == 0
		IF FLAG:AUTOMODE
			PRINTFORML 「ん～」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「ん～」
		ENDIF
	ELSE
		IF FLAG:AUTOMODE
			PRINTFORML 「……あぅっ」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「……あぅっ」
		ENDIF
	ENDIF
	RESETCOLOR
	;口上記入済み
	RETURN
ENDIF

