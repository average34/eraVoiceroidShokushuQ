	;HPダメージ
	CASE 12
		;妊娠中ランダム
		IF RAND:2 == 0 && CFLAG:36 >= 3
			PRINTFORML 「お、おなかはやだぁっ……」
			CALL WAIT_AUTOMODE(400, 0)
		;戦意喪失or諦めor深い催眠
		ELSEIF CFLAG:29 || TFLAG:16 == 2 || CFLAG:43 > 1
			PRINTFORML 「あ……うぅ……」
			CALL WAIT_AUTOMODE(400, 0)
