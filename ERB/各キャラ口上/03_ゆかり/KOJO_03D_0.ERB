						CALL WAIT_AUTOMODE(400, 0)
					ELSE
						PRINTFORML 「うーん、やっぱりみんな攻撃の美しさなんて気にしないよね～」
						CALL WAIT_AUTOMODE(400, 0)
					ENDIF
				ENDIF
			;溜め撃ち
			CASE 13
				;溜め
				IF CFLAG:31 == 2
					;MP1/4以下or妊娠中or要睡眠以上
					IF MAXBASE:MP/4 >= BASE:MP || CFLAG:36 >= 3 || FLAG:7 >= 7
