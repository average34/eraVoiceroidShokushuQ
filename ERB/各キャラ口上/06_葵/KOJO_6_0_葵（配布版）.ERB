;
;
;・eraVoiceroid触手宮_葵口上
;
;-------------------------------------------------------------------------
;
;・催眠についての独自解釈
;
;	・拘束・肉体操作系催眠
;		命令された通りに体が動いてしまい、自分の意思では体を動かすことができなくなるが、
;		喋る（喘ぐ）ことは可能
;		催眠といえばやっぱりこれ
;		
;	・発情・感情操作系催眠
;		葵の好きな人（イメージ上の理想の人）が幻覚として見えてしまう+発情作用
;		葵ちゃん妄想癖強そう
;		一応相手が男でも女でも大丈夫なようにしてあります
;		
;	・無意識・感覚操作系催眠
;		体が無意識の内に動いてしまい、催眠が深くなればなるほど様々なことが認識できなくなる（感覚操作マシマシ）
;		また、触手に奉仕することが当たり前だと思いこむ（ドS風？）

;-------------------------------------------------------------------------

;	・葵ちゃん口上、丁寧語マシマシ版

;-------------------------------------------------------------------------
;
;口上選択時のタイトル
@KOJO_TITLE_6_0
RESULTS = eraVoiceroid触手宮_葵ちゃん口上

;口上選択時の詳細説明
;フリーフォーマットです。ご自由にどうぞ
@KOJO_DETAIL_6_0
PRINTFORML 触手宮葵ちゃんの口上です
PRINTFORML 残酷表現Hard以上でHPが1/5以下になると一部口上が変わるのでご注意ください
PRINTFORML 男・ふたなりには大体対応してると思います
PRINTFORML 詳しくはReadmeをご覧ください
PRINTFORML ｺｯｼｮﾘ版です、配布厳禁

;デフォルトの文字色を返す関数
;これが無い場合、デフォルトはシステムカラー（白文字）になります
@KOJO_DEFAULT_COLOR_6_0
;　髪の色から
RETURN RGBCOLOR(73, 189, 240)

;口上本体
@KOJO_6_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

;
;口上側で地の文の制御もするつもりなら、地の文制御と口上本体は分けた方が無難です
;触手服の場合は非表示
IF ARG:0 == 440 || ARG:0 == 444
;トラップ淫蟲Bのニプルファックは地の文非表示
ELSEIF ARG:0 == 200 && ARG:1 == 147 && ARG:2 == 1
;上記以外の場合、地の文はデフォルトのものを使用します
ELSE
	CALL KOJO_6_0_SYSTEM(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
ENDIF



;そのキャラの口上の文字色を反映します
SETCOLOR CFLAG:700


;↓↓↓　　ここから口上のメイン処理　　↓↓↓

SELECTCASE ARG:0
	;味方の攻撃時
	CASE 1
		SELECTCASE ARG:1
		;基本コマンド
			;防御
			CASE 1
				;リロード時
				IF ARG:2 == 1
				;通常防御時、分けないと2度表示されるらしい
				ELSE
					;拘束・丸呑み時
					IF CFLAG:20 || CFLAG:21
						PRINTFORMW 「お、落ち着いて……ここは、抜け出せる隙を……っくうぅ！」
					;発情
					ELSEIF CFLAG:28
						PRINTFORM 「はぁっ、はぁーっ……
						CALL HEARTCB(0xD02830)
						PRINTFORM 　や、だめ……っ
						CALL HEARTCB(0xD02830)
						PRINTFORMW 」
					;MP1/4以下
					ELSEIF MAXBASE:MP/4 >= BASE:MP
						PRINTFORML 「ううぅ……もうやめて……やめて下さい……っ」
						PRINTFORMDW 葵は怯えながら、震える体で身を固めている……
					;HP1/5以下
					ELSEIF MAXBASE:HP/5 >= BASE:HP
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「う、ううっ……こ、これ以上喰らうのは……まずいです……」
								PRINTFORMDW 葵は傷だらけの体で身を固めた……
							CASEELSE
								PRINTFORML 「はぁ、はぁ……も、もう痛いのはイヤっ……」
								PRINTFORMDW 自らの身を必死で守る葵は、恐怖によって震えていた……
						ENDSELECT
					;全裸
					ELSEIF !BASE:CP
						;男の娘
						IF TALENT:122
							PRINTFORML 「く……こ、このぉ……」
							PRINTFORMDW 葵は羞恥に顔を赤らめながら、股間を手で覆い隠している……
						ELSE
							PRINTFORML 「み、見ないでぇ！　なんなんですか、もう……！」
							PRINTFORMDW 葵は片手で恥部を隠しながら、もう片方の手で胸を隠している……
						ENDIF
					;催眠時
					ELSEIF CFLAG:43
						
					;その他
					ELSE
						PRINTDATAW
							DATAFORM 「ここは様子見ですね……」
							DATAFORM 「あまり消耗したくないですからね」
						ENDDATA
					ENDIF
				ENDIF
			;アイテム
			CASE 2
				;煙玉
				IF ARG:2 == 59
					PRINTFORMW 「こんなときは……これです！」
				;自分に使用
				ELSEIF ARG:3 == TARGET
					SELECTCASE ARG:2
					;お茶
					CASE 50,51
						PRINTDATAW
							DATAFORM 「これを飲めば、少しは楽になります」
							DATAFORM 「ゆっくり飲んでる暇なんてないですね……」
							DATAFORM 「お腹を壊しませんように……！」
						ENDDATA
					;お酒（MP回復のほう）
					CASE 52,53
						PRINTDATAW
							DATAFORM 「はぁー……落ち着く味……と言っても、落ち着かせてくれそうにないですけど……」
							DATAFORM 「……よし、まだ頑張れます」
						ENDDATA
					;着替えセット
					CASE 54
						;全裸
						IF !BASE:CP
							PRINTDATAW
								DATAFORM 「うー……こんな所にある服なんて着たくないんですけど……」
								DATAFORM 「は、早く着替えないと……！」
							ENDDATA
						;全裸じゃないとき
						ELSE
							PRINTDATAW
								DATAFORM 「き、着替えてるとこ、見ないでくださいね！？」
								DATAFORM 「あ、あっち向いてください！　このヘンタイっ！！」
							ENDDATA
						ENDIF
					;包帯
					CASE 67
						PRINTFORMW 「応急処置、ってところですね」
					;その他
					CASEELSE
						PRINTDATAW
							DATAFORM 「あ、あれ？　どこだでしたっけ……あった！」
							DATAFORM 「消費期限とか、大丈夫かな……？」
							DATAFORM 「こんな時は……これです！」
						ENDDATA
					ENDSELECT
				;仲間に使用
				ELSE
					SELECTCASE ARG:2
					;お茶
					CASE 50,51
						PRINTDATAW
							DATAFORM 「大丈夫ですか？　これを飲んで下さい」
							DATAFORM 「傷を治すには……はい、これを！」
						ENDDATA
					;お酒（MP回復のほう）
					CASE 52,53
						PRINTDATAW
							DATAFORM 「大丈夫ですか？　これを飲んで落ち着いて下さい」
							DATAFORM 「……無理は、しないで下さいね？」
						ENDDATA
					;着替えセット
					CASE 54
						PRINTDATAW
							DATAFORM 「今のうちにこの服に着替えてください」
							DATAFORM 「ほら、早くこれに着替えたほうがいいですよ！」
						ENDDATA
					;気付け薬
					CASE 58
						;対象が喪失
						IF CFLAG:(ARG:3):29
							PRINTDATAW
								DATAFORM 「は、早く起こさないと……！　ん、これで！」
								DATAFORM 「まだ助けられるはずです……！」
							ENDDATA
						ELSE
						ENDIF
					;包帯
					CASE 67
						PRINTFORMW 「ん、\@ TALENT:122 ? ぼく # 私 \@がやってあげますから……！　じっとしてて！」
					;その他
					CASEELSE
						PRINTDATAW
							DATAFORM 「これを使ってください！」
							DATAFORM 「これ、どうぞ！」
							DATAFORM 「えぇっと、こんな時は……はい！」
						ENDDATA
					ENDSELECT
				ENDIF
			;前進
			CASE 3
				;発情
				IF CFLAG:28
					SELECTCASE RAND:2
						CASE 0
							PRINTFORM 「はぁ、はぁー……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　もっと、もっと距離を詰めないと……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORML 」
							PRINTFORMDL 葵は空ろな目をして、フラフラと触手に近づいていった……
						CASEELSE
							PRINTFORM 「はぁっ、はぁっ、あれ、欲しいよぉ……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORML 」
							PRINTFORMDL 発情しきった葵の目には、触手しか映っていないようだ……
					ENDSELECT
				;HP1/5以下またはMP1/4以下
				ELSEIF MAXBASE:HP/5 >= BASE:HP || MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「うっ……ぐううぅ、早く倒さなきゃ……！」
				;ふつう
				ELSE
					PRINTDATAL
						DATAFORM 「よし、やります！」
						DATAFORM 「あ、貴方なんか怖くない！」
					ENDDATA
				ENDIF
			;後退
			CASE 4
				;発情
				IF CFLAG:28
					SELECTCASE RAND:2
						CASE 0
							PRINTFORM 「うぅ……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　……はっ、ダメですよ、何考えてるんですか……
							CALL HEARTCB(0xD02830)
							PRINTFORML ！」
							PRINTFORMDL 葵は邪な気持ちをなんとか抑え込み後ずさった……
						CASEELSE
							PRINTFORM 「い、今は来ちゃダメ……！　ダメ、ダメですってぇ
							CALL HEARTCB(0xD02830)
							PRINTFORML ！」
							PRINTFORMDL 葵は敵から逃げるように後退した……
					ENDSELECT
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「\@ TALENT:122 ? く……来るなぁ # こ……来ないでぇ \@！　これ以上、近づかないでくださいよ！！」
					PRINTFORMDL 葵は後ずさるように敵との距離を離した……
				;HP1/5以下
				ELSEIF MAXBASE:HP/5 >= BASE:HP
					PRINTFORML 「やだ、もう、痛いのはイヤです……！」
					PRINTFORMDL 痛みへの恐怖を覚えながら、葵は逃げるように後ろへ下がった……
				;ふつう
				ELSE
					PRINTDATAL
						DATAFORM 「ここは一旦引かないと……」
						DATAFORM 「べ、別に怖いとか\@ TALENT:122 ? じゃない # じゃないですからね\@！」
					ENDDATA
				ENDIF
			;撤退
			CASE 5
				;成功する逃走
				IF ARG:2 == 1
					;発情
					IF CFLAG:28
						SELECTCASE RAND:2
							CASE 0
								PRINTFORM 「う……ああぁッ
								CALL HEARTCB(0xD02830)
								PRINTFORM ！　無理、もう無理ですぅッ
								CALL HEARTCB(0xD02830)
								PRINTFORML ！」
							CASEELSE
								PRINTFORM 「に、逃げなきゃ……このままじゃ、おかしくなりますぅ……ッ
								CALL HEARTCB(0xD02830)
								PRINTFORML ！」
						ENDSELECT
					;MP1/4以下
					ELSEIF MAXBASE:MP/4 >= BASE:MP
						PRINTFORML 「無理ですっ、こんな奴の相手なんて、もう無理です……ッ！」
						PRINTFORMDL 葵は震える肩を抱きながら、全力で逃走した……
					;HP1/5以下
					ELSEIF MAXBASE:HP/5 >= BASE:HP
						PRINTFORML 「い゛っ！？　い……痛い……けど、早く逃げなきゃ……！」
						PRINTFORMDL 葵は全身の痛みを必死で堪え、何とか逃げ出した……
					;ふつう
					ELSE
						PRINTDATAL
							DATAFORM 「ここは……逃げなきゃ！」
							DATAFORM 「もういやだぁ！」
						ENDDATA
					ENDIF
				;失敗した
				ELSE
					;発情
					IF CFLAG:28
						SELECTCASE RAND:2
							CASE 0
								PRINTFORM 「あ……
								CALL HEARTCB(0xD02830,2)
								PRINTFORM 　もう、逃げられないかも……
								CALL HEARTCB(0xD02830)
								PRINTFORML 」
								PRINTFORMDW 逃走に失敗した葵は、敵の前で空ろな目をして立ちすくんでいる……
							CASEELSE
								PRINTFORM 「ううっ、逃がしてよぉッ
								CALL HEARTCB(0xD02830)
								PRINTFORM ！　がまん、できなくなっちゃうぅ……ッ
								CALL HEARTCB(0xD02830)
								PRINTFORML ！」
								PRINTFORMDW 逃走に失敗した葵は、敵に向かって顔を真っ赤にさせながらそう叫んだ……
						ENDSELECT
					;MP1/4以下
					ELSEIF MAXBASE:MP/4 >= BASE:MP
						PRINTFORML 「……ひぃッ！？　や……やだぁ、こっち、\@ TALENT:122 ? 来るなぁああああッ # 来ないでええええぇッ \@！！」
						PRINTFORMDL 逃げ出そうと思った道を塞がれ、葵はパニックになっている……
					;HP1/5以下
					ELSEIF MAXBASE:HP/5 >= BASE:HP
						PRINTFORML 「い゛っ！？　い……痛、い……ッ！　ダメ、無理です……ッ！！」
						PRINTFORMDL 葵は走り出そうとしたが、全身を襲うあまりの痛みに、その場で蹲ってしまった……
					;ふつう
					ELSE
						PRINTDATAL
							DATAFORM 「……そんなに私を犯したいんですか？」
							DATAFORM 「……すんなり通すつもりは無い\@ TALENT:122 ? か # んですね \@…」
							DATAFORM 「回り込まれた！？」
						ENDDATA
					ENDIF
				ENDIF
			
			;攻撃攻撃
			;弱攻撃
			CASE 10
				;発情
				IF CFLAG:28
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「お、落ち着いて……しっかり、しっかり当て\@ TALENT:122 ? るんだ # れば \@……」
						CASEELSE
							PRINTFORM 「こ、こんなのしか、撃てません……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORMW 」
					ENDSELECT
				;MP1/4以下またはHP1/5以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:HP/5 >= BASE:HP
					PRINTDATAW
						DATAFORM 「確実に……でも、こんなのじゃ……」
						DATAFORM 「もう、早く終わってください……ッ」
					ENDDATA
				;ふつう
				ELSE
					PRINTDATAW
						DATAFORM 「まずはこれで！」
						DATAFORM 「ええい！」
						DATAFORM 「確実に当てます……！」
					ENDDATA
				ENDIF
			;中攻撃
			CASE 11
				;発情
				IF CFLAG:28
					SELECTCASE RAND:2
						CASE 0
							PRINTFORM 「ッはぁ、だめっ、集中できないよぉ……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORMW 」
						CASEELSE
							PRINTFORM 「ぜ、全然狙いが定まらないです……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORMW 」
					ENDSELECT
				;MP1/4以下またはHP1/5以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:HP/5 >= BASE:HP
					PRINTDATAW
						DATAFORM 「お願い、当たってください……！」
						DATAFORM 「む、向こう行って！　近寄らないでください……ッ！！」
					ENDDATA
				;ふつう
				ELSE
					PRINTDATAW
						DATAFORM 「当たって下さい！」
					ENDDATA
				ENDIF
			;強攻撃
			CASE 12
				;発情
				IF CFLAG:28
					SELECTCASE RAND:2
						CASE 0
							PRINTFORM 「ッはぁ、だめっ、集中できないよぉ……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORMW 」
						CASEELSE
							PRINTFORM 「ぜ、全然狙いが定まらない……ッ
							CALL HEARTCB(0xD02830)
							PRINTFORMW 」
					ENDSELECT
				;拘束・丸呑み時
				ELSEIF CFLAG:20 || CFLAG:21
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「くっ……ええいっ……！」
							PRINTFORMDW 葵は全ての力を振り絞った！
						CASEELSE
							PRINTFORMW 「外したら……いや、こんな奴に避けられるわけ……！」
					ENDSELECT
				;MP1/4以下またはHP1/5以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:HP/5 >= BASE:HP
					PRINTDATAW
						DATAFORM 「お願い、当たってぇ……！」
						DATAFORM 「む、向こう行って！　近寄らないでよ……ッ！」
					ENDDATA
				;ふつう
				ELSE
					PRINTDATAW
						DATAFORM 「強めのやつを……はああっ！」
					ENDDATA
				ENDIF
			;溜め撃ち
			CASE 13
				;溜め
				IF ARG:2 == 0
					;発情
					IF CFLAG:28
						SELECTCASE RAND:2
							CASE 0
								PRINTFORM 「ふーっ、集中……！　……ぅ、はぁッ
								CALL HEARTCB(0xD02830)
								PRINTFORML 」
							CASEELSE
								PRINTFORM 「が、がまん……
								CALL HEARTCB(0xD02830)
								PRINTFORM 　せめて、今だけは集中しないと……ッ
								CALL HEARTCB(0xD02830)
								PRINTFORML 」
								PRINTFORMDL 葵は自慰をしようとする手をなんとか静止し、溜めに専念した……
						ENDSELECT
					;MP1/4以下またはHP1/5以下
					ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:HP/5 >= BASE:HP
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「この一撃で、終わらせなきゃ……！」
							CASEELSE
								PRINTFORMW 「いっ……今は、攻撃、してこないで……よ」
								PRINTFORMDL 葵は微かに震えながら溜めを開始した……
						ENDSELECT
					;ふつう
					ELSE
						PRINTDATAL
							DATAFORM 「ふーっ、集中……！」
							DATAFORM 「謝るなら今のうちですよ……まあ、謝っても許してあげませんけど！」
							DATAFORM 「ふふっ、耐えられるかな……？」
							DATAFORM 「……お祈りは済んだ？」
						ENDDATA
					ENDIF
				;撃ち
				ELSE
					;発情
					IF CFLAG:28
						PRINTFORM 「うぅっ　はぁああ―――ッ
						CALL HEARTCB(0xD02830)
						PRINTFORMW ！」
					;MP1/4以下またはHP1/5以下
					ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:HP/5 >= BASE:HP
						PRINTFORML 「……お願い、これで終わってぇ―――ッ！！」
					;ふつう
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「……これで決まりです！」
							CASEELSE
								PRINTFORMW 「……やぁああああッ！！」
						ENDSELECT
					ENDIF
				ENDIF
			;ばらまき弾
			CASE 14
				;発情
				IF CFLAG:28
					PRINTFORM 「うーっ、もう狙えないですよぉ
					CALL HEARTCB(0xD02830)
					PRINTFORML ！」
					PRINTFORMDW 葵は狙いをつけて撃つのを諦め、四方八方に攻撃をばら撒いた！
				;ふつう
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「これで足止めして、次に繋げる…！」
						CASEELSE
							PRINTFORML 「ふっ！はあっ！」
							PRINTFORMDW 葵の放った攻撃は、いつの間にか敵を囲んでいた！
					ENDSELECT
				ENDIF
			;貫通弾
			CASE 15
				;発情
				IF CFLAG:28
					PRINTFORM 「き、気持ち悪い、一掃してあげます……ッ
					CALL HEARTCB(0xD02830)
					PRINTFORML 」
					PRINTFORMDW そうは言うものの、葵の触手を見る目は明らかに言葉と一致していない……
				;MP1/4以下またはHP1/5以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP || MAXBASE:HP/5 >= BASE:HP
					PRINTFORMW 「これでまとめて倒します……！」
				;ふつう
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「この角度から撃てば！」
						CASEELSE
							PRINTFORML 「どいてください！！」
							PRINTFORMDW 葵の放った攻撃は、次々と敵を貫いていった！
					ENDSELECT
				ENDIF
			;消耗武器
			;弱攻撃
			CASE 20
				;発情
				IF CFLAG:28
					SELECTCASE RAND:2
						CASE 0
							PRINTFORM 「はぁっ、はぁっ
							CALL HEARTCB(0xD02830)
							PRINTFORM 　落ちついて、まずはしっかり当てる\@ TALENT:122 ? んだ # んです \@……ッ
