@EXTRA_ACTION_1309(ENEMY_ID, CHARA_ID)
#DIM ENEMY_ID
#DIM CHARA_ID
;LOCAL
;1	命中率

CALL PRINT_MESSAGE(CHARA_ID, 17, TFLAG:99, CALLNAME:CHARA_ID, SAVESTR:ENEMY_ID, CHARA_ID)

;相手絶頂時に相手の戦闘経験をダウンさせる
IF CFLAG:CHARA_ID:35

	EXP:CHARA_ID:3 -= 5000
	IF EXP:CHARA_ID:3 < 0

		EXP:CHARA_ID:3 = 0

	ENDIF

	EXP:CHARA_ID:4 -= 5000
	IF EXP:CHARA_ID:4 < 0

		EXP:CHARA_ID:4 = 0

	ENDIF



ELSE



ENDIF

