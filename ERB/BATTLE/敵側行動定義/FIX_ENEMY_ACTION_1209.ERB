@FIX_ENEMY_ACTION_1209(ENEMY_ID)
#DIM ENEMY_ID
#DIM CHARA_ID

TFLAG:52 = ENEMY_ID
TFLAG:54 = DA:ENEMY_ID:51
CHARA_ID = DA:ENEMY_ID:51
TFLAG:55 = 3
TFLAG:60 = 1

TFLAG:57 = DA:ENEMY_ID:5
;NFダメージ補正
TFLAG:58 = 10000 / TFLAG:57 + 200 + CALC_NF_DAMAGE(CHARA_ID, 0)
TFLAG:61 = 150

TFLAG:64 = 15
TFLAG:65 = 50
TFLAG:90 = 500

TFLAG:59 = 0
TFLAG:91 = 0
TFLAG:92 = 0
TFLAG:99 = 1209
TFLAG:98 = 1

TFLAG:63 = 1

CALL ACTION_FILTER(-1, 4, 1209, CALLNAME:(TFLAG:54), SAVESTR:ENEMY_ID, TFLAG:54, TFLAG:63)

@EXTRA_ACTION_1209(ENEMY_ID, CHARA_ID)
#DIM ENEMY_ID
#DIM CHARA_ID

;射精した
IF TFLAG:98 > 1
	SIF TALENT:CHARA_ID:130 >= 3
		CFLAG:CHARA_ID:506 += TALENT:CHARA_ID:130 * 10
	CFLAG:CHARA_ID:512 += 1
	TFLAG:98 = 1
ELSE
	SIF TALENT:CHARA_ID:130 >= 3
		CFLAG:CHARA_ID:506 += TALENT:CHARA_ID:130 * 5
ENDIF

CFLAG:CHARA_ID:512 += 1

IF GET_BUST(CHARA_ID) >= 2
	CALL MILK_COME(CHARA_ID, 1, 0)
	LOCAL:0 = RESULT:0
	CALL PRINT_MESSAGE(CHARA_ID,17, 957, CALLNAME:CHARA_ID, SAVESTR:ENEMY_ID, CHARA_ID)
	CALL MILK_ADJUST(CHARA_ID, LOCAL:0)
ENDIF
