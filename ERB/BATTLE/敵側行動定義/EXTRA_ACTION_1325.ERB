@EXTRA_ACTION_1325(ENEMY_ID, CHARA_ID)
#DIM ENEMY_ID
#DIM CHARA_ID
;LOCAL
;1	命中率

;自慰系ダメージ

;命中率チェック
CALL GET_HIT_RATE_ENEMY(ENEMY_ID, CHARA_ID, 3)
LOCAL:1 = RESULT

IF RAND:100 < LOCAL:1
;●ダメージコモンだけなので、体の開発経験は増えないようだ。
	CALL PRINT_MESSAGE(CHARA_ID, 17, TFLAG:99, CALLNAME:(CHARA_ID), SAVESTR:(ENEMY_ID), CHARA_ID)

	TFLAG:63 = 3
	LOCAL:5 = CALC_SENSITIVITY(CHARA_ID, TFLAG:63, 1300 + RAND:400)
	CALL DAMAGE_COMMON(CHARA_ID, 2, LOCAL:5, 100)
ELSE
	CALL PRINT_MESSAGE(CHARA_ID, 27, TFLAG:99, CALLNAME:(CHARA_ID), SAVESTR:(ENEMY_ID), CHARA_ID)
ENDIF
