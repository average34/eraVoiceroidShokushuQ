@FIX_ENEMY_ACTION_34(ENEMY_ID)
#DIM ENEMY_ID

TFLAG:52 = ENEMY_ID
TFLAG:54 = 1
TFLAG:55 = 6
TFLAG:66 = 1

CALL ACTION_FILTER(-1, 7, 34, CALLNAME:(TFLAG:54), SAVESTR:ENEMY_ID, TFLAG:54)

IF FLAG:1 == 99 || TFLAG:15 == 10
	RETURN
ELSE
	CALL CREATE_ENEMY(55, CFLAG:1:4)
	TFLAG:15 ++
	TFLAG:(TFLAG:15) = FLAG:1 - 1
	DA:ENEMY_ID:1 = DA:ENEMY_ID:1 / 3 * 2
	DA:(TFLAG:(TFLAG:15)):1 = DA:ENEMY_ID:1
ENDIF