@FIX_ENEMY_ACTION_719(ENEMY_ID)
#DIM ENEMY_ID
#DIM CHARA_ID

TFLAG:52 = ENEMY_ID
TFLAG:54 = DA:ENEMY_ID:51
CHARA_ID = DA:ENEMY_ID:51
TFLAG:55 = 3
TFLAG:60 = 1

TFLAG:57 = DA:ENEMY_ID:5

TFLAG:58 = 10000/ TFLAG:57 + 170
TFLAG:61 = 150

TFLAG:64 = 15
TFLAG:65 = 50

TFLAG:59 = 0
TFLAG:90 = 500
TFLAG:91 = 0
TFLAG:92 = 8
SETBIT TFLAG:92, RAND:3


TFLAG:63 = 3
DA:ENEMY_ID:87 = RAND:3

;2体目の部位がBでちんこ使うパターン＆巨乳以上かつ拡張乳腺LV3以上でNF判定、判定成功でNFありメッセージになる
IF GETBIT(TFLAG:92, 1) && DA:ENEMY_ID:87 == 0 && GET_BUST(CHARA_ID) >= 1 && TALENT:CHARA_ID:162 >= 3
	TRYCALL CALC_NIPPLE_FUCK(CHARA_ID, 3)
	IF RESULT:0 == 1
		TFLAG:98 = 1
		TFLAG:99 = 719
		;ダメージ追加処理
		TFLAG:58 += CALC_NF_DAMAGE(CHARA_ID, 0)
	ELSE
		TFLAG:98 = 0
		TFLAG:99 = 0
	ENDIF
ELSE
	TFLAG:98 = 0
	TFLAG:99 = 0
ENDIF

CALL ACTION_FILTER(-1, 4, 719, CALLNAME:(TFLAG:54), SAVESTR:ENEMY_ID, TFLAG:54, TFLAG:63)


@EXTRA_ACTION_719(ENEMY_ID, CHARA_ID)
#DIM ENEMY_ID
#DIM CHARA_ID

;NFしている
IF TFLAG:98 > 1
	CFLAG:CHARA_ID:512 += 1
	LOCAL:0 = CALC_NF_DAMAGE(CHARA_ID, 0)
	CALL EXTASY(CHARA_ID, 1, LOCAL:0 / 2, 500)
ENDIF

