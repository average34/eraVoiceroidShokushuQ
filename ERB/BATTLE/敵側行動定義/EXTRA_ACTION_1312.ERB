@EXTRA_ACTION_1312(ENEMY_ID, CHARA_ID)
#DIM ENEMY_ID
#DIM CHARA_ID
;LOCAL
;1	命中率　侵入者有利のときは、技も強化される。

LOCAL:1 = 50

IF FLAG:10 > 0

	LOCAL:1 = 70

ENDIF

IF FLAG:11 > 0

	LOCAL:1 = 90

ENDIF


IF RAND:100 < LOCAL:1

	IF FLAG:10 > 0
		IF RELATION:1:(NO:(FLAG:10)) < 140 && CFLAG:(FLAG:10):5 > 499
			RELATION:1:(NO:(FLAG:10)) = 140
		ENDIF
	ENDIF
	IF FLAG:11 > 0
		IF RELATION:1:(NO:(FLAG:11)) < 140 && CFLAG:(FLAG:11):5 > 499
			RELATION:1:(NO:(FLAG:11)) = 140
		ENDIF
	ENDIF

	CALL KANKEI_RANK(NO:CHARA_ID, CHARA_ID)

	CALL PRINT_MESSAGE(CHARA_ID, 17, TFLAG:99, CALLNAME:CHARA_ID, SAVESTR:ENEMY_ID, CHARA_ID)

	CALL GENKAKU_1312(CHARA_ID)


;CFLAG:CHARA_ID:50 = 45
;CFLAG:CHARA_ID:51 = 1
CFLAG:CHARA_ID:52 = 0
CFLAG:CHARA_ID:54 = 0
