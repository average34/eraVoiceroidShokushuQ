@FIX_ENEMY_ACTION_1023(ENEMY_ID)
#DIM ENEMY_ID

TFLAG:52 = ENEMY_ID
TFLAG:54 = DA:ENEMY_ID:51
TFLAG:55 = 3
TFLAG:60 = 1

TFLAG:57 = DA:ENEMY_ID:5

TFLAG:58 = 1000 / TFLAG:57 + 20
TFLAG:61 = 140

TFLAG:64 = 0
TFLAG:65 = 20

TFLAG:91 = 0
TFLAG:93 = 0

TFLAG:63 = 2
TFLAG:99 = 1023

CALL ACTION_FILTER(-1, 4, 1023, CALLNAME:(TFLAG:54), SAVESTR:ENEMY_ID, TFLAG:54, TFLAG:64, TFLAG:63)


@EXTRA_ACTION_1023(ENEMY_ID, CHARA_ID)
#DIM ENEMY_ID
#DIM CHARA_ID
;LOCAL
;1	命中率

;C妊娠はどう考えてもコンフィグなのでC責めを隠れ蓑にする
;通常妊娠と同じように処理して寄生攻撃メインにした場合部位が変わるためC性攻撃メイン
IF GET_PENIS(CHARA_ID) && FLAG:518
	CFLAG:CHARA_ID:176 = ENEMY_ID
	CFLAG:CHARA_ID:175 += 10 + RAND:30
;尿道責めも同じくなので、同じ攻撃で2種に分岐
ELSEIF FLAG:522 == 1
	BASE:CHARA_ID:6 += 10 + RAND:30
ENDIF

CALL PRINT_MESSAGE(CHARA_ID, 17, TFLAG:99, CALLNAME:CHARA_ID, SAVESTR:ENEMY_ID, CHARA_ID)

