;精液ダメージ　中毒2レベル以上だと身体が疼きます
@SAMEN_DAMAGE(CHARA_ID, MP_DECREASE_RATE)
#DIM CHARA_ID
#DIM MP_DECREASE_RATE
IF ABL:CHARA_ID:14 > 1
	TFLAG:52 = 0
	TFLAG:55 = 3
	TFLAG:63 = 3
	TFLAG:65 = 0
	TFLAG:95 = 1
	;ダメージは精液量・中毒の2乗・V感度に比例
	LOCAL:1 = CFLAG:CHARA_ID:60 * ABL:CHARA_ID:14 * ABL:CHARA_ID:14 * CALC_SENSITIVITY(CHARA_ID, TFLAG:63, 100) / 10
	CALL PRINT_MESSAGE(CHARA_ID, 500, 0, CALLNAME:CHARA_ID, "")
	CALL DAMAGE_COMMON(CHARA_ID, 2, LOCAL:1 + RAND:300, MP_DECREASE_RATE)
ENDIF


