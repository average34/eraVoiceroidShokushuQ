@DEBUG_COMMAND_CFLAG入力(キャラID,入力)
#DIM キャラID
#DIM 入力
#DIM 前値
#DIM LCOUNT
前値 = CFLAG:(キャラID):(入力)
IF 入力 == 81
	$LOOP_CFLAG_081
	PRINTFORML 現在の値：{CFLAG:(キャラID):(入力)}
	PRINTL 変更後の値(ビット)を入力してください。
	CALL DEBUG_COMMAND_インプラント入力(キャラID)
	SELECTCASE RESULT
		CASE -1
		;オーバーフロー防止
		CASE 64 TO 997, IS >= 1000 ,IS <= -2
			CLEARLINE 7
			GOTO LOOP_CFLAG_081
		CASE 998
			FOR LCOUNT, 1, 11
				SETBIT CFLAG:(キャラID):81, LCOUNT
			NEXT
			CLEARLINE 7
			GOTO LOOP_CFLAG_081
		CASE 999
			CFLAG:(キャラID):81 = 0
			CLEARLINE 7
			GOTO LOOP_CFLAG_081
		CASEELSE
			INVERTBIT CFLAG:(キャラID):81, RESULT
			CLEARLINE 7
			GOTO LOOP_CFLAG_081
	ENDSELECT
ELSE
	PRINTFORML 現在の値：{CFLAG:(キャラID):(入力)}
	PRINTL 変更後の値を入力してください。
	
	$INPUT_CFLAG_VAR
	INPUT
	
	CFLAG:(キャラID):(入力) = RESULT
ENDIF

IF 入力 == 81
	RETURN CFLAG:(キャラID):入力,前値
ELSE
	RETURN RESULT,前値
ENDIF



