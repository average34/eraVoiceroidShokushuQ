				TALENT:(キャラID):(入力) = -1
	;羽
	CASE 213
		TALENT:(キャラID):(入力) ++
			SIF TALENT:(キャラID):(入力) > 2
				TALENT:(キャラID):(入力) = 0
	;尻尾
	CASE 215
			SIF TALENT:(キャラID):(入力) == 2
				TALENT:(キャラID):(入力) = 10
		TALENT:(キャラID):(入力) ++
			SIF TALENT:(キャラID):(入力) > 12
				TALENT:(キャラID):(入力) = 0
	;母乳粘度
	CASE 220
		TALENT:(キャラID):(入力) ++
			SIF TALENT:(キャラID):(入力) > 6
				TALENT:(キャラID):(入力) = 0
	;超々乳
	CASE 222
		TALENT:(キャラID):(入力) ++
		SIF	TALENT:(キャラID):(入力) > PLUS_HB2
			TALENT:(キャラID):(入力) = 0
		TALENT:(キャラID):107 = 0
		TALENT:(キャラID):108 = 0
		TALENT:(キャラID):109 = 0
		TALENT:(キャラID):110 = 0
		TALENT:(キャラID):158 = 0
		TALENT:(キャラID):223 = 0
	;接地乳房
	CASE 223
		TALENT:(キャラID):(入力) ++
		SIF TALENT:(キャラID):(入力) > PLUS_HB3
			TALENT:(キャラID):(入力) = 0
		TALENT:(キャラID):107 = 0
		TALENT:(キャラID):108 = 0
		TALENT:(キャラID):109 = 0
		TALENT:(キャラID):110 = 0
		TALENT:(キャラID):158 = 0
		TALENT:(キャラID):222 = 0
	CASEELSE
		IF TALENT:(キャラID):(入力)
			TALENT:(キャラID):(入力) = 0
		ELSE
			TALENT:(キャラID):(入力) = 1
		ENDIF
ENDSELECT

